Dialogue: 0,1:26:45.95,1:26:49.28,Default,,0,0,0,,# 星月之下 拨弄吉他 #\NMoon and the stars Strumming guitars
Dialogue: 0,1:26:49.45,1:26:53.66,Default,,0,0,0,,# 狂欢节庆 华丽壮大 #\NThat's why we love Carnaval
Dialogue: 0,1:26:58.04,1:27:01.59,Default,,0,0,0,,# 热爱我们的丛林生活 #\NLoving our life in the jungle
Dialogue: 0,1:27:01.67,1:27:04.97,Default,,0,0,0,,# 一切都那么狂热自由 #\NEverything's wild and free
