Dialogue: 0,1:14:00.25,1:14:04.11,Default,,0000,0000,0000,,Thật độc đáo, thật ấn tượng, kết thúc hay lắm.
Dialogue: 0,1:14:04.18,1:14:05.41,Default,,0000,0000,0000,,Cám ơn, Kendall.
Dialogue: 0,1:14:05.48,1:14:07.28,Default,,0000,0000,0000,,Cám ơn ngài.
Dialogue: 0,1:14:14.29,1:14:16.32,Default,,0000,0000,0000,,Uh, xin lỗi về điều này.
Dialogue: 0,1:14:16.39,1:14:19.36,Default,,0000,0000,0000,,Tôi chỉ, uh, chỉ thư giản cặp mắt của mình thôi.
Dialogue: 0,1:14:23.00,1:14:25.60,Default,,0000,0000,0000,,Skeeter. Đến cậu.
Dialogue: 0,1:14:42.45,1:14:43.94,Default,,0000,0000,0000,,Cậu ổn chứ, Skeeter?
Dialogue: 0,1:14:45.86,1:14:48.42,Default,,0000,0000,0000,,Con ông đốt vào lưỡi tôi.
Dialogue: 0,1:14:49.13,1:14:50.15,Default,,0000,0000,0000,,Xin lỗi?
Dialogue: 0,1:14:50.23,1:14:53.96,Default,,0000,0000,0000,,Con ong ... nó đốt lưỡi tôi.
Dialogue: 0,1:14:57.77,1:14:59.60,Default,,0000,0000,0000,,"Một con ong đốt vào lưỡi tôi",
Dialogue: 0,1:14:59.67,1:15:02.66,Default,,0000,0000,0000,,Oh, cậu hiểu anh ta à? Oh.
Dialogue: 0,1:15:02.74,1:15:06.07,Default,,0000,0000,0000,,Làm thế nào 1 cong ong đốt vào cái lưỡi của cậu?
Dialogue: 0,1:15:06.14,1:15:07.51,Default,,0000,0000,0000,,Uh...
Dialogue: 0,1:15:07.58,1:15:10.05,Default,,0000,0000,0000,,Tôi đang ăn kem, thì bất ngờ 1 con ong ...
Dialogue: 0,1:15:10.11,1:15:13.57,Default,,0000,0000,0000,,Uh, nghĩa là con ông trong kem,\Nvà anh ấy liếm phải nó.
Dialogue: 0,1:15:13.65,1:15:17.68,Default,,0000,0000,0000,,Hmm. Anh có thể phiên dịch phần trình\Nbày của Skeeter cho chúng tôi?
Dialogue: 0,1:15:17.76,1:15:20.05,Default,,0000,0000,0000,,- Mmm!\N- Uh... Được!
Dialogue: 0,1:15:20.12,1:15:21.59,Default,,0000,0000,0000,,Ừ, tôi có thể mà.
Dialogue: 0,1:15:23.90,1:15:25.49,Default,,0000,0000,0000,,Cám ơn, sẵn sàng chưa?
Dialogue: 0,1:15:25.56,1:15:27.62,Default,,0000,0000,0000,,- Sẵn sàng rồi.\N- Sẵn sàng?
Dialogue: 0,1:15:34.84,1:15:39.17,Default,,0000,0000,0000,,{\i1}"Tôi đã giành ngày cuối tùng của mình trong\Nkhách sạn, khách sạn nơi tôi sống..."{\i0}
Dialogue: 0,1:15:41.61,1:15:43.95,Default,,0000,0000,0000,,"Với 2 đứa cháu trai và cháu gái của minh".
Dialogue: 0,1:15:48.42,1:15:53.38,Default,,0000,0000,0000,,{\i1}"Với bọn trẻ, mọi thứ về khách sạn\Nnày đều kỳ lạ và tuyệt vời."{\i0}
Dialogue: 0,1:15:54.96,1:15:57.52,Default,,0000,0000,0000,,"Ngủ trên những chiếc giường khác nhau."
Dialogue: 0,1:15:59.23,1:16:00.79,Default,,0000,0000,0000,,"Qua lại tiền sảnh."
Dialogue: 0,1:16:03.00,1:16:05.23,Default,,0000,0000,0000,,"Nhảy lên và xuống trên mình cá sấu."
Dialogue: 0,1:16:05.30,1:16:06.57,Default,,0000,0000,0000,,[Grunts]
Dialogue: 0,1:16:10.38,1:16:13.11,Default,,0000,0000,0000,,{\i1}"Lên và xuống với cái thang máy."{\i0}
Dialogue: 0,1:16:13.18,1:16:14.24,Default,,0000,0000,0000,,Ah!
Dialogue: 0,1:16:14.31,1:16:16.11,Default,,0000,0000,0000,,Xin lỗi. Ừ, bây giờ tôi đã thấy con cá sấu đó,
Dialogue: 0,1:16:16.18,1:16:17.81,Default,,0000,0000,0000,,... không thích hợp trong hoàn cảnh này.
Dialogue: 0,1:16:23.86,1:16:28.26,Default,,0000,0000,0000,,"Một số khách sạn cố gắn làm cho nó trông có\Nvẻ càng giống như 1 mái nhà càng tốt."
Dialogue: 0,1:16:30.09,1:16:32.06,Default,,0000,0000,0000,,"Nhưng họ đã bỏ qua điểm mấu chốt."
Dialogue: 0,1:16:36.80,1:16:40.46,Default,,0000,0000,0000,,"Nếu các bạn muốn ở 1 nơi giống như nhà\Nmình, vậy tại sao không ở nhà cho rồi?
Dialogue: 0,1:16:44.74,1:16:48.27,Default,,0000,0000,0000,,{\i1}"Khách hàng của chúng ta nên trải\Nnghiệm qua 1 lối thoát mỗi ngày."{\i0}
Dialogue: 0,1:16:53.18,1:16:57.52,Default,,0000,0000,0000,,"Và đó là cách tôi thích để ứng dụng\Ntrong khách sạn mới của chúng ta"
Dialogue: 0,1:17:02.06,1:17:06.62,Default,,0000,0000,0000,,{\i1}"Những điều mà mọi đứa trẻ đều\Nbiết và mọi người lớn đều quên."{\i0}
Dialogue: 0,1:17:13.34,1:17:16.27,Default,,0000,0000,0000,,"Giống như cha tôi đã từng nói,
Dialogue: 0,1:17:16.34,1:17:20.78,Default,,0000,0000,0000,,... niềm vui của con chỉ giới hạn\Nbởi trí tưởng tượng của con."
Dialogue: 0,1:17:23.82,1:17:26.68,Default,,0000,0000,0000,,- Dễ thương quá đi mất.\N- Yeah? Được rồi. Bình tĩnh.
Dialogue: 0,1:17:26.75,1:17:28.18,Default,,0000,0000,0000,,Chỉ là cách thể hiện thôi.
Dialogue: 0,1:17:28.25,1:17:32.19,Default,,0000,0000,0000,,Và đó là gì? Xin lỗi, tôi không hiểu cái đó.
Dialogue: 0,1:17:34.49,1:17:37.72,Default,,0000,0000,0000,,Nó .... thật .. rực rỡ!
Dialogue: 0,1:17:37.80,1:17:41.03,Default,,0000,0000,0000,,Chúc mừng chàng trai của tôi. Cậu vừa thắng\Nchiếc chìa khóa để đến vương quốc rồi.
Dialogue: 0,1:17:53.88,1:17:57.61,Default,,0000,0000,0000,,Boogie, đừng làm thế, vi trùng không đấy.
Dialogue: 0,1:18:00.25,1:18:03.05,Default,,0000,0000,0000,,Chúc mừng. Làm tốt lắm.\NChờm đá lên lưỡi nhé.
Dialogue: 0,1:18:03.12,1:18:05.56,Default,,0000,0000,0000,,- Ổn mà.\N- Chúc mừng, Skeeter.
Dialogue: 0,1:18:05.62,1:18:07.56,Default,,0000,0000,0000,,- Chúc mừng, thật tuyệt vời.\N- Yeah?
Dialogue: 0,1:18:09.83,1:18:11.16,Default,,0000,0000,0000,,Thật xuất sắc.
Dialogue: 0,1:18:12.30,1:18:13.66,Default,,0000,0000,0000,,Chúc mừng, Skeeter.
Dialogue: 0,1:18:13.73,1:18:16.93,Default,,0000,0000,0000,,Được mà, Kendall.\NYeah. Không có cảm thấy buồn à, anh bạn.
Dialogue: 0,1:18:17.00,1:18:19.99,Default,,0000,0000,0000,,Tất nhiên là không. Cậu xứng đáng được nó mà.\NCậu là người đàn ông giỏi hơn tôi mà.
Dialogue: 0,1:18:20.07,1:18:22.54,Default,,0000,0000,0000,,{\i1}- Cậu rất cứng rắnl.\N- Yeah. Ai cũng bảo thế.{\i0}
Dialogue: 0,1:18:22.61,1:18:26.10,Default,,0000,0000,0000,,{\i1}Tôi sẽ không có tâm trạng để phá bỏ\Nngôi trường mà 2 đứa cháu mình đang ở.{\i0}
Dialogue: 0,1:18:26.18,1:18:28.15,Default,,0000,0000,0000,,Cái gì?
Dialogue: 0,1:18:29.58,1:18:32.28,Default,,0000,0000,0000,,Cậu biết chỗ để xây khách sạn mới mà?
Dialogue: 0,1:18:32.35,1:18:34.25,Default,,0000,0000,0000,,Anh đang nói gì thế?
Dialogue: 0,1:18:34.32,1:18:36.55,Default,,0000,0000,0000,,- Hay lằm, con trai ta.\N- Oh, thưa ngài.
Dialogue: 0,1:18:36.62,1:18:40.82,Default,,0000,0000,0000,,Con ong đó thực sự có hiệu quả trong việc thay\Nđổi cách nói chuyện của cậu, làm nó dễ thương hơn.
Dialogue: 0,1:18:40.89,1:18:43.52,Default,,0000,0000,0000,,Vâng, tôi vui lắm. Tôi muốn nói với ngài về vị trí,
Dialogue: 0,1:18:43.59,1:18:45.03,Default,,0000,0000,0000,,... để chúng ta xây khách sạn.
Dialogue: 0,1:18:45.10,1:18:47.26,Default,,0000,0000,0000,,{\i1}Chúc mừng sinh nhật ngài!{\i0}
Dialogue: 0,1:18:47.33,1:18:50.56,Default,,0000,0000,0000,,{\i1}Chúc mừng sinh nhật ngài!{\i0}
Dialogue: 0,1:18:50.64,1:18:53.63,Default,,0000,0000,0000,,{\i1}Chúc mừng sinh nhật/i>{\i0}
Dialogue: 0,1:19:15.49,1:19:18.66,Default,,0000,0000,0000,,Bronson, anh bị sa thải!!
Dialogue: 0,1:19:20.03,1:19:22.40,Default,,0000,0000,0000,,Sa thải?
Dialogue: 0,1:19:22.47,1:19:23.63,Default,,0000,0000,0000,,Oh...
Dialogue: 0,1:19:23.70,1:19:25.73,Default,,0000,0000,0000,,Thì ra đó là cách nó liên kết.
Dialogue: 0,1:19:36.98,1:19:38.01,Default,,0000,0000,0000,,Vâng?
Dialogue: 0,1:19:38.08,1:19:39.98,Default,,0000,0000,0000,,Cô có biết lớp của cô Jill Hastings ở đâu không?
Dialogue: 0,1:19:40.05,1:19:41.38,Default,,0000,0000,0000,,Đằng đó kìa!
Dialogue: 0,1:19:43.39,1:19:45.79,Default,,0000,0000,0000,,Sao cô không trả lời điện thoại của tôi?
Dialogue: 0,1:19:45.86,1:19:48.33,Default,,0000,0000,0000,,Bởi vì tôi biết là anh gọi.
Dialogue: 0,1:19:48.39,1:19:50.66,Default,,0000,0000,0000,,{\i1}Cô phải tin tôi, Jill.\NTôi không có ý gì cả ...{\i0}
Dialogue: 0,1:19:50.73,1:19:55.19,Default,,0000,0000,0000,,{\i1}Đừng xé nhỏ sự tôn trọng của tôi.\NTôi làm thế với anh vì lời bào chữa không thỏa đáng.{\i0}
Dialogue: 0,1:19:56.53,1:20:00.66,Default,,0000,0000,0000,,Chỉ là đi thật xa và tránh xa ra.
Dialogue: 0,1:20:02.74,1:20:06.18,Default,,0000,0000,0000,,Cậu Skeeter? Có phải cậu muốn\Nthiệu rụi trường của tụi cháu,
Dialogue: 0,1:20:06.24,1:20:09.04,Default,,0000,0000,0000,,... vì chúng cháu đã thiêu cậu trong câu chuyện hả?
Dialogue: 0,1:20:09.11,1:20:11.38,Default,,0000,0000,0000,,Không, cậu không làm thế
Dialogue: 0,1:20:11.45,1:20:14.25,Default,,0000,0000,0000,,Chúng cháu đã nghĩ rằng cậu\Nđã trở thành 1 chàng trai tốt.
Dialogue: 0,1:20:28.20,1:20:29.53,Default,,0000,0000,0000,,Là cậu sao.
Dialogue: 0,1:20:48.82,1:20:51.65,Default,,0000,0000,0000,,"Skeeto đánh bại Kendallo."
Dialogue: 0,1:20:53.02,1:20:54.96,Default,,0000,0000,0000,,Không thực sự như vậy.
Dialogue: 0,1:20:55.03,1:20:59.09,Default,,0000,0000,0000,,- Yeah?\N- Hey.
Dialogue: 0,1:20:59.16,1:21:02.06,Default,,0000,0000,0000,,Oh, hey.\NChào chị trở về.
Dialogue: 0,1:21:03.63,1:21:05.36,Default,,0000,0000,0000,,Chị cũng giận em phải không?
Dialogue: 0,1:21:05.44,1:21:09.27,Default,,0000,0000,0000,,Không như Jill, nhưng, uh, có giận ...
Dialogue: 0,1:21:09.34,1:21:11.97,Default,,0000,0000,0000,,Em không biết khách sạn mới sẽ được xây ở đó.
Dialogue: 0,1:21:12.71,1:21:13.87,Default,,0000,0000,0000,,Chị hiểu.
Dialogue: 0,1:21:13.95,1:21:15.04,Default,,0000,0000,0000,,Thế chị giận cái gì?
Dialogue: 0,1:21:15.11,1:21:16.64,Default,,0000,0000,0000,,{\i1}Em cho bọn trẻ ăn đồ béo à?{\i0}
Dialogue: 0,1:21:16.71,1:21:18.91,Default,,0000,0000,0000,,Không, chị cũng đoán em sẽ làm thế.
Dialogue: 0,1:21:18.98,1:21:22.11,Default,,0000,0000,0000,,Chị giận bởi vì em đã nói với bọn trẻ,
Dialogue: 0,1:21:22.19,1:21:24.52,Default,,0000,0000,0000,,... rằng trong cuộc sống thực tại thì\Nkhông có kết thúc nào tốt đẹp.
Dialogue: 0,1:21:24.59,1:21:26.52,Default,,0000,0000,0000,,Nhìn xung quanh chị xem, Wendy.
Dialogue: 0,1:21:26.59,1:21:28.72,Default,,0000,0000,0000,,Chị có thấy kết thúc tốt đẹp nào không?
Dialogue: 0,1:21:31.03,1:21:36.43,Default,,0000,0000,0000,,Chị không biết. Em và cha luôn có\Nnhiều niềm vui trong căn phòng này.
Dialogue: 0,1:21:36.50,1:21:38.16,Default,,0000,0000,0000,,Dù với bất cứ lý do gì, chị thì không.
Dialogue: 0,1:21:38.24,1:21:41.30,Default,,0000,0000,0000,,Chị luôn giễu cợt, chị khó tính.
Dialogue: 0,1:21:41.37,1:21:44.17,Default,,0000,0000,0000,,Đám mây đen.\NCái gáo nước
Dialogue: 0,1:21:44.24,1:21:46.37,Default,,0000,0000,0000,,Một con ... cá chết.
Dialogue: 0,1:21:46.44,1:21:49.07,Default,,0000,0000,0000,,Ừ, tất cả mấy thứ đó.
Dialogue: 0,1:21:49.15,1:21:51.17,Default,,0000,0000,0000,,Nhưng khi chị để Bobbi\Nvà Patrick với em,
Dialogue: 0,1:21:51.25,1:21:54.38,Default,,0000,0000,0000,,Chị đoán là mình mong em sẽ bồi dưỡng tâm hồn chúng.
Dialogue: 0,1:21:54.45,1:21:58.65,Default,,0000,0000,0000,,Cho chúng ánh sáng, cho chúng\Nniềm vui ... và thích thú với chúng.
Dialogue: 0,1:21:59.72,1:22:01.25,Default,,0000,0000,0000,,Chị nghĩ là cha cũng thích thế.
Dialogue: 0,1:22:02.76,1:22:05.96,Default,,0000,0000,0000,,Dù sao, um, chị cũng đã nhận được việc ở Arizona.
Dialogue: 0,1:22:06.03,1:22:08.26,Default,,0000,0000,0000,,Dạy học, không phải chủ nhiệm.
Dialogue: 0,1:22:08.33,1:22:09.80,Default,,0000,0000,0000,,Tuyệt quá.
Dialogue: 0,1:22:09.87,1:22:12.96,Default,,0000,0000,0000,,À, ừ, em có thể đế chơi khi bọn chị đã ổn định.
Dialogue: 0,1:22:13.04,1:22:15.70,Default,,0000,0000,0000,,Chị biết sau chuyện này bọn\Ntrẻ sẽ muốn gặp lại em.
Dialogue: 0,1:22:22.35,1:22:23.81,Default,,0000,0000,0000,,Em yêu chị.
Dialogue: 0,1:22:23.88,1:22:25.21,Default,,0000,0000,0000,,Yêu em nhiều.
Dialogue: 0,1:22:29.99,1:22:34.86,Default,,0000,0000,0000,,{\i1}Và Skeeter đã ngồi trên giường\Ncủa mình, cảm thấy hối hận,{\i0}
Dialogue: 0,1:22:34.93,1:22:37.29,Default,,0000,0000,0000,,{\i1}.. tự hỏi bản thân làm sao xếp lại những mảnh\Nvỡ của cuộc sống anh ta lại với nhau,r{\i0}
Dialogue: 0,1:22:37.36,1:22:39.39,Default,,0000,0000,0000,,{\i1}... sau 1 tuần kỳ diệu.{\i0}
Dialogue: 0,1:22:39.46,1:22:43.16,Default,,0000,0000,0000,,{\i1}- Kết thúc hay quá hả?\N- Đó là kết thúc của con đấy, con trai?{\i0}
Dialogue: 0,1:22:43.23,1:22:46.47,Default,,0000,0000,0000,,{\i1}Cha nghĩ đó là 1 phần hơi buồn,\Nvà con có thể làm nó tốt hơn mà.{\i0}
Dialogue: 0,1:22:46.54,1:22:48.37,Default,,0000,0000,0000,,Cái gì? Con làm cách nào để nó tốt hơn đây?
Dialogue: 0,1:22:48.44,1:22:50.93,Default,,0000,0000,0000,,{\i1}Trong những câu chuyện mà ta đã kể cho con,{\i0}
Dialogue: 0,1:22:51.01,1:22:52.84,Default,,0000,0000,0000,,{\i1}... chỉ khi mọi thứ nhìn có vẻ trống trải,{\i0}
Dialogue: 0,1:22:52.91,1:22:56.47,Default,,0000,0000,0000,,{\i1}... người anh hùng có thể làm gì đó bất ngờ và dũng cảm,{\i0}
Dialogue: 0,1:22:56.55,1:22:59.35,Default,,0000,0000,0000,,{\i1}... để đánh bại bọn xấu, gìn giữ ánh\Nsáng và có được những cô gái!{\i0}
Dialogue: 0,1:23:00.42,1:23:02.44,Default,,0000,0000,0000,,Rồi, con có thể làm điều đó như thế nào?
Dialogue: 0,1:23:02.52,1:23:04.11,Default,,0000,0000,0000,,{\i1}Đó là câu chuyện của con, không phải của ta.{\i0}
Dialogue: 0,1:23:04.19,1:23:06.32,Default,,0000,0000,0000,,{\i1}Nhưng con phải vó được bước\Nđi tốt nhất, hãy làm đi, con trai.{\i0}
Dialogue: 0,1:23:17.30,1:23:19.46,Default,,0000,0000,0000,,Ngài Nottingham! Ngài Nottingham?
Dialogue: 0,1:23:19.54,1:23:22.06,Default,,0000,0000,0000,,Tôi tự hỏi có thể nói chuyện với ngài 1 lát không.
Dialogue: 0,1:23:22.14,1:23:25.34,Default,,0000,0000,0000,,Tên tôi là Jill Hastings.\NTôi là giáo viên tại trường tiểu học Webster.
Dialogue: 0,1:23:25.41,1:23:28.94,Default,,0000,0000,0000,,{\i1}Giữ trường của chúng ta!\NGiữ trường của chúng ta!{\i0}
Dialogue: 0,1:23:29.01,1:23:32.47,Default,,0000,0000,0000,,{\i1}Giữ trường của chúng ta!\NGiữ trường của chúng ta!{\i0}
Dialogue: 0,1:23:32.55,1:23:34.25,Default,,0000,0000,0000,,{\i1}Giữ trường của chúng ta!{\i0}
Dialogue: 0,1:23:34.32,1:23:36.01,Default,,0000,0000,0000,,{\i1}Giữ trường của chúng ta!{\i0}
Dialogue: 0,1:23:36.09,1:23:39.18,Default,,0000,0000,0000,,Các anh đã vào vị trí chưa.\NChúng tôi đã sẵn sàng cho việc phá vỡ.
Dialogue: 0,1:23:39.26,1:23:41.42,Default,,0000,0000,0000,,Rất tốt.\NNgài Nottingham nói ông ấy sẽ gọi,
Dialogue: 0,1:23:41.49,1:23:43.59,Default,,0000,0000,0000,,{\i1}... nếu ông ấy có vấn đề về sự thay đổi,{\i0}
Dialogue: 0,1:23:43.66,1:23:46.22,Default,,0000,0000,0000,,... nên nếu chúng ta không nghe gì về ông ấy trong 20 phút nữa,
Dialogue: 0,1:23:46.30,1:23:48.32,Default,,0000,0000,0000,,Chúng ta sẽ .... tiến hành phá bỏ.
Dialogue: 0,1:23:54.41,1:23:57.93,Default,,0000,0000,0000,,Quý cô, chiến tranh đã kết thúc,\Ncô thua rồi. Tôi xin lỗi.
Dialogue: 0,1:24:01.61,1:24:04.84,Default,,0000,0000,0000,,Đó không phải là 1 cuộc chiến, ngài Nottingham.\NChúng ta đang nói về bọn trẻ.
Dialogue: 0,1:24:04.92,1:24:07.94,Default,,0000,0000,0000,,Nó sẽ gây rắc rối khác ở khách sạn của ông,
Dialogue: 0,1:24:08.02,1:24:10.54,Default,,0000,0000,0000,,... nó sẽ không có kết quả để thay đổi đâu ...
Dialogue: 0,1:24:10.62,1:24:12.52,Default,,0000,0000,0000,,Này, này! Barry, Jill.\NLàm gì thế hả?
Dialogue: 0,1:24:12.59,1:24:15.49,Default,,0000,0000,0000,,Chúng tôi vừa đến thôi.\NDonna, cô còn nhớ Jill mà phải không?
Dialogue: 0,1:24:15.56,1:24:19.76,Default,,0000,0000,0000,,Tất nhiên! Ôi chúa ơi.\NCái áo đó trong dễ thương quá!
Dialogue: 0,1:24:19.83,1:24:22.66,Default,,0000,0000,0000,,Uh, cám ơn.
Dialogue: 0,1:24:22.73,1:24:24.22,Default,,0000,0000,0000,,Bronson, cậu đang làm gì ở đây thế?
Dialogue: 0,1:24:24.30,1:24:26.74,Default,,0000,0000,0000,,Tôi làm gì ở đây à?\NYeah, tôi làm gì ở đây nhỉ?
Dialogue: 0,1:24:26.80,1:24:30.64,Default,,0000,0000,0000,,Oh, uh, à, ngài Bronson đây\Ngiống như 1 công dân hay lo lắng,
Dialogue: 0,1:24:30.71,1:24:32.44,Default,,0000,0000,0000,,... muốn mang những ý kiến của mình để\Nthu hút sự quan tâm của tôi,
Dialogue: 0,1:24:32.51,1:24:35.14,Default,,0000,0000,0000,,.. trước khi tôi giải quyết gọn lẹ vài vấn đề về tài sản.
Dialogue: 0,1:24:35.21,1:24:38.58,Default,,0000,0000,0000,,Và đó là mấu chốt vấn đề để tôi phân tích trong nhiều năm qua.
Dialogue: 0,1:24:38.65,1:24:40.81,Default,,0000,0000,0000,,- Nhiều năm?\N- Nhiều năm.
Dialogue: 0,1:24:40.89,1:24:44.41,Default,,0000,0000,0000,,Bronson, có phải anh đang chơi banh ngửa với tôi?
Dialogue: 0,1:24:44.49,1:24:45.85,Default,,0000,0000,0000,,Đúng vậy đấy, thưa ngài.
Dialogue: 0,1:24:45.92,1:24:49.15,Default,,0000,0000,0000,,{\i1}... bởi vì đơn xin của ông không hợp lý cho lắm cho nên...{\i0}
Dialogue: 0,1:24:50.53,1:24:52.26,Default,,0000,0000,0000,,Từ chối.
Dialogue: 0,1:24:53.83,1:24:55.13,Default,,0000,0000,0000,,Từ chối?
Dialogue: 0,1:24:55.20,1:24:57.06,Default,,0000,0000,0000,,Ừ, nhưng tin tốt là,
Dialogue: 0,1:24:57.13,1:24:59.57,Default,,0000,0000,0000,,Donna và tôi đã tìm cho ngài 1 phần bất động sản khác,
Dialogue: 0,1:24:59.64,1:25:03.51,Default,,0000,0000,0000,,... nằm trên bờ biển ở Santa Monica,\Ncách xa mọi sự khó chịu.
Dialogue: 0,1:25:03.57,1:25:06.13,Default,,0000,0000,0000,,Bãi biển đã là sự lựa chọn đầu tiên\Ncủa tôi nhưng nó không được bán.
Dialogue: 0,1:25:06.21,1:25:07.68,Default,,0000,0000,0000,,Giờ thì được.
Dialogue: 0,1:25:07.75,1:25:09.98,Default,,0000,0000,0000,,Đúng vậy! Có tuyệt không nào?
Dialogue: 0,1:25:10.48,1:25:11.57,Default,,0000,0000,0000,,Thật không?
Dialogue: 0,1:25:11.65,1:25:13.08,Default,,0000,0000,0000,,Bạn bè mà?
Dialogue: 0,1:25:13.15,1:25:15.14,Default,,0000,0000,0000,,Oh, oh.\NỪ, à, vi trùng.
Dialogue: 0,1:25:15.22,1:25:18.75,Default,,0000,0000,0000,,Dĩ vãng rồi.\NLại đây. Lại đây.
Dialogue: 0,1:25:18.82,1:25:22.19,Default,,0000,0000,0000,,Không, không, không, không, không.
Dialogue: 0,1:25:22.26,1:25:25.09,Default,,0000,0000,0000,,Tôi đang chạm vào ngài, và nó ổn mà. Nhìn xem.
Dialogue: 0,1:25:25.16,1:25:27.26,Default,,0000,0000,0000,,Aw...
Dialogue: 0,1:25:27.33,1:25:31.17,Default,,0000,0000,0000,,Ooh. Thích không nào.\NTốt mà. Hãy để chúng ta gần gũi hơn.
Dialogue: 0,1:25:31.24,1:25:34.93,Default,,0000,0000,0000,,Thật tuyệt và kinh dị.
Dialogue: 0,1:25:38.08,1:25:40.13,Default,,0000,0000,0000,,Chú ý, làm ơn, quý ông quý bà.
Dialogue: 0,1:25:40.21,1:25:44.58,Default,,0000,0000,0000,,{\i1}Chúng tôi đang làm 1 công việc có độ nhảy\Ncảm cao, mìn điều khiển bằng sóng rất nguy hiểm.{\i0}
Dialogue: 0,1:25:44.65,1:25:46.38,Default,,0000,0000,0000,,Boo!
Dialogue: 0,1:25:46.45,1:25:50.39,Default,,0000,0000,0000,,{\i1}Và để tránh bất cứ tai nạn đáng tiếc nào,{\i0}
Dialogue: 0,1:25:50.45,1:25:52.92,Default,,0000,0000,0000,,Tôi khuyên tất cả các bạn nên tắt điện thoại.
Dialogue: 0,1:25:52.99,1:25:56.22,Default,,0000,0000,0000,,Tất cả các bạn, làm ơn\NTắt điện thoại của mình.
Dialogue: 0,1:25:57.83,1:25:59.89,Default,,0000,0000,0000,,Anh đã sửa lại mọi sai lầm của mình đấy?
Dialogue: 0,1:25:59.96,1:26:01.90,Default,,0000,0000,0000,,Không phải mọi thứ.
Dialogue: 0,1:26:05.44,1:26:07.63,Default,,0000,0000,0000,,- Này?\N- Chúng ta xuất hiện 1 vấn đề.
Dialogue: 0,1:26:07.71,1:26:09.67,Default,,0000,0000,0000,,Tôi không thể gọi vào di động của Kendall,
Dialogue: 0,1:26:09.74,1:26:13.54,Default,,0000,0000,0000,,... để ngăn chặn cuộc phá vỡ sẽ bắt đầu trong ... 13 phút nữa.
Dialogue: 0,1:26:13.61,1:26:15.51,Default,,0000,0000,0000,,Chúng ta phải làm mấy dấu hiệu này thật đáng sợ.
Dialogue: 0,1:26:15.58,1:26:18.64,Default,,0000,0000,0000,,Chúng ta cần 1 cửa sổ, nơi mà mấy\Nngười thi công có thể thấy nó.
Dialogue: 0,1:26:18.72,1:26:20.58,Default,,0000,0000,0000,,Sau đó họ sẽ thay đổi suy nghĩ của mình.
Dialogue: 0,1:26:22.15,1:26:23.81,Default,,0000,0000,0000,,Bobbi, em tìm được này.
Dialogue: 0,1:26:28.06,1:26:29.92,Default,,0000,0000,0000,,Đó là Prius của tôi!
Dialogue: 0,1:26:29.99,1:26:31.79,Default,,0000,0000,0000,,- OK. Xe anh đâu?\N- Xe à?
Dialogue: 0,1:26:31.86,1:26:33.92,Default,,0000,0000,0000,,{\i1}Tôi trả nó về khách sạn rồi.{\i0}
Dialogue: 0,1:26:34.00,1:26:36.59,Default,,0000,0000,0000,,{\i1}Nghĩ sao mà anh lại trả nó về khách sạn?{\i0}
Dialogue: 0,1:26:36.67,1:26:38.00,Default,,0000,0000,0000,,Theo tôi!\NTheo tôi!
Dialogue: 0,1:26:38.07,1:26:40.40,Default,,0000,0000,0000,,Xin lỗi.
Dialogue: 0,1:26:41.84,1:26:43.70,Default,,0000,0000,0000,,Cho 1 lý do chính đáng!
Dialogue: 0,1:26:46.54,1:26:48.88,Default,,0000,0000,0000,,{\i1}Anh chưa bao giờ lái moto à?{\i0}
Dialogue: 0,1:26:48.95,1:26:50.28,Default,,0000,0000,0000,,Chưa!
Dialogue: 0,1:27:21.45,1:27:23.14,Default,,0000,0000,0000,,Ôi lạy chúa!
Dialogue: 0,1:27:26.82,1:27:28.75,Default,,0000,0000,0000,,Không, không, không!
Dialogue: 0,1:27:33.72,1:27:35.32,Default,,0000,0000,0000,,Ooh! Yah! Booya!
Dialogue: 0,1:27:46.24,1:27:49.47,Default,,0000,0000,0000,,Có ai thấy Bobbi? Patrick?
Dialogue: 0,1:27:49.54,1:27:51.91,Default,,0000,0000,0000,,Bobbi! Patrick!
Dialogue: 0,1:28:14.67,1:28:16.36,Default,,0000,0000,0000,,Tao sẽ cho mày ....
Dialogue: 0,1:28:18.17,1:28:20.19,Default,,0000,0000,0000,,Chơi không đẹp!
Dialogue: 0,1:28:20.27,1:28:23.71,Default,,0000,0000,0000,,Này, mọi người, còn 60 giây!\NWhoo-hoo!
Dialogue: 0,1:28:23.77,1:28:26.68,Default,,0000,0000,0000,,Khoan đã! Chờ 1 phút!\NTôi không thể tìm con của mình!
Dialogue: 0,1:28:26.74,1:28:28.87,Default,,0000,0000,0000,,- Chắc bọn chúng trong đó!\N- Hấp hối hả.
Dialogue: 0,1:28:28.95,1:28:31.78,Default,,0000,0000,0000,,{\i1}Chung tôi đã dọn dẹp bên trong đó nãy giờ, được chứ?\NMọi thứ sẽ tốt thôi..{\i0}
Dialogue: 0,1:28:34.08,1:28:35.88,Default,,0000,0000,0000,,Không! Để tôi qua!
Dialogue: 0,1:28:35.95,1:28:38.95,Default,,0000,0000,0000,,Này, chúng ta phải kiểm tra hết\Ncác phòng trước khi cho nổ chứ.
Dialogue: 0,1:28:39.02,1:28:40.82,Default,,0000,0000,0000,,{\i1}Chúng ta đã kiểm tra rồi!{\i0}
Dialogue: 0,1:28:40.89,1:28:42.12,Default,,0000,0000,0000,,Chính tôi làm vậy.
Dialogue: 0,1:28:43.53,1:28:46.19,Default,,0000,0000,0000,,Cậu Skeeter!
Dialogue: 0,1:28:46.26,1:28:48.73,Default,,0000,0000,0000,,Này! Ra khỏi đó mau!
Dialogue: 0,1:28:48.80,1:28:50.20,Default,,0000,0000,0000,,- Năm\N- Không
Dialogue: 0,1:28:50.27,1:28:51.79,Default,,0000,0000,0000,,- Giữ cái xích!\N- Bốn...
Dialogue: 0,1:28:53.04,1:28:55.01,Default,,0000,0000,0000,,Ba...
Dialogue: 0,1:28:55.64,1:28:57.23,Default,,0000,0000,0000,,Hai...
Dialogue: 0,1:28:57.31,1:28:58.30,Default,,0000,0000,0000,,Một!
Dialogue: 0,1:29:05.05,1:29:07.98,Default,,0000,0000,0000,,Nhà vua đã tuyên bố.
Dialogue: 0,1:29:08.99,1:29:11.08,Default,,0000,0000,0000,,{\i1}Khách sạn sẽ chuyển sang nơi khác.{\i0}
Dialogue: 0,1:29:11.16,1:29:13.56,Default,,0000,0000,0000,,Ngôi trường vẫn giữ lại.
Dialogue: 0,1:29:13.62,1:29:15.85,Default,,0000,0000,0000,,{\i1}Trường tiểu học Webster sẽ còn mãi!{\i0}
Dialogue: 0,1:29:21.20,1:29:22.36,Default,,0000,0000,0000,,- Mẹ!\N- Mẹ!
Dialogue: 0,1:29:22.43,1:29:23.99,Default,,0000,0000,0000,,Bobbi! Patrick!
Dialogue: 0,1:29:25.27,1:29:26.46,Default,,0000,0000,0000,,Oh!
Dialogue: 0,1:29:28.97,1:29:32.77,Default,,0000,0000,0000,,Này, này, này. Các nhóc đã đánh giá sai cậu rồi nhé.
Dialogue: 0,1:29:32.84,1:29:35.47,Default,,0000,0000,0000,,Lại đây! Cậu chỉ muốn ôm các cháu thôi.
Dialogue: 0,1:29:35.55,1:29:38.01,Default,,0000,0000,0000,,Các cháu có nghĩ rằng sẽ bỏ phiếu cho cậu ngay bây giờ không?
Dialogue: 0,1:29:38.08,1:29:40.71,Default,,0000,0000,0000,,Cháu biết cậu là 1 chàng trai tốt nhất mà.
Dialogue: 0,1:29:40.78,1:29:42.18,Default,,0000,0000,0000,,Yeah.
Dialogue: 0,1:29:42.25,1:29:46.28,Default,,0000,0000,0000,,Uh, Patrick, chào.\NMình là Trisha Sparks.
Dialogue: 0,1:29:46.36,1:29:49.66,Default,,0000,0000,0000,,Mình chỉ muốn bạn biết rằng ... mình cảm ơn vì đã cứu ngôi trường.
Dialogue: 0,1:29:51.70,1:29:53.39,Default,,0000,0000,0000,,Western.  Western.
Dialogue: 0,1:29:53.46,1:29:58.13,Default,,0000,0000,0000,,- Không có lời cảm ơn nào là cần thiết thưa quý cô.\N- Chàng trai của tôi, chàng trai của tôi.
Dialogue: 0,1:29:58.20,1:30:02.30,Default,,0000,0000,0000,,Làm thế nào để bày tỏ cho cậu biết sự biết ơn của mình.
Dialogue: 0,1:30:02.37,1:30:08.07,Default,,0000,0000,0000,,Oh-ho-ho! Làm đi.
Dialogue: 0,1:30:09.21,1:30:13.21,Default,,0000,0000,0000,,Ooh! Hoo-hoo-hoo!
Dialogue: 0,1:30:13.28,1:30:14.94,Default,,0000,0000,0000,,Này, có phải cô bé ấy hơi lớn tuổi với con?
Dialogue: 0,1:30:15.02,1:30:17.99,Default,,0000,0000,0000,,- Cô ấy tuyệt lắm.\N- Vậy à. Thế cháu nghĩ gì nào?
Dialogue: 0,1:30:18.06,1:30:19.85,Default,,0000,0000,0000,,Nếu Master Stinky có 1 nụ hôn,
Dialogue: 0,1:30:19.92,1:30:23.05,Default,,0000,0000,0000,,... thì có nên, cậu có nên nhận nó từ cô người cá không?
Dialogue: 0,1:30:23.13,1:30:25.19,Default,,0000,0000,0000,,- Lại đây, nào.\N- Ooh!
Dialogue: 0,1:30:39.54,1:30:42.14,Default,,0000,0000,0000,,{\i1}Giờ thì nhìn con trai ta vừa làm gì nào.{\i0}
Dialogue: 0,1:30:42.21,1:30:45.05,Default,,0000,0000,0000,,{\i1}Quay trở lại thế giới quản lý khách sạn sang trọng,{\i0}
Dialogue: 0,1:30:45.12,1:30:48.11,Default,,0000,0000,0000,,{\i1}... để bắt đầu công việc kinh doanh nhỏ của gia đình.{\i0}
Dialogue: 0,1:30:48.19,1:30:52.35,Default,,0000,0000,0000,,{\i1}Và hãy chúc phúc cho trái tim của nó,\Nthậm chí ngay sau khi ta đặt tên cho nó.{\i0}
Dialogue: 0,1:30:52.42,1:30:55.72,Default,,0000,0000,0000,,Này, nói xem nào, có vẻ như Bugsy cùng ăn với mọi người.
Dialogue: 0,1:30:57.76,1:30:59.63,Default,,0000,0000,0000,,Chúng ta hết kẹo dẻo rồi.
Dialogue: 0,1:30:59.70,1:31:03.06,Default,,0000,0000,0000,,Oh, đúng rồi. Hãy để anh xử lý.\NPhòng  phục vụ!
Dialogue: 0,1:31:05.17,1:31:07.19,Default,,0000,0000,0000,,- Vâng, thưa ngài?\N- Chào, Kendall.
Dialogue: 0,1:31:07.27,1:31:08.80,Default,,0000,0000,0000,,Uh, Bugsy vừa ăn hết kẹo dẻo rồi.
Dialogue: 0,1:31:08.87,1:31:11.43,Default,,0000,0000,0000,,Nếu anh có thể lấy cho nó 1 ít, thì tuyệt biết bao.
Dialogue: 0,1:31:11.51,1:31:13.10,Default,,0000,0000,0000,,Theo ý ngài.\NCòn gì nữa không ạ?
Dialogue: 0,1:31:13.18,1:31:15.27,Default,,0000,0000,0000,,Uh, không cần anh nữa, Kendall, nhưng, uh...
Dialogue: 0,1:31:15.35,1:31:17.01,Default,,0000,0000,0000,,... Tạp vụ đâu?
Dialogue: 0,1:31:18.35,1:31:19.98,Default,,0000,0000,0000,,Này, Aspen.
Dialogue: 0,1:31:20.05,1:31:22.42,Default,,0000,0000,0000,,Hãy chắc rằng cái lồng của Bugsy được sạch sẽ,
Dialogue: 0,1:31:22.49,1:31:24.08,Default,,0000,0000,0000,,... trước khi nó giải quyết vào buổi sáng?
Dialogue: 0,1:31:24.15,1:31:27.02,Default,,0000,0000,0000,,Kẹo dẹo của nó đang bắt đầu chống\Nđối rồi, chuẩn bị bốc mùi đấy.
Dialogue: 0,1:31:30.59,1:31:31.86,Default,,0000,0000,0000,,Được rồi.
Dialogue: 0,1:31:33.36,1:31:34.69,Default,,0000,0000,0000,,Ok, tạm biệt các cậu.
Dialogue: 0,1:31:34.77,1:31:37.10,Default,,0000,0000,0000,,{\i1}Ta nghĩ Bugsy đang cố nói với chúng ta,{\i0}
Dialogue: 0,1:31:37.17,1:31:39.14,Default,,0000,0000,0000,,{\i1}... rằng câu chuyện đã đến hồi kết thúc.{\i0}
Dialogue: 0,1:31:39.20,1:31:40.93,Default,,0000,0000,0000,,{\i1}Nhưng trước khi đi, ta muốn nói với các bạn rằng,{\i0}
Dialogue: 0,1:31:41.00,1:31:43.40,Default,,0000,0000,0000,,{\i1}... người hùng và kẻ độc ác của các bạn đang làm gì lúc này.{\i0}
Dialogue: 0,1:31:43.47,1:31:45.37,Default,,0000,0000,0000,,{\i1}Ông bạn già của tôi, Barry Nottingham,{\i0}
Dialogue: 0,1:31:45.44,1:31:48.00,Default,,0000,0000,0000,,{\i1}... đã không còn sợ vi khuẩn đến mức,{\i0}
Dialogue: 0,1:31:48.08,1:31:50.41,Default,,0000,0000,0000,,{\i1}... để lại việc kinh doanh khách sạn,{\i0}
Dialogue: 0,1:31:50.48,1:31:53.07,Default,,0000,0000,0000,,{\i1}... bắt đầu tiến vào lĩnh vực y tế.{\i0}
Dialogue: 0,1:31:53.15,1:31:57.45,Default,,0000,0000,0000,,{\i1}Anh ta hiện là y tá trưởng trường tiểu học Webster.{\i0}
Dialogue: 0,1:31:57.52,1:31:59.79,Default,,0000,0000,0000,,{\i1}Violet bây giờ đang điều hành 1 hệ thống khách sạn,{\i0}
Dialogue: 0,1:31:59.86,1:32:02.26,Default,,0000,0000,0000,,{\i1}... cùng với chồng, Mickey,{\i0}
Dialogue: 0,1:32:02.33,1:32:04.39,Default,,0000,0000,0000,,{\i1}... người trước kia làm trong phòng dịch vụ hầu bàn,{\i0}
Dialogue: 0,1:32:04.46,1:32:07.29,Default,,0000,0000,0000,,{\i1}... bây giờ nằm trong top 9 người giàu nhất thế giới.{\i0}
Dialogue: 0,1:32:08.43,1:32:10.37,Default,,0000,0000,0000,,{\i1}Còn Skeeter và Jill?{\i0}
Dialogue: 0,1:32:10.43,1:32:13.87,Default,,0000,0000,0000,,{\i1}Sau đám cưới đôi của thế kỷ,{\i0}
Dialogue: 0,1:32:13.94,1:32:18.57,Default,,0000,0000,0000,,{\i1}... họ sống ... à, sống hạnh phúc về saui,{\i0}
Dialogue: 0,1:32:18.64,1:32:20.41,Default,,0000,0000,0000,,{\i1}... cùng điều hành khách sạn Marty,{\i0}
Dialogue: 0,1:32:20.48,1:32:23.71,Default,,0000,0000,0000,,{\i1}... và giành nhiều thời gian với 2 đứa cháu của mình,{\i0}
Dialogue: 0,1:32:23.78,1:32:26.54,Default,,0000,0000,0000,,{\i1}... những đứa trẻ mà cuộc phiêu lưu của chúng chỉ vừa bắt đầu.{\i0}
Dialogue: 0,1:32:26.62,1:32:30.64,Default,,0000,0000,0000,,{\i1}Mặc dù không phải ai cũng thích thú.{\i0}
Dialogue: 0,1:32:30.72,1:32:33.52,Default,,0000,0000,0000,,{\i1}Nhưng đó lại là chuyện hoàn toàn khác.{\i0}
