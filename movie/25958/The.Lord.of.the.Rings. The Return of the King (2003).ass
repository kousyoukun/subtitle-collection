Dialogue: 0,0:56:17.55,0:56:21.71,Default,,0000,0000,0000,,cầu thang bí mật
Dialogue: 0,0:56:22.30,0:56:24.58,Default,,0000,0000,0000,,leo lên
Dialogue: 0,0:56:44.92,0:56:47.55,Default,,0000,0000,0000,,-Không,Frodo -Không phải đường đó.
Dialogue: 0,0:56:52.50,0:56:55.24,Default,,0000,0000,0000,,-Hắn đang làm gì vậy? -Không
Dialogue: 0,0:56:55.53,0:56:58.49,Default,,0000,0000,0000,,-Họ đang gọi tôi. -Không
Dialogue: 0,0:57:28.22,0:57:31.06,Default,,0000,0000,0000,,Trốn!Trốn mau!
Dialogue: 0,0:58:15.26,0:58:17.97,Default,,0000,0000,0000,,Tôi có thể cảm nhận được lưỡi gươm của hắn
Dialogue: 0,0:58:29.75,0:58:31.79,Default,,0000,0000,0000,,cuối cùng thì cũng đã tới
Dialogue: 0,0:58:33.70,0:58:36.45,Default,,0000,0000,0000,,-Trận chiến vĩ đại vào thời của chúng ta.
Dialogue: 0,0:59:15.68,0:59:18.80,Default,,0000,0000,0000,,Đi mau,Hobbit.Leo lên,chúng ta phải leo lên
Dialogue: 0,0:59:28.80,0:59:31.68,Default,,0000,0000,0000,,ván cờ đã được đặt.
Dialogue: 0,0:59:31.97,0:59:34.40,Default,,0000,0000,0000,,hòa bình sẽ kết thúc
Dialogue: 0,0:59:37.05,0:59:39.61,Default,,0000,0000,0000,,lên,lên,leo lên thang nào
Dialogue: 0,0:59:39.88,0:59:42.44,Default,,0000,0000,0000,,và sau đó chui vào cái hang
Dialogue: 0,0:59:42.71,0:59:45.79,Default,,0000,0000,0000,,này,cái gì trong cái hang đó?
Dialogue: 0,0:59:46.46,0:59:49.09,Default,,0000,0000,0000,,nghe này,tốt hơn là ngươi nghe cho rõ
Dialogue: 0,0:59:49.09,0:59:52.17,Default,,0000,0000,0000,,bất cứ chuyện gì xảy ra với cậu ta,ngươi sẽ phải trả giá
Dialogue: 0,0:59:52.45,0:59:53.97,Default,,0000,0000,0000,,một hơi thở khác lạ...
Dialogue: 0,0:59:54.20,0:59:57.24,Default,,0000,0000,0000,,một sợi tóc gáy ta dựng ngược,tất cả sẽ kết thúc.
Dialogue: 0,0:59:57.58,1:00:00.14,Default,,0000,0000,0000,,không lén lút.không còn tên lén lút nữa
