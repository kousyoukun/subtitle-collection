Dialogue: 0,1:39:50.88,1:39:54.76,Default,,0000,0000,0000,,nó sẽ không sống sót nổi với tà khí lan ra từ Mordor
Dialogue: 0,1:39:55.67,1:39:58.90,Default,,0000,0000,0000,,ánh sáng của Evenstar đã mờ
Dialogue: 0,1:39:59.21,1:40:03.05,Default,,0000,0000,0000,,quyền lực của Sauron càng tăng,sức lực của nó càng suy giảm
Dialogue: 0,1:40:03.38,1:40:06.98,Default,,0000,0000,0000,,cuộc sống của Arwen giờ đây gắn liền với chiếc nhẫn
Dialogue: 0,1:40:07.88,1:40:10.34,Default,,0000,0000,0000,,bóng tối đã bao phủ chúng ta, Aragorn.
Dialogue: 0,1:40:10.83,1:40:12.42,Default,,0000,0000,0000,,hồi kết đã đến
Dialogue: 0,1:40:12.87,1:40:15.38,Default,,0000,0000,0000,,không phải là kết cục của chúng ta,mà là của hắn
Dialogue: 0,1:40:16.24,1:40:19.68,Default,,0000,0000,0000,,cậu ra trận,nhưng không phải để thắng
Dialogue: 0,1:40:20.83,1:40:24.34,Default,,0000,0000,0000,,quân đội của Sauron đã đến Minas Tirith,cậu biết điều này
Dialogue: 0,1:40:24.66,1:40:28.89,Default,,0000,0000,0000,,nhưng hắn đã bí mật gửi một đạo quân khác tấn công từ dòng sông
Dialogue: 0,1:40:29.53,1:40:32.81,Default,,0000,0000,0000,,đoàn tàu của Corsair đã hạ thủy từ hướng nam
Dialogue: 0,1:40:33.57,1:40:35.56,Default,,0000,0000,0000,,chúng sẽ đến thành phố trong 2 ngày
Dialogue: 0,1:40:36.15,1:40:38.87,Default,,0000,0000,0000,,lực lượng của chúng quá đông,Aragorn
Dialogue: 0,1:40:39.69,1:40:41.73,Default,,0000,0000,0000,,cậu cần thêm người
Dialogue: 0,1:40:42.19,1:40:43.86,Default,,0000,0000,0000,,không có ai cả
Dialogue: 0,1:40:45.69,1:40:49.44,Default,,0000,0000,0000,,còn những kẻ sống trên núi
Dialogue: 0,1:40:58.60,1:41:00.66,Default,,0000,0000,0000,,những tên sát thủ
Dialogue: 0,1:41:00.93,1:41:02.28,Default,,0000,0000,0000,,..phản loạn
Dialogue: 0,1:41:04.80,1:41:06.87,Default,,0000,0000,0000,,cậu nên kêu gọi họ chiến đấu
Dialogue: 0,1:41:07.13,1:41:08.65,Default,,0000,0000,0000,,chúng không tin gì cả
Dialogue: 0,1:41:08.88,1:41:10.92,Default,,0000,0000,0000,,chúng sẽ không trả lời ai
Dialogue: 0,1:41:11.17,1:41:14.13,Default,,0000,0000,0000,,chúng sẽ đáp lời vị vua của Gondor
Dialogue: 0,1:41:21.38,1:41:26.29,Default,,0000,0000,0000,,Anduril,ánh lửa của phương tây,rèn lại tại lò rèn của Narsil
Dialogue: 0,1:41:40.79,1:41:45.02,Default,,0000,0000,0000,,Sauron sẽ không bao giờ quên Thanh gươm của Elendil
Dialogue: 0,1:41:59.23,1:42:03.78,Default,,0000,0000,0000,,thanh gươm đã từng gãy nát,sẽ trở lại Minas Tirith.
Dialogue: 0,1:42:04.15,1:42:06.46,Default,,0000,0000,0000,,người nào vận dụng được sức mạnh của thanh gươm này.
Dialogue: 0,1:42:06.73,1:42:10.44,Default,,0000,0000,0000,,có thể triệu tập lực lượng mạnh mẽ hơn bất cứ đội quân nào trên thế giới này.
Dialogue: 0,1:42:12.44,1:42:14.50,Default,,0000,0000,0000,,hãy quên đi kiếp du mục
Dialogue: 0,1:42:14.76,1:42:17.96,Default,,0000,0000,0000,,trở thành người mà cậu được sinh ra để trở thành
Dialogue: 0,1:42:18.26,1:42:20.17,Default,,0000,0000,0000,,hãy đi đến Dimholt
Dialogue: 0,1:42:23.17,1:42:27.93,Default,,0000,0000,0000,,ta hy vọng vào loài người
Dialogue: 0,1:42:31.30,1:42:35.26,Default,,0000,0000,0000,,hy vọng cho bản thân ta thì không còn nữa
Dialogue: 0,1:42:46.85,1:42:48.44,Default,,0000,0000,0000,,sao anh lại làm thế?
Dialogue: 0,1:42:48.97,1:42:52.04,Default,,0000,0000,0000,,trận chiến xảy ra ở hướng đông,anh không thể rời bỏ trước lúc lâm trận
Dialogue: 0,1:42:54.60,1:42:56.59,Default,,0000,0000,0000,,anh không thể bỏ mặc mọi người
Dialogue: 0,1:42:57.97,1:42:59.48,Default,,0000,0000,0000,,Éowyn....
Dialogue: 0,1:42:59.72,1:43:01.84,Default,,0000,0000,0000,,bọn em cần anh
Dialogue: 0,1:43:03.04,1:43:04.84,Default,,0000,0000,0000,,sao em lại đến?
Dialogue: 0,1:43:06.58,1:43:08.46,Default,,0000,0000,0000,,anh không biết sao?
Dialogue: 0,1:43:17.83,1:43:20.71,Default,,0000,0000,0000,,em chỉ yêu một hình bóng,một tâm tưởng
Dialogue: 0,1:43:24.75,1:43:27.05,Default,,0000,0000,0000,,anh không thể cho em những gì em tìm kiếm
Dialogue: 0,1:43:40.65,1:43:44.41,Default,,0000,0000,0000,,anh mong em hạnh phúc ngay từ lần đầu anh gặp em
Dialogue: 0,1:44:03.68,1:44:05.88,Default,,0000,0000,0000,,anh nghĩ anh định đi đâu đó?
Dialogue: 0,1:44:06.14,1:44:07.41,Default,,0000,0000,0000,,không phải lần này
Dialogue: 0,1:44:07.64,1:44:09.76,Default,,0000,0000,0000,,lần này ông phải ở lại,Gimli
Dialogue: 0,1:44:12.56,1:44:15.27,Default,,0000,0000,0000,,anh không học được gì về sự bướng bỉnh của người lùn sao?
Dialogue: 0,1:44:15.56,1:44:19.06,Default,,0000,0000,0000,,anh hãy chấp nhận đi,chúng tôi sẽ đi với anh
Dialogue: 0,1:44:41.49,1:44:43.56,Default,,0000,0000,0000,,-chuyện gì xảy ra vậy? -anh ta đang đi đâu vậy?
Dialogue: 0,1:44:43.99,1:44:45.79,Default,,0000,0000,0000,,-anh ta đang đi đâu vậy?
Dialogue: 0,1:44:48.45,1:44:50.40,Default,,0000,0000,0000,,tôi không hiểu
Dialogue: 0,1:44:54.91,1:44:56.62,Default,,0000,0000,0000,,tướng quân Aragorn
Dialogue: 0,1:44:57.11,1:44:59.71,Default,,0000,0000,0000,,sao anh ta lại rời đây ngay trước khi vào trận
Dialogue: 0,1:45:01.03,1:45:03.02,Default,,0000,0000,0000,,anh ta ra đi vì không có hy vọng nào cả
Dialogue: 0,1:45:03.78,1:45:05.80,Default,,0000,0000,0000,,anh ta đi vì anh ta buộc phải đi
Dialogue: 0,1:45:06.06,1:45:07.70,Default,,0000,0000,0000,,quân số ta quá ít
Dialogue: 0,1:45:07.93,1:45:10.01,Default,,0000,0000,0000,,chúng ta không thể đánh bại đội quân Mordor
Dialogue: 0,1:45:12.60,1:45:14.19,Default,,0000,0000,0000,,không...
Dialogue: 0,1:45:14.68,1:45:15.88,Default,,0000,0000,0000,,...chúng ta không thể
Dialogue: 0,1:45:18.81,1:45:22.44,Default,,0000,0000,0000,,nhưng dù sao đi nữa chúng ta sẽ gặp họ ở trận chiến
Dialogue: 0,1:45:37.00,1:45:38.68,Default,,0000,0000,0000,,ta đã để lại di huấn
Dialogue: 0,1:45:41.42,1:45:44.54,Default,,0000,0000,0000,,mọi người sẽ theo con theo di huấn của ta
Dialogue: 0,1:45:51.25,1:45:53.40,Default,,0000,0000,0000,,hãy ngồi lên ngai vàng của ta ở Hội trường vàng
Dialogue: 0,1:45:54.74,1:45:57.46,Default,,0000,0000,0000,,hãy bảo vệ Edoras...
Dialogue: 0,1:45:57.74,1:45:59.97,Default,,0000,0000,0000,,nếu trận chiến này thất bại
Dialogue: 0,1:46:01.16,1:46:04.43,Default,,0000,0000,0000,,còn nghĩa vụ nào khác mà con phải làm nữa không,thưa bệ hạ?
Dialogue: 0,1:46:06.78,1:46:08.66,Default,,0000,0000,0000,,nghĩa vụ?
Dialogue: 0,1:46:10.91,1:46:12.18,Default,,0000,0000,0000,,không
Dialogue: 0,1:46:20.52,1:46:22.56,Default,,0000,0000,0000,,ta mong con hãy cười lên lần nữa
Dialogue: 0,1:46:26.06,1:46:30.85,Default,,0000,0000,0000,,không nên khóc thương những kẻ đã đến lúc phải ra đi
Dialogue: 0,1:46:35.89,1:46:39.25,Default,,0000,0000,0000,,con sẽ sống để thấy những ngày đổi mới
Dialogue: 0,1:46:42.25,1:46:46.36,Default,,0000,0000,0000,,và không còn tuyệt vọng nữa
Dialogue: 0,1:46:52.67,1:46:56.22,Default,,0000,0000,0000,,ở đây mà có quân đội sao?
Dialogue: 0,1:46:56.55,1:46:58.30,Default,,0000,0000,0000,,có một đạo quân bị nguyền rủa
Dialogue: 0,1:47:00.34,1:47:02.77,Default,,0000,0000,0000,,rất lâu về trước,những người con của ngọn núi đã thề...
Dialogue: 0,1:47:03.30,1:47:05.94,Default,,0000,0000,0000,,...với vị vua cuối cùng của Gondor...
Dialogue: 0,1:47:06.63,1:47:08.13,Default,,0000,0000,0000,,...rằng sẽ đáp lời kêu gọi...
Dialogue: 0,1:47:08.37,1:47:10.01,Default,,0000,0000,0000,,...chiến đấu
Dialogue: 0,1:47:10.58,1:47:12.65,Default,,0000,0000,0000,,nhưng khi thời khắc đến...
Dialogue: 0,1:47:13.37,1:47:15.88,Default,,0000,0000,0000,,...khi Gondor gần thất thủ...
Dialogue: 0,1:47:16.16,1:47:17.96,Default,,0000,0000,0000,,...họ đã trốn chạy...
Dialogue: 0,1:47:18.20,1:47:21.43,Default,,0000,0000,0000,,...lẫn trốn vào bóng tối của ngọn núi
Dialogue: 0,1:47:22.62,1:47:24.85,Default,,0000,0000,0000,,và Isildur đã nguyền rủa họ...
Dialogue: 0,1:47:26.49,1:47:30.20,Default,,0000,0000,0000,,...không bao giờ được yên nghỉ cho đến khi học hoàn thành lời thề
Dialogue: 0,1:47:34.65,1:47:37.68,Default,,0000,0000,0000,,ai sẽ gọi họ vào lúc lâm nguy này?
Dialogue: 0,1:47:38.27,1:47:40.42,Default,,0000,0000,0000,,những con người đã bị quên lãng
Dialogue: 0,1:47:40.69,1:47:43.08,Default,,0000,0000,0000,,người thừa kế của người mà họ đã thề ước
Dialogue: 0,1:47:43.65,1:47:45.44,Default,,0000,0000,0000,,anh ta sẽ đến từ hướng bắc.
Dialogue: 0,1:47:46.02,1:47:47.61,Default,,0000,0000,0000,,cần hướng dẫn anh ta
Dialogue: 0,1:47:48.77,1:47:52.13,Default,,0000,0000,0000,,anh ta sẽ đi qua cánh cửa dẫn đến cái chết
Dialogue: 0,1:48:10.72,1:48:13.87,Default,,0000,0000,0000,,đến máu của tôi cũng muốn đông lại
Dialogue: 0,1:48:19.55,1:48:21.41,Default,,0000,0000,0000,,đường đi đã đóng
Dialogue: 0,1:48:22.25,1:48:24.12,Default,,0000,0000,0000,,đóng lại bởi những kẻ đã chết
Dialogue: 0,1:48:24.83,1:48:26.98,Default,,0000,0000,0000,,và người chết canh giữ nó
Dialogue: 0,1:48:27.62,1:48:29.10,Default,,0000,0000,0000,,cánh cửa đã đóng
Dialogue: 0,1:48:40.49,1:48:41.93,Default,,0000,0000,0000,,Brego!
Dialogue: 0,1:48:45.49,1:48:48.08,Default,,0000,0000,0000,,tôi không sợ cái chết
Dialogue: 0,1:49:00.88,1:49:03.19,Default,,0000,0000,0000,,ừm,việc này đúng là chưa từng thấy bao giờ
Dialogue: 0,1:49:03.46,1:49:07.94,Default,,0000,0000,0000,,người lùn không dám vào nơi loài tiên vào sao?
Dialogue: 0,1:49:08.30,1:49:09.57,Default,,0000,0000,0000,,ô
Dialogue: 0,1:49:09.80,1:49:12.51,Default,,0000,0000,0000,,ôi,sao tôi cứ phải nghe mãi thế này
Dialogue: 0,1:49:27.62,1:49:29.77,Default,,0000,0000,0000,,chúng ta phải đi thật khẽ
Dialogue: 0,1:49:30.03,1:49:31.71,Default,,0000,0000,0000,,con đường còn dài lắm
Dialogue: 0,1:49:31.95,1:49:34.91,Default,,0000,0000,0000,,người và ngựa phải có đủ sức chiến đấu khi đến đích
Dialogue: 0,1:49:40.52,1:49:43.40,Default,,0000,0000,0000,,chiến tranh không dành cho người Hobbit,ngài Meriadoc
Dialogue: 0,1:49:43.86,1:49:45.77,Default,,0000,0000,0000,,tất cả bạn bè của thần đã tham chiến
Dialogue: 0,1:49:46.02,1:49:47.82,Default,,0000,0000,0000,,thật đáng xấu hổ khi ở lại phía sau
Dialogue: 0,1:49:48.06,1:49:50.10,Default,,0000,0000,0000,,đến Minas Tirith mất 3 ngày đường
Dialogue: 0,1:49:50.35,1:49:52.63,Default,,0000,0000,0000,,và không một chiến sĩ nào của ta có thể mang cậu theo như một gánh nặng
Dialogue: 0,1:49:53.73,1:49:55.76,Default,,0000,0000,0000,,tôi muốn chiến đấu
Dialogue: 0,1:49:56.35,1:49:58.26,Default,,0000,0000,0000,,ta sẽ không nói nữa
Dialogue: 0,1:50:15.13,1:50:17.01,Default,,0000,0000,0000,,đi với tôi
Dialogue: 0,1:50:18.55,1:50:19.90,Default,,0000,0000,0000,,công nương của tôi
Dialogue: 0,1:50:20.13,1:50:22.41,Default,,0000,0000,0000,,lập đội hình,tiến lên
Dialogue: 0,1:50:22.67,1:50:25.74,Default,,0000,0000,0000,,lập đội hình,tiến lên
Dialogue: 0,1:50:28.95,1:50:30.10,Default,,0000,0000,0000,,tiến lên
Dialogue: 0,1:50:30.33,1:50:33.16,Default,,0000,0000,0000,,đến Gondor nhanh
Dialogue: 0,1:51:13.52,1:51:16.52,Default,,0000,0000,0000,,gì thế?
Dialogue: 0,1:51:16.76,1:51:18.24,Default,,0000,0000,0000,,anh thấy gì?
Dialogue: 0,1:51:18.55,1:51:20.59,Default,,0000,0000,0000,,tôi thấy bóng người
Dialogue: 0,1:51:23.34,1:51:24.66,Default,,0000,0000,0000,,và ngựa nữa
Dialogue: 0,1:51:25.47,1:51:26.62,Default,,0000,0000,0000,,ở đâu
Dialogue: 0,1:51:26.84,1:51:29.68,Default,,0000,0000,0000,,chỉ phảng phất như một áng mây
Dialogue: 0,1:51:32.22,1:51:34.33,Default,,0000,0000,0000,,gương giáo lên
Dialogue: 0,1:51:35.05,1:51:38.35,Default,,0000,0000,0000,,như một bụi cây mùa đông được phủ trong sương mù
Dialogue: 0,1:51:39.75,1:51:41.82,Default,,0000,0000,0000,,người chết đang theo sau
Dialogue: 0,1:51:43.12,1:51:44.56,Default,,0000,0000,0000,,họ phải được triệu gọi
Dialogue: 0,1:51:45.29,1:51:47.04,Default,,0000,0000,0000,,người chết?
Dialogue: 0,1:51:47.50,1:51:49.14,Default,,0000,0000,0000,,triệu hồi?
Dialogue: 0,1:51:49.62,1:51:51.42,Default,,0000,0000,0000,,tôi biết mà
Dialogue: 0,1:51:54.83,1:51:56.50,Default,,0000,0000,0000,,rất tốt
Dialogue: 0,1:51:57.29,1:51:59.28,Default,,0000,0000,0000,,tốt lắm!Legolas!
Dialogue: 0,1:52:37.26,1:52:38.62,Default,,0000,0000,0000,,đừng nhìn xuống
Dialogue: 0,1:53:31.19,1:53:36.11,Default,,0000,0000,0000,,kẻ nào vào lãnh địa của ta?
Dialogue: 0,1:53:40.07,1:53:42.17,Default,,0000,0000,0000,,kẻ sẽ có lòng trung thành của người
Dialogue: 0,1:53:45.22,1:53:50.34,Default,,0000,0000,0000,,người chết sẽ không tha thứ cho người sống đi qua
Dialogue: 0,1:53:50.89,1:53:53.45,Default,,0000,0000,0000,,ngươi sẽ cho ta qua
Dialogue: 0,1:54:17.04,1:54:20.27,Default,,0000,0000,0000,,đường đi đã đóng
Dialogue: 0,1:54:21.58,1:54:25.86,Default,,0000,0000,0000,,nó được tạo ra bởi những người đã chết
Dialogue: 0,1:54:26.42,1:54:30.40,Default,,0000,0000,0000,,và cái chết nắm giữ chúng
Dialogue: 0,1:54:40.10,1:54:43.05,Default,,0000,0000,0000,,lối đi đã khép
Dialogue: 0,1:54:44.93,1:54:47.44,Default,,0000,0000,0000,,giờ các người phải chết
Dialogue: 0,1:54:51.43,1:54:54.14,Default,,0000,0000,0000,,ta kêu gọi các người hoàn thành lời thề
Dialogue: 0,1:54:54.58,1:54:59.34,Default,,0000,0000,0000,,không ai ngoài vua của Gondor có thể ra lệnh cho ta
Dialogue: 0,1:55:05.12,1:55:07.19,Default,,0000,0000,0000,,thanh gươm đó đã vỡ rồi
Dialogue: 0,1:55:09.92,1:55:12.03,Default,,0000,0000,0000,,nó đã được tái tạo
Dialogue: 0,1:55:18.03,1:55:21.51,Default,,0000,0000,0000,,chiến đấu với chúng ta
Dialogue: 0,1:55:21.74,1:55:24.17,Default,,0000,0000,0000,,...và lấy lại danh dự của các người
Dialogue: 0,1:55:28.45,1:55:29.72,Default,,0000,0000,0000,,các ngươi nói gì?
Dialogue: 0,1:55:41.98,1:55:43.45,Default,,0000,0000,0000,,các ngươi nói gì?
Dialogue: 0,1:55:44.27,1:55:45.98,Default,,0000,0000,0000,,chỉ phí thời gian thôi,Aragorn
Dialogue: 0,1:55:46.22,1:55:49.26,Default,,0000,0000,0000,,bọn chúng không có danh dự khi sống,và giờ khi chết cũng thế
Dialogue: 0,1:55:49.89,1:55:52.28,Default,,0000,0000,0000,,ta là người kế thừa của Isidur
Dialogue: 0,1:55:52.93,1:55:54.81,Default,,0000,0000,0000,,chiến đấu vì ta
Dialogue: 0,1:55:55.47,1:55:58.91,Default,,0000,0000,0000,,và ta sẽ giúp các người thực hiện lời thề
Dialogue: 0,1:56:02.97,1:56:04.85,Default,,0000,0000,0000,,các ngươi nói gì?
Dialogue: 0,1:56:12.83,1:56:14.95,Default,,0000,0000,0000,,ta xin hứa
Dialogue: 0,1:56:15.33,1:56:19.17,Default,,0000,0000,0000,,chiến đấu,và ta sẽ giải phóng các người khỏi sự đày ải này
Dialogue: 0,1:56:20.79,1:56:22.23,Default,,0000,0000,0000,,các ngươi nói gì?
Dialogue: 0,1:56:25.58,1:56:28.42,Default,,0000,0000,0000,,đứng dậy đi,bọn phản bội
Dialogue: 0,1:56:55.45,1:56:56.43,Default,,0000,0000,0000,,ra ngoài
