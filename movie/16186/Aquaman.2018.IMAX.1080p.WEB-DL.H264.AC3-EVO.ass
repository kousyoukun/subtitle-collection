Dialogue: 0,0:00:22.92,0:00:29.92,Default,,0,0,0,,vietsub: quanphamkt\N phimkhoahochay.weebly.com
Dialogue: 0,0:00:31.92,0:00:35.92,Default,,0,0,0,,Vịnh Amnesty, Maine năm 1985
Dialogue: 0,0:01:05.99,0:01:07.92,Default,,0,0,0,,{\i1}Jules Verne\N đã từng viết,{\i}
Dialogue: 0,0:01:07.92,0:01:10.42,Default,,0,0,0,,{\i1}thả trôi hai con tàu\N vào vùng biển vắng{\i}
Dialogue: 0,0:01:10.42,0:01:13.76,Default,,0,0,0,,{\i1}không quan tâm có gió hay thủy triều,\Nthì chúng vẫn sẽ trôi dạt vào nhau."{\i}
Dialogue: 0,0:01:35.85,0:01:40.52,Default,,0,0,0,,{\i1}Đó là cách cha mẹ tôi gặp nhau.\NNhư số mệnh hai con tàu đã đến với nhau.{\i}
Dialogue: 0,0:01:54.40,0:01:55.60,Default,,0,0,0,,Ổn rồi, không sao đâu.
Dialogue: 0,0:02:03.31,0:02:07.64,Default,,0,0,0,,Bình tĩnh nào. Cô đang bị ngạt nước
Dialogue: 0,0:02:21.09,0:02:23.59,Default,,0,0,0,,{\i1}Họ đến từ\N các thế giới khác nhau.{\i}
Dialogue: 0,0:02:24.96,0:02:27.06,Default,,0,0,0,,{\i1}Nhưng cuộc sống, giống như biển cả,{\i}
Dialogue: 0,0:02:27.07,0:02:29.54,Default,,0,0,0,,{\i1}có cách mang mọi người\N lại gần nhau.,{\i}
Dialogue: 0,0:03:12.61,0:03:13.88,Default,,0,0,0,,Uh...
Dialogue: 0,0:03:15.18,0:03:16.92,Default,,0,0,0,,Tôi sẽ làm cho cô\N một ít trứng.
Dialogue: 0,0:03:19.29,0:03:20.68,Default,,0,0,0,,Đừng ăn con chó của tôi nhé?
Dialogue: 0,0:03:24.29,0:03:25.62,Default,,0,0,0,,Không sao. Ổn mà.
Dialogue: 0,0:03:25.62,0:03:27.16,Default,,0,0,0,,Tôi pha trà rồi đây.
Dialogue: 0,0:03:32.13,0:03:33.63,Default,,0,0,0,,Hãy thử uống đi.
Dialogue: 0,0:03:33.63,0:03:34.90,Default,,0,0,0,, Nó sẽ làm cho cô khá hơn.
Dialogue: 0,0:03:55.82,0:03:57.96,Default,,0,0,0,,Vậy cô là ai?
Dialogue: 0,0:04:02.19,0:04:03.73,Default,,0,0,0,,Atlanna.
Dialogue: 0,0:04:05.50,0:04:07.23,Default,,0,0,0,,Nữ hoàng của Atlantis.
Dialogue: 0,0:04:08.97,0:04:10.87,Default,,0,0,0,,Ồ
Dialogue: 0,0:04:10.87,0:04:15.84,Default,,0,0,0,,Nữ hoàng của Atlantis, thú vị đấy!
Dialogue: 0,0:04:46.97,0:04:49.04,Default,,0,0,0,,{\i1}Bà ấy đã chạy trốn\N khỏi một cuộc hôn nhân sắp đặt.{\i}
Dialogue: 0,0:04:49.04,0:04:51.07,Default,,0,0,0,,{\i1}Bỏ lại sau lưng cả thế giới của mình.{\i}
Dialogue: 0,0:04:51.08,0:04:53.28,Default,,0,0,0,,{\i1}Nhưng trong ngọn hải đăng của cha tôi,{\i}
Dialogue: 0,0:04:53.28,0:04:55.75,Default,,0,0,0,,{\i1}Bà ấy đã tìm thấy\Nthứ gì đó bất ngờ.{\i}
Dialogue: 0,0:04:58.38,0:05:02.35,Default,,0,0,0,,{\i1}Và cha tôi, đã tìm thấy\N tình yêu của đời mình.{\i}
Dialogue: 0,0:05:06.99,0:05:08.56,Default,,0,0,0,,{\i1}Cơn bão cấp 4 còn được gọi là Arthur{\i}
Dialogue: 0,0:05:08.56,0:05:11.46,Default,,0,0,0,,{\i1}sẽ đổ bộ vào đất liền\N14h chiều nay{\i}
Dialogue: 0,0:05:11.46,0:05:13.80,Default,,0,0,0,,Đặt tên cho nó là Arthur thì sao nhỉ?
Dialogue: 0,0:05:14.40,0:05:15.87,Default,,0,0,0,,Theo tên cơn bão à.
Dialogue: 0,0:05:15.87,0:05:20.57,Default,,0,0,0,,Đó cũng là tên một vị vua.
Dialogue: 0,0:05:20.57,0:05:23.37,Default,,0,0,0,,Thằng bé còn hơn thế nữa.
Dialogue: 0,0:05:23.38,0:05:27.04,Default,,0,0,0,,Đứa bé này là nhân chứng sống mà\Nnhân dân cả hai chủng tộc chúng ta\Ncó thể cùng chung sống với nhau.
Dialogue: 0,0:05:27.05,0:05:29.91,Default,,0,0,0,,Nó sẽ có thể hợp nhất thế giới\Ncủa chúng ta một ngày nào đó.
Dialogue: 0,0:05:37.96,0:05:42.09,Default,,0,0,0,,Và Cây đinh ba chỉ có thể được sử dụng\Nbởi người Atlantis mạnh mẽ nhất.
Dialogue: 0,0:05:42.09,0:05:47.43,Default,,0,0,0,,Và nó đã mang lại cho vua\N quyền làm chủ 7 đại dương.
Dialogue: 0,0:05:47.43,0:05:51.64,Default,,0,0,0,,Làm cho ông ta trở nên mạnh mẽ\Nđến nỗi cả đại dương sẽ trở nên ghen tị,
Dialogue: 0,0:05:51.64,0:05:56.11,Default,,0,0,0,,Nó đã phá hủy Atlantis bằng\Nmột trận động đất khủng khiếp.
Dialogue: 0,0:05:56.11,0:05:58.51,Default,,0,0,0,,Khiến Atlantis chìm xuống đáy đại dương.
Dialogue: 0,0:05:58.51,0:06:03.11,Default,,0,0,0,,Nhưng truyền thuyết kể\N rằng một ngày nào đó
Dialogue: 0,0:06:03.11,0:06:05.45,Default,,0,0,0,,Ai đó, có thể là một vị vua mới\Nsẽ sử dụng sức mạnh của cây đinh ba
Dialogue: 0,0:06:05.45,0:06:08.68,Default,,0,0,0,,để đưa Atlantis quay trở lại.
Dialogue: 0,0:06:14.83,0:06:16.76,Default,,0,0,0,,Tom, ẵm nó!
Dialogue: 0,0:06:35.05,0:06:36.41,Default,,0,0,0,,{\i1}Nữ hoàng Atlan.{\i}
Dialogue: 0,0:06:36.41,0:06:38.55,Default,,0,0,0,,{\i1}Theo lệnh của\N Vua Orvax,{\i}
Dialogue: 0,0:06:38.55,0:06:41.65,Default,,0,0,0,,{\i1}bà phải quay trở lại Atlantis{\i}
Dialogue: 0,0:06:41.65,0:06:42.92,Default,,0,0,0,,Atlanna!
Dialogue: 0,0:06:45.52,0:06:47.99,Default,,0,0,0,,{\i1}Nhưng mẹ tôi không ở\N trên đất liền được bao lâu{\i}
Dialogue: 0,0:06:49.83,0:06:52.63,Default,,0,0,0,,{\i1}vì vị vua đang trị vì Atlantis\N đã tìm ra nơi bà ẩn náu.{\i}
Dialogue: 0,0:07:05.21,0:07:07.48,Default,,0,0,0,,- Atlanna!\N- Tom!
Dialogue: 0,0:07:07.48,0:07:09.38,Default,,0,0,0,,- Vũ khí của em!\N- Đây!
Dialogue: 0,0:07:35.84,0:07:38.77,Default,,0,0,0,,{\i1}Hai thế giới của bà và ông ấy\Nđáng lẽ không nên gặp nhau{\i}
Dialogue: 0,0:07:38.78,0:07:42.18,Default,,0,0,0,,{\i1}và tôi là "sản phẩm" của\N một tình yêu không nên có.{\i}
Dialogue: 0,0:07:42.18,0:07:44.15,Default,,0,0,0,,Em không cần phải làm điều này,\NAtlanna. Làm ơn đi.
Dialogue: 0,0:07:44.15,0:07:48.28,Default,,0,0,0,,Không. Họ sẽ luôn tìm thấy em.
Dialogue: 0,0:07:48.29,0:07:50.05,Default,,0,0,0,,Lần tới họ sẽ \Ngửi một đội quân.
Dialogue: 0,0:07:52.12,0:07:56.56,Default,,0,0,0,,Em phải quay lại.\NĐó là cách duy nhất để cứu con chúng ta.
Dialogue: 0,0:07:59.83,0:08:01.57,Default,,0,0,0,,Và cứu anh nữa.
Dialogue: 0,0:08:06.14,0:08:08.17,Default,,0,0,0,,Nơi em ở,
Dialogue: 0,0:08:09.81,0:08:12.81,Default,,0,0,0,,biển cả sẽ mang nước mắt của chúng ta đi.
Dialogue: 0,0:08:15.35,0:08:16.35,Default,,0,0,0,,Không phải ở đây.
Dialogue: 0,0:08:17.68,0:08:19.35,Default,,0,0,0,,Ở đây em sẽ cảm nhận được chúng .
Dialogue: 0,0:08:27.22,0:08:31.56,Default,,0,0,0,,Em sẽ trở lại cho đến khi\N mọi việc lắng xuống.
Dialogue: 0,0:08:32.43,0:08:33.56,Default,,0,0,0,,Một ngày nào đó.
Dialogue: 0,0:08:33.56,0:08:36.50,Default,,0,0,0,,Ngay tại đây. Vào lúc bình minh.
Dialogue: 0,0:08:39.44,0:08:41.00,Default,,0,0,0,,Chúng ta sẽ lại gặp nhau.
Dialogue: 0,0:08:42.87,0:08:45.74,Default,,0,0,0,,Con hãy mạnh mẽ lên,\N hoàng tử bé nhỏ của mẹ.
Dialogue: 0,0:08:45.74,0:08:47.94,Default,,0,0,0,,Mẹ sẽ luôn ở bên con.
Dialogue: 0,0:08:56.22,0:08:59.42,Default,,0,0,0,,Đừng để nó quên em,\N anh cũng đừng quên em nhé!
Dialogue: 0,0:09:24.25,0:09:26.85,Default,,0,0,0,,{\i1}Khởi nguồn của sự sống\N được bắt đầu từ đại dương{\i0}
Dialogue: 0,0:09:26.85,0:09:29.12,Default,,0,0,0,,{\i1}vì thế để hiểu rõ chính mình{\i0}
Dialogue: 0,0:09:29.12,0:09:31.09,Default,,0,0,0,,{\i1}chúng ta phải quay trở lại \Nnơi khởi nguồn.{\i0}
Dialogue: 0,0:09:31.09,0:09:35.96,Default,,0,0,0,,Ngày nay, chúng ta có bản đồ sao Hỏa\Ncòn chi tiết hơn bản đồ đáy biển của trái đất.
Dialogue: 0,0:09:35.96,0:09:38.09,Default,,0,0,0,,Đi nào. \NĐến đây, coi cá nè.
Dialogue: 0,0:09:38.09,0:09:40.56,Default,,0,0,0,,- Thôi nào, đến đây.\N- Các em!
Dialogue: 0,0:09:40.56,0:09:42.83,Default,,0,0,0,, Các em dừng lại đi.
Dialogue: 0,0:09:42.83,0:09:45.00,Default,,0,0,0,,Đi nào
Dialogue: 0,0:09:45.00,0:09:48.94,Default,,0,0,0,,Xin chào, chú cá.
Dialogue: 0,0:09:48.94,0:09:52.07,Default,,0,0,0,,Tên tớ là Arthur. Tên các bạn là gì?
Dialogue: 0,0:09:52.07,0:09:55.28,Default,,0,0,0,,Các bạn có đói không? Tớ cũng vậy
Dialogue: 0,0:09:55.28,0:09:57.81,Default,,0,0,0,,Xem kìa. Arthur đang nói chuyện với cá.
Dialogue: 0,0:09:57.81,0:09:59.51,Default,,0,0,0,,Mày thật là điên, Arthur.
Dialogue: 0,0:09:59.52,0:10:02.15,Default,,0,0,0,,- Dừng lại! \N - Mày đang làm gì vậy, đồ quái dị!
Dialogue: 0,0:10:02.15,0:10:04.32,Default,,0,0,0,,Dừng lại! Đừng!
Dialogue: 0,0:10:04.32,0:10:05.52,Default,,0,0,0,,Hãy để tớ yên!
Dialogue: 0,0:11:08.52,0:11:12.52,Default,,0,0,0,,Ngày hôm nay
Dialogue: 0,0:12:03.04,0:12:04.30,Default,,0,0,0,,Ngươi là ai?
Dialogue: 0,0:12:04.31,0:12:06.01,Default,,0,0,0,,Nào, đi nào.\NDi chuyển đi.
Dialogue: 0,0:12:06.01,0:12:08.18,Default,,0,0,0,,Nào. Mau lên.
Dialogue: 0,0:12:17.29,0:12:18.99,Default,,0,0,0,,Thưa ngài, chúng tôi đã bắt\N được thuyền trưởng.
Dialogue: 0,0:12:18.99,0:12:20.85,Default,,0,0,0,,Tín hiệu nhiễu cầu cứu đã bị tắt.
Dialogue: 0,0:12:20.86,0:12:22.49,Default,,0,0,0,, Không ai nghe thấy đâu.
Dialogue: 0,0:12:22.49,0:12:24.89,Default,,0,0,0,,Nhưng họ nghe thấy rồi.\NÔng có thể truy cập lại.
Dialogue: 0,0:12:24.89,0:12:29.53,Default,,0,0,0,,Tao sẽ thỏa thuận với mày.\NTao sẽ không cho mày biết cách làm thuyền trưởng,
Dialogue: 0,0:12:29.53,0:12:31.73,Default,,0,0,0,,và mày đừng có dạy đời tao cách làm cướp biển.
Dialogue: 0,0:12:42.81,0:12:44.48,Default,,0,0,0,,Phần còn lại của phi hành đoàn ở đâu?
Dialogue: 0,0:12:44.48,0:12:46.31,Default,,0,0,0,,Họ đã quyết định rút lui,
Dialogue: 0,0:12:46.31,0:12:48.41,Default,,0,0,0,,và đang cố thủ trong khoang ngư lôi.
Dialogue: 0,0:12:48.42,0:12:50.12,Default,,0,0,0,,Danh tiếng của chúng ta vang vọng khắp nơi.
Dialogue: 0,0:12:50.12,0:12:51.78,Default,,0,0,0,,Danh tiếng của con chứ?
Dialogue: 0,0:12:51.79,0:12:53.22,Default,,0,0,0,, Đây là chiến thắng của con.
Dialogue: 0,0:12:58.03,0:12:59.43,Default,,0,0,0,,Đây.
Dialogue: 0,0:13:00.93,0:13:03.20,Default,,0,0,0,,Con không lấy mẩu sắt \Nrác rưởi này đâu,
Dialogue: 0,0:13:03.20,0:13:04.63,Default,,0,0,0,,Nó là tình yêu của cuộc đời cha mà.
Dialogue: 0,0:13:06.13,0:13:08.47,Default,,0,0,0,,Cha chưa bao giờ kể cho con câu chuyện về nó.
Dialogue: 0,0:13:09.40,0:13:11.91,Default,,0,0,0,,Đây là của ông nội con.
Dialogue: 0,0:13:11.91,0:13:15.68,Default,,0,0,0,,Ông là một trong những người nhái\Nđầu tiên chiến đấu trong Thế chiến hai.
Dialogue: 0,0:13:15.68,0:13:21.41,Default,,0,0,0,,Ông ấy là một chiến binh dưới nước, \Nđơn vị của ông ấy có biệt danh là Manta.
Dialogue: 0,0:13:21.42,0:13:24.18,Default,,0,0,0,,Nhưng sau chiến tranh, \Nđất nước đã lãng quên mất ổng
Dialogue: 0,0:13:24.19,0:13:28.76,Default,,0,0,0,,Vì vậy, ổng đã quay trở lại biển,\Nđể sinh tồn bằng trí thông minh của mình.
Dialogue: 0,0:13:29.29,0:13:31.32,Default,,0,0,0,,Và con dao này,
Dialogue: 0,0:13:31.33,0:13:34.36,Default,,0,0,0,,ông ấy đã đưa nó\Ncho cha khi cha bằng tuổi con.
Dialogue: 0,0:13:37.16,0:13:38.87,Default,,0,0,0,,Và bây giờ, nó là của con, con trai à.
Dialogue: 0,0:14:07.70,0:14:09.63,Default,,0,0,0,,Chúng ta vừa đâm trúng một cái gì đó.
Dialogue: 0,0:14:11.13,0:14:14.30,Default,,0,0,0,,Không. Một cái gì đó đâm vào chúng ta.
Dialogue: 0,0:14:14.30,0:14:15.80,Default,,0,0,0,,Thưa ngài, có gì đó ngoài kia.
Dialogue: 0,0:14:15.80,0:14:17.00,Default,,0,0,0,,Một chiếc tàu ngầm khác?
Dialogue: 0,0:14:17.00,0:14:18.64,Default,,0,0,0,,Không. Tôi nghĩ đó là một người đàn ông.
Dialogue: 0,0:15:07.05,0:15:08.59,Default,,0,0,0,,Đó không phải là người.
Dialogue: 0,0:15:12.19,0:15:16.66,Default,,0,0,0,,{\i1}Alpha, ở bên phải. Đội Bravo, dọn đường đi.{\i}
Dialogue: 0,0:15:38.22,0:15:39.62,Default,,0,0,0,,Tôi có quyền được lên tàu chứ?.
Dialogue: 0,0:17:10.14,0:17:11.41,Default,,0,0,0,,Đó là Aquaman !
Dialogue: 0,0:17:12.04,0:17:15.04,Default,,0,0,0,,Ra khỏi đây nhanh đi !\NĐể tôi giải quyết chúng, đã trễ mất bữa tiệc trên kia rôì.
Dialogue: 0,0:17:39.84,0:17:42.88,Default,,0,0,0,,Tao đã chờ đợi \Nquá lâu cho việc này.
Dialogue: 0,0:17:56.52,0:17:58.12,Default,,0,0,0,,Tao có nên biết mày là ai không nhỉ?
Dialogue: 0,0:17:59.36,0:18:01.53,Default,,0,0,0,,Tao cướp bóc khắp nơi trên đại dương.
Dialogue: 0,0:18:02.49,0:18:03.76,Default,,0,0,0,,Mày là Aquaman.
Dialogue: 0,0:18:03.76,0:18:06.03,Default,,0,0,0,,Trước sau gì tao với mày\N cũng đấu với nhau thôi.
Dialogue: 0,0:18:10.04,0:18:11.47,Default,,0,0,0,,Vậy hãy đừng để điều đó xảy ra lần nữa.
Dialogue: 0,0:18:12.00,0:18:13.07,Default,,0,0,0,,Lên
Dialogue: 0,0:19:07.36,0:19:08.56,Default,,0,0,0,,Đau đấy.
Dialogue: 0,0:19:16.34,0:19:17.74,Default,,0,0,0,,Tên khốn, dám bắn ta à...
Dialogue: 0,0:19:21.91,0:19:23.21,Default,,0,0,0,,Cha!
Dialogue: 0,0:19:25.24,0:19:27.04,Default,,0,0,0,,Cha sao?
Dialogue: 0,0:19:29.51,0:19:30.72,Default,,0,0,0,,Đó là con mày à?
Dialogue: 0,0:19:35.95,0:19:37.15,Default,,0,0,0,,Xấu hổ với mày.
Dialogue: 0,0:19:39.06,0:19:40.82,Default,,0,0,0,,Chúng mày vớ vào mớ hỗn độn này,
Dialogue: 0,0:19:40.83,0:19:42.09,Default,,0,0,0,,thì hãy tự mình thoát ra đi.
Dialogue: 0,0:19:42.09,0:19:43.10,Default,,0,0,0,,Khốn kiếp!
Dialogue: 0,0:19:52.54,0:19:53.91,Default,,0,0,0,,Cha!
Dialogue: 0,0:20:02.25,0:20:03.28,Default,,0,0,0,,Cha!
Dialogue: 0,0:20:06.32,0:20:07.68,Default,,0,0,0,,Đợi đã!
Dialogue: 0,0:20:07.69,0:20:09.52,Default,,0,0,0,, Giúp tao, ổng bị mắc kẹt!
Dialogue: 0,0:20:09.52,0:20:11.29,Default,,0,0,0,,Mày không thể bỏ mặc ông ấy như thế này!
Dialogue: 0,0:20:13.12,0:20:14.52,Default,,0,0,0,,Làm ơn đi!
Dialogue: 0,0:20:14.53,0:20:18.60,Default,,0,0,0,,Mày đã giết người vô tội.\NMày hãy xin biển cả sự tha thứ.
Dialogue: 0,0:20:25.60,0:20:27.60,Default,,0,0,0,,Bị vỡ
Dialogue: 0,0:20:32.21,0:20:33.51,Default,,0,0,0,,Ra khỏi đây! Con hiểu chưa!
Dialogue: 0,0:20:33.51,0:20:35.18,Default,,0,0,0,,Im đi, con sẽ không rời xa cha!
Dialogue: 0,0:20:35.18,0:20:39.15,Default,,0,0,0,,Con cần phải sống để có thể\N giết chết thằng chó đó.
Dialogue: 0,0:20:42.42,0:20:44.75,Default,,0,0,0,,- Bây giờ, hãy đi đi!\N- Không!
Dialogue: 0,0:20:44.76,0:20:46.32,Default,,0,0,0,,- Đi!\N- Im đi!
Dialogue: 0,0:20:51.16,0:20:52.16,Default,,0,0,0,,Không!
Dialogue: 0,0:20:52.83,0:20:53.83,Default,,0,0,0,,Không!
Dialogue: 0,0:21:11.62,0:21:13.29,Default,,0,0,0,,Mẹ kiếp!
Dialogue: 0,0:21:13.85,0:21:14.85,Default,,0,0,0,,Đi.
Dialogue: 0,0:22:33.26,0:22:34.86,Default,,0,0,0,,Con lúc nào cũng luôn biết nơi để tìm ra cha.
Dialogue: 0,0:22:35.70,0:22:37.07,Default,,0,0,0,,Như một thói quen mà.
Dialogue: 0,0:22:45.94,0:22:48.74,Default,,0,0,0,,- Thôi nào, cha sẽ đãi con bữa sáng.\N- Được rồi
Dialogue: 0,0:22:51.78,0:22:55.05,Default,,0,0,0,,Con có muốn cha đổ nó\N vào bình sữa em bé không?
Dialogue: 0,0:22:58.12,0:22:59.89,Default,,0,0,0,,Làm thế nào mà con có thể thở dưới nước,
Dialogue: 0,0:22:59.89,0:23:02.29,Default,,0,0,0,,Nhưng con lại uống rượu\N thua cha trên bàn nhậu thế?
Dialogue: 0,0:23:02.29,0:23:05.13,Default,,0,0,0,,Đó là siêu năng lực của cha.
Dialogue: 0,0:23:05.13,0:23:06.73,Default,,0,0,0,,{\i1}Một tàu ngầm của người Nga{\i}
Dialogue: 0,0:23:06.73,0:23:08.96,Default,,0,0,0,,{\ i1} bị tấn công vào đêm qua\Nbởi cướp biển công nghệ cao. {\ i}
Dialogue: 0,0:23:08.97,0:23:11.10,Default,,0,0,0,,{\i1}Một nhóm hải tặc bị nghi ngờ{\i}
Dialogue: 0,0:23:11.10,0:23:13.97,Default,,0,0,0,,{\ i1} liên quan đến vụ mất tích\N của Navy được phân loại là {\ i}
Dialogue: 0,0:23:13.97,0:23:16.47,Default,,0,0,0,,{\ i1} tàu ngầm tàng hình nguyên mẫu. {\ i}
Dialogue: 0,0:23:16.47,0:23:19.81,Default,,0,0,0,,{\i1}Và trong quá trình điều tra, chúng tôi hiện\Ncó các báo cáo chưa được xác nhận{\i}
Dialogue: 0,0:23:19.81,0:23:23.55,Default,,0,0,0,,{\i1}rằng một người siêu nhân được\N cộng đồng mạng xã hội gọi là Aquaman{\i}
Dialogue: 0,0:23:23.55,0:23:25.42,Default,,0,0,0,,{\i1}là kẻ đã rất táo bạo để giải cứu vụ này...{\i}
Dialogue: 0,0:23:26.52,0:23:27.85,Default,,0,0,0,,Không phải con.
Dialogue: 0,0:23:27.85,0:23:32.02,Default,,0,0,0,,Ôi chết tiệt. Con đang làm việc đó phải không?
Dialogue: 0,0:23:32.02,0:23:34.29,Default,,0,0,0,,Vulco đã đào tạo con còn gì?
Dialogue: 0,0:23:34.29,0:23:38.53,Default,,0,0,0,,Cha biết con không thể đứng ngoài cuộc mãi mãi.
Dialogue: 0,0:23:38.53,0:23:40.63,Default,,0,0,0,,Mẹ con luôn biết con là người đặc biệt.
Dialogue: 0,0:23:40.63,0:23:42.20,Default,,0,0,0,,Thôi đừng nói nữa.
Dialogue: 0,0:23:42.20,0:23:45.27,Default,,0,0,0,,Bà ấy tin rằng con là người hợp nhất hai thế giới của chúng ta.
Dialogue: 0,0:23:45.27,0:23:46.44,Default,,0,0,0,,Cha vui lòng dừng lại đi.
Dialogue: 0,0:23:49.64,0:23:53.61,Default,,0,0,0,,Dù vấn đề của Atlantis là gì, con vẫn tuyệt vời hơn họ.
Dialogue: 0,0:23:53.61,0:23:55.64,Default,,0,0,0,,Atlantis đã giết mẹ của con.
Dialogue: 0,0:23:55.65,0:23:57.14,Default,,0,0,0,,Con không biết điều đó có chắc chắn không mà?.
Dialogue: 0,0:23:57.15,0:23:58.51,Default,,0,0,0,,Có, con chắc chắn 100%.
Dialogue: 0,0:23:58.51,0:24:02.32,Default,,0,0,0,,Họ giết bà ấy, vì bà đã yêu cha và sinh ra con.
Dialogue: 0,0:24:02.92,0:24:04.52,Default,,0,0,0,,Và cha biết điều đó.
Dialogue: 0,0:24:08.76,0:24:12.76,Default,,0,0,0,,Con trai, một ngày nào đó con phải ngừng tự trách mình.
Dialogue: 0,0:24:18.97,0:24:20.20,Default,,0,0,0,,Này anh bạn.
Dialogue: 0,0:24:21.64,0:24:24.04,Default,,0,0,0,,Anh bạn là cậu bé người cá trên TV à?
Dialogue: 0,0:24:25.28,0:24:26.41,Default,,0,0,0,,Ồ, tuyệt.
Dialogue: 0,0:24:32.68,0:24:36.08,Default,,0,0,0,,Là người cá. Anh muốn gì?
Dialogue: 0,0:24:36.08,0:24:38.09,Default,,0,0,0,,Tôi sẽ cho anh bạn biết những gì tôi muốn.
Dialogue: 0,0:24:41.76,0:24:43.46,Default,,0,0,0,,Tôi có thể chụp ảnh với anh bạn không?
Dialogue: 0,0:24:43.46,0:24:46.39,Default,,0,0,0,,Anh bạn giống như anh hùng của chúng tôi,
Dialogue: 0,0:24:46.40,0:24:50.03,Default,,0,0,0,,Có ý nghĩa với chúng tôi lắm.\NNhanh thôi.
Dialogue: 0,0:24:54.60,0:24:55.94,Default,,0,0,0,,Sao cũng được.
Dialogue: 0,0:24:55.94,0:24:57.54,Default,,0,0,0,,- Được rồi. Làm nhanh đi\N- Đừng chạm vào tôi.
Dialogue: 0,0:24:57.54,0:24:59.84,Default,,0,0,0,,Đúng, đó chính xác là những gì tôi muốn làm ở đây.
Dialogue: 0,0:24:59.84,0:25:02.44,Default,,0,0,0,,Đứng sát vào chúng tôi đi. \NMọi người đều cùng cười lên.
Dialogue: 0,0:25:20.80,0:25:23.80,Default,,0,0,0,,Ở một nơi nào đó dưới đáy Đại Tây Dương
Dialogue: 0,0:25:55.80,0:25:58.83,Default,,0,0,0,,Hắn hiểu tầm quan trọng của cuộc gặp mặt này.
Dialogue: 0,0:25:58.84,0:26:00.80,Default,,0,0,0,,Hắn sẽ nghe anh thôi.
Dialogue: 0,0:26:19.95,0:26:22.16,Default,,0,0,0,,- Vua Orm.\N- Vua Nereus.
Dialogue: 0,0:26:22.16,0:26:25.59,Default,,0,0,0,,Theo tôi, anh đã chọn một địa điểm để họp\Nquá gần thềm lục địa của người mặt đất đấy.
Dialogue: 0,0:26:25.59,0:26:28.43,Default,,0,0,0,,Ngài không nhận ra hội đồng của các vị vua à?
Dialogue: 0,0:26:28.43,0:26:31.83,Default,,0,0,0,,Vào thời của Atlan,\NKhi bảy Vương quốc là một,
Dialogue: 0,0:26:31.83,0:26:34.53,Default,,0,0,0,,tổ tiên của chúng ta đã tập hợp ở đây.
Dialogue: 0,0:26:34.54,0:26:37.24,Default,,0,0,0,,Ở đây ở đầu bàn là vị vua đứng đầu Atlantis.
Dialogue: 0,0:26:38.04,0:26:40.94,Default,,0,0,0,,Chủng tộc Xebel luôn ở bên cạnh ông ấy.
Dialogue: 0,0:26:40.94,0:26:42.81,Default,,0,0,0,,Chủng tộc Brine, chủng tộc người cá\N của vương quốc Tritonis,
Dialogue: 0,0:26:42.81,0:26:45.04,Default,,0,0,0,,Chủng tộc Desert và Trench chưa sụp đổ.
Dialogue: 0,0:26:45.05,0:26:47.15,Default,,0,0,0,,Còn vương quốc bí ẩn nào đó\N dưới biển sâu còn chưa biến mất.
Dialogue: 0,0:26:47.15,0:26:51.12,Default,,0,0,0,,Cùng nhau, chúng ta là đế chế vĩ đại nhất\Nmà thế giới từng thấy.
Dialogue: 0,0:26:51.12,0:26:53.12,Default,,0,0,0,,Nhưng bây giờ, ngồi trên ngai vàng của Atlan,
Dialogue: 0,0:26:53.12,0:26:55.82,Default,,0,0,0,,đang bị kìm hãm bởi luật pháp cổ hủ\N và nền chính trị lạc hậu
Dialogue: 0,0:26:55.82,0:26:57.99,Default,,0,0,0,,Trong khi mối đe dọa ở trên đầu\N chúng ta ngày càng gia tăng.
Dialogue: 0,0:26:57.99,0:27:00.30,Default,,0,0,0,,Bạo lực luôn làm vấy bẩn bề mặt đất và đại dương.
Dialogue: 0,0:27:01.10,0:27:02.53,Default,,0,0,0,,Họ sẽ tự hủy diệt nhau thôi.
Dialogue: 0,0:27:02.53,0:27:05.37,Default,,0,0,0,,Không, phải hủy diệt chúng\N trước khi chúng tiêu diệt ta.
Dialogue: 0,0:27:05.37,0:27:08.64,Default,,0,0,0,,Chúng ta đã ẩn nấp đủ lâu rồi.
Dialogue: 0,0:27:08.64,0:27:12.44,Default,,0,0,0,,Đã đến lúc Atlantis trở lại.
Dialogue: 0,0:27:12.44,0:27:14.61,Default,,0,0,0,,Chúng tôi tôn vinh Vua Nereus
Dialogue: 0,0:27:16.01,0:27:19.41,Default,,0,0,0,,bằng cách mời người Xebel trở thành vương quốc đầu tiên
Dialogue: 0,0:27:19.41,0:27:21.71,Default,,0,0,0,,gia nhập liên minh của Vua Orm.
Dialogue: 0,0:27:21.72,0:27:23.55,Default,,0,0,0,,Vulko, ông làm như đó là sự lựa chọn à?
Dialogue: 0,0:27:23.55,0:27:25.72,Default,,0,0,0,,Theo luật, ông cần quá bán trong số\N bảy vương quốc chấp nhận
Dialogue: 0,0:27:25.72,0:27:27.39,Default,,0,0,0,,để mở một cuộc tấn công.
Dialogue: 0,0:27:27.39,0:27:29.59,Default,,0,0,0,,Người của vương quốc bí ẩn và người Desert\N đã bị diệt vong từ lâu.
Dialogue: 0,0:27:29.59,0:27:31.66,Default,,0,0,0,,Người Trench thì giống như thú vật và man rợ.
Dialogue: 0,0:27:31.66,0:27:34.29,Default,,0,0,0,,Người Brine sẽ không tham gia và \Nvương quốc Tritonis là những kẻ hèn nhát.
Dialogue: 0,0:27:34.30,0:27:39.43,Default,,0,0,0,,Không có quân đội của tôi tới thuyết phục họ\Nkế hoạch của anh sẽ tiêu tan.
Dialogue: 0,0:27:39.43,0:27:41.80,Default,,0,0,0,,Nhưng tôi biết anh thực sự muốn gì.
Dialogue: 0,0:27:41.80,0:27:44.67,Default,,0,0,0,,Khi anh nhận được cam kết của 4 vương quốc,
Dialogue: 0,0:27:44.67,0:27:46.34,Default,,0,0,0,,Anh sẽ được coi là
Dialogue: 0,0:27:47.48,0:27:48.74,Default,,0,0,0,,Bá chủ của đại dương.
Dialogue: 0,0:27:50.21,0:27:52.21,Default,,0,0,0,,Đó chỉ là cái danh.
Dialogue: 0,0:27:52.21,0:27:54.21,Default,,0,0,0,,Tôi không ngốc nghếch như vậy đâu, Vua Orm.
Dialogue: 0,0:27:54.22,0:27:55.68,Default,,0,0,0,,Là Bá chủ Đại dương, anh sẽ là người chỉ huy
Dialogue: 0,0:27:55.68,0:27:58.48,Default,,0,0,0,,của quân đội vĩ đại nhất trên hành tinh này.
Dialogue: 0,0:27:58.49,0:28:00.49,Default,,0,0,0,,Tôi là sự lựa chọn tự nhiên để lãnh đạo nó.
Dialogue: 0,0:28:02.62,0:28:04.72,Default,,0,0,0,,Vậy...
Dialogue: 0,0:28:04.73,0:28:07.36,Default,,0,0,0,,còn tin đồn về một người khác thì sao?
Dialogue: 0,0:28:07.36,0:28:10.96,Default,,0,0,0,,Một người Atlantis đang sống trên bề mặt
Dialogue: 0,0:28:10.97,0:28:13.43,Default,,0,0,0,,Có mang dòng máu hoàng gia.
Dialogue: 0,0:28:13.43,0:28:17.34,Default,,0,0,0,,Anh có thể ngồi trên ngai vàng của Atlantis.
Dialogue: 0,0:28:17.34,0:28:19.47,Default,,0,0,0,,Nhưng không có tính chính danh.\NLàm thế nào anh có thể hy vọng thống nhất đế chế?
Dialogue: 0,0:28:19.47,0:28:22.81,Default,,0,0,0,,Tên khốn của mẹ tôi thậm chí chưa từng đến Atlantis.
Dialogue: 0,0:28:22.81,0:28:24.04,Default,,0,0,0,,Lòng trung thành của hắn là với...
Dialogue: 0,0:28:34.82,0:28:37.26,Default,,0,0,0,,Cư dân bề mặt! Di chuyển ra ngoài!
Dialogue: 0,0:28:58.04,0:28:59.31,Default,,0,0,0,,Nhà vua các ngươi bị thương!
Dialogue: 0,0:29:58.94,0:30:01.14,Default,,0,0,0,,Tôi không cố khơi mào cuộc chiến.
Dialogue: 0,0:30:01.88,0:30:03.61,Default,,0,0,0,,Cuộc chiến đã bắt đầu từ trước.
Dialogue: 0,0:30:03.61,0:30:07.21,Default,,0,0,0,,Vậy, đã đến lúc gửi cho họ một thông điệp\Nrằng chúng ta đang ở đây.
Dialogue: 0,0:30:11.55,0:30:12.92,Default,,0,0,0,,- Cha ổn mà.\N- Dừng lại đi
Dialogue: 0,0:30:12.92,0:30:14.72,Default,,0,0,0,,Đây là chìa khóa.\NCảm ơn cha.
Dialogue: 0,0:30:14.72,0:30:17.12,Default,,0,0,0,,Hãy về nhà và hoàn thiện hình xăm của con đi..
Dialogue: 0,0:30:17.12,0:30:18.62,Default,,0,0,0,,Nếu ông nội của con vẫn còn sống,
Dialogue: 0,0:30:18.63,0:30:20.69,Default,,0,0,0,,Ông ấy sẽ cốc đầu chúng ta khi chúng ta không uống hết.
Dialogue: 0,0:30:22.66,0:30:25.46,Default,,0,0,0,,Nghe lời con đi. Vẫn còn nhiều thời gian cho việc đó mà.
Dialogue: 0,0:30:25.47,0:30:28.17,Default,,0,0,0,,Cha phải đi ngủ.
Dialogue: 0,0:30:30.57,0:30:31.67,Default,,0,0,0,,Hiểu rồi.
Dialogue: 0,0:30:38.38,0:30:39.94,Default,,0,0,0,,Tôi đang tìm kiếm anh.
Dialogue: 0,0:30:39.95,0:30:41.28,Default,,0,0,0,,Ồ không, không...
Dialogue: 0,0:30:41.28,0:30:42.65,Default,,0,0,0,,Tôi cần anh đi cùng tôi đến Atlantis.
Dialogue: 0,0:30:42.65,0:30:44.48,Default,,0,0,0,,Nghe này, tôi sẽ nói với cô điều tương tự mà tôi
Dialogue: 0,0:30:44.48,0:30:46.48,Default,,0,0,0,,đã nói với lão già sao biển chết tiệt Vulko đó.
Dialogue: 0,0:30:46.49,0:30:48.52,Default,,0,0,0,,Rất vui được gặp, thưa quý cô.
Dialogue: 0,0:30:48.52,0:30:50.52,Default,,0,0,0,,Không, cảm ơn. Tôi không quan tâm.
Dialogue: 0,0:30:50.52,0:30:53.69,Default,,0,0,0,,Anh đã đánh bại Steppenwolf và cứu lấy Atlantis.
Dialogue: 0,0:30:53.69,0:30:55.86,Default,,0,0,0,,Điều đó không liên quan gì đến Atlantis.
Dialogue: 0,0:30:55.86,0:30:59.20,Default,,0,0,0,,Người anh em cùng mẹ khác cha của anh,\NVua Orm, sắp tuyên chiến
Dialogue: 0,0:30:59.20,0:31:01.13,Default,,0,0,0,,với thế giới mặt đất. Hàng tỷ người sẽ chết.
Dialogue: 0,0:31:01.14,0:31:02.57,Default,,0,0,0,,Ngay cả người của anh và của tôi.
Dialogue: 0,0:31:03.37,0:31:05.27,Default,,0,0,0,,Chúng ta phải ngăn anh ta lại.
Dialogue: 0,0:31:09.81,0:31:11.71,Default,,0,0,0,,Và làm thế nào để làm điều đó?
Dialogue: 0,0:31:11.71,0:31:15.18,Default,,0,0,0,,Anh là con trai đầu lòng của Nữ hoàng Atlanna.
Dialogue: 0,0:31:15.18,0:31:17.48,Default,,0,0,0,,Ngai vàng là của anh bởi anh là con trưởng.
Dialogue: 0,0:31:17.48,0:31:22.99,Default,,0,0,0,,Cách duy nhất để ngăn chặn cuộc chiến này\Nvà cứu cả hai thế giới
Dialogue: 0,0:31:22.99,0:31:26.56,Default,,0,0,0,,Chính là để anh đảm nhận vị trí\N xứng đáng với tư cách là vị Vua.
Dialogue: 0,0:31:29.26,0:31:32.40,Default,,0,0,0,,Cô nghĩ rằng nếu tôi chỉ xuất hiện,\N Orm sẽ từ bỏ ngai vàng?
Dialogue: 0,0:31:32.40,0:31:35.97,Default,,0,0,0,,Tôi là tên khốn khiến nữ hoàng của cô đã bị xử tử.
Dialogue: 0,0:31:35.97,0:31:38.60,Default,,0,0,0,,Hãy tin tôi. Tôi không phải là vua.
Dialogue: 0,0:31:38.60,0:31:39.47,Default,,0,0,0,,Tôi đồng ý.
Dialogue: 0,0:31:39.47,0:31:41.87,Default,,0,0,0,,Thật tuyệt vời. Đi đi
Dialogue: 0,0:31:41.87,0:31:44.84,Default,,0,0,0,,Nhưng, Vulko dường như tin vào anh vì một số lý do.
Dialogue: 0,0:31:44.84,0:31:46.48,Default,,0,0,0,,Và đó là lý do tôi ở đây.
Dialogue: 0,0:31:46.48,0:31:49.38,Default,,0,0,0,,Vulko đã biết về vị trí của\Ncây đinh ba bị mất của Atlantis.
Dialogue: 0,0:31:49.38,0:31:51.45,Default,,0,0,0,,Ôi, truyện cổ tích. Đó là một huyền thoại.
Dialogue: 0,0:31:51.45,0:31:52.98,Default,,0,0,0,,Không phải là một huyền thoại.
Dialogue: 0,0:31:52.99,0:31:55.09,Default,,0,0,0,,Với cây đinh ba thiêng liêng, mọi người sẽ lắng nghe anh.
Dialogue: 0,0:31:55.09,0:31:58.39,Default,,0,0,0,,Sau đó, anh sẽ lấy lại tính chính danh của mình và truất ngôi Orm.
Dialogue: 0,0:31:58.39,0:32:02.26,Default,,0,0,0,,Tính chính danh của tôi? Nó chết với mẹ tôi rồi.
Dialogue: 0,0:32:02.26,0:32:04.66,Default,,0,0,0,,Nhưng tôi sẽ hứa với cô điều này.\NNếu Orm tấn công,
Dialogue: 0,0:32:04.66,0:32:07.83,Default,,0,0,0,,tôi sẽ đối xử với hắn ta theo cách tương tự\Nngười của cô đã đối xử với mẹ tôi.
Dialogue: 0,0:32:09.17,0:32:10.60,Default,,0,0,0,,Một cách tàn nhẫn không thương tiếc.
Dialogue: 0,0:32:35.60,0:32:38.60,Default,,0,0,0,,Ở một nơi nào đó tại vùng phía Bắc Thái Bình Dương
Dialogue: 0,0:33:08.06,0:33:12.66,Default,,0,0,0,,{\i1}Tàu ngầm đó hầu như không hoạt động.\NNhưng nó đã hoàn thành mục đích của nó.{\i}
Dialogue: 0,0:33:12.67,0:33:16.43,Default,,0,0,0,,Một trong những người của anh đã can thiệp.
Dialogue: 0,0:33:16.43,0:33:18.50,Default,,0,0,0,,{\i1}Hắn không phải là một trong số chúng tôi.{\i}
Dialogue: 0,0:33:18.50,0:33:21.41,Default,,0,0,0,,Hắn đã giết cha tôi.
Dialogue: 0,0:33:22.18,0:33:24.64,Default,,0,0,0,,{\i1}Giao kèo giữa chúng ta đã kết thúc.{\i}
Dialogue: 0,0:33:28.31,0:33:31.75,Default,,0,0,0,,- Cầm lấy số tiền này.\N- Tôi muốn Aquaman.
Dialogue: 0,0:34:42.72,0:34:43.92,Default,,0,0,0,,Cha!
Dialogue: 0,0:34:49.19,0:34:50.39,Default,,0,0,0,,Cha!
Dialogue: 0,0:35:17.86,0:35:18.86,Default,,0,0,0,,Cha!
Dialogue: 0,0:35:23.43,0:35:24.66,Default,,0,0,0,,Nào!
Dialogue: 0,0:35:26.20,0:35:27.66,Default,,0,0,0,,Nào!
Dialogue: 0,0:35:30.40,0:35:32.74,Default,,0,0,0,,Nào, thở đi!
Dialogue: 0,0:35:33.54,0:35:36.27,Default,,0,0,0,,Thôi nào! Ông ấy không còn thở.
Dialogue: 0,0:35:36.27,0:35:37.57,Default,,0,0,0,,Nhích ra! Hãy để tôi.
Dialogue: 0,0:35:45.12,0:35:46.48,Default,,0,0,0,,Nào, cố lên!
Dialogue: 0,0:35:58.03,0:35:59.03,Default,,0,0,0,,Cha!
Dialogue: 0,0:36:13.51,0:36:15.81,Default,,0,0,0,,Cô biết đấy, tôi thậm chí còn không biết tên của cô.
Dialogue: 0,0:36:15.81,0:36:18.58,Default,,0,0,0,,Tôi là Công chúa xứ Xebell, Mera Challa
Dialogue: 0,0:36:20.28,0:36:21.58,Default,,0,0,0,,Anh có thể gọi tôi là Mera.
Dialogue: 0,0:36:23.35,0:36:24.89,Default,,0,0,0,,Cảm ơn cô, Mera.
Dialogue: 0,0:36:46.04,0:36:47.51,Default,,0,0,0,,Lạy Chúa.
Dialogue: 0,0:36:48.21,0:36:49.85,Default,,0,0,0,,Đây là do Orm đang gây ra.
Dialogue: 0,0:36:51.65,0:36:53.55,Default,,0,0,0,,Điều tồi tệ nhất vẫn chưa xảy ra.
Dialogue: 0,0:37:01.16,0:37:02.99,Default,,0,0,0,,{\i1}Có cơn đại hồng thủy\NCác tòa nhà đã bị phá hủy{\i}
Dialogue: 0,0:37:02.99,0:37:05.69,Default,,0,0,0,,{\i1}và cảnh báo lũ quét sẽ xảy ra\Ntrong vài giờ tới..{\i}
Dialogue: 0,0:37:05.70,0:37:08.30,Default,,0,0,0,,{\i1}Cảnh tượng chưa từng thấy khắp nơi \Ntrên thế giới trong ngày hôm nay{\i}
Dialogue: 0,0:37:08.30,0:37:11.97,Default,,0,0,0,,{\i1}Chúng ta đang chứng kiến chiến hạm và rác thải\Nđã bị sóng lớn đánh vào bờ.{\i}
Dialogue: 0,0:37:11.97,0:37:15.30,Default,,0,0,0,,{\i1}Đây là những cảnh\Nrác thải ở ven bờ biển{\i}
Dialogue: 0,0:37:15.31,0:37:17.81,Default,,0,0,0,,{\i1}và đó là những thứ rác\Nđến từ nhiều nơi trên thế giới.{\i}
Dialogue: 0,0:37:17.81,0:37:21.08,Default,,0,0,0,,{\i1}Rác từ hàng thập kỷ qua bị vứt xuống biển\Nđã được ném trở lại đất liền.{\i}
Dialogue: 0,0:37:21.08,0:37:22.91,Default,,0,0,0,,{\i1}Nhiều câu hỏi được đặt ra,{\i}
Dialogue: 0,0:37:22.91,0:37:24.28,Default,,0,0,0,,{\i1}"Đây có phải là thảm họa tự nhiên\Nhoặc thứ gì khác hay không?"{\i}
Dialogue: 0,0:37:24.28,0:37:26.88,Default,,0,0,0,,{\i1}Không. Đây không phải là thảm họa tự nhiên.{\i}
Dialogue: 0,0:37:26.88,0:37:29.22,Default,,0,0,0,,{\i1}Họ đã ném rác và chiến hạm của chúng ta.{\i}
Dialogue: 0,0:37:29.22,0:37:31.62,Default,,0,0,0,,{\i1}Đây là cuộc chạm trán đầu tiên\Ncủa chúng ta với người Atlantis.{\i}
Dialogue: 0,0:37:31.62,0:37:34.92,Default,,0,0,0,,{\i1}Lại nữa, giả thuyết Atlantis điên rồ của anh.{\i}
Dialogue: 0,0:37:34.93,0:37:36.82,Default,,0,0,0,,{\i1}Anh hoàn toàn không có bằng chứng nào về điều này. Tiến sĩ Shin.{\i}
Dialogue: 0,0:37:36.83,0:37:38.26,Default,,0,0,0,,{\i1}Chống con mắt ra mà xem.{\i}
Dialogue: 0,0:37:38.26,0:37:39.83,Default,,0,0,0,,{\i1}Chúng ta đã có một người Atlantis sống giữa chúng ta{\i}
Dialogue: 0,0:37:39.83,0:37:41.56,Default,,0,0,0,,{\i1}và tên của anh ấy là Aquaman.{\i}
Dialogue: 0,0:37:49.14,0:37:50.87,Default,,0,0,0,,Đây là đâu?
Dialogue: 0,0:37:50.87,0:37:55.08,Default,,0,0,0,,Đây là vị trí mà Vulko đã dạy tôi bài học đầu tiên.
Dialogue: 0,0:37:55.08,0:37:58.41,Default,,0,0,0,,Cháu phải quên tất cả những lời dạy của thế giới mặt đất.
Dialogue: 0,0:37:58.42,0:38:03.18,Default,,0,0,0,,Đi sâu hơn để khám phá bản năng Atlantis của cháu.
Dialogue: 0,0:38:03.19,0:38:05.45,Default,,0,0,0,,Nhưng cháu đã biết bơi.
Dialogue: 0,0:38:05.46,0:38:07.19,Default,,0,0,0,,Chưa là gì cả.
Dialogue: 0,0:38:10.46,0:38:12.69,Default,,0,0,0,,Vì vậy, chúng ta đã rõ.
Dialogue: 0,0:38:12.70,0:38:14.73,Default,,0,0,0,,Tôi sẽ giúp cô dừng cuộc chiến này,
Dialogue: 0,0:38:15.33,0:38:16.97,Default,,0,0,0,, thế là xong.
Dialogue: 0,0:38:17.83,0:38:19.77,Default,,0,0,0,,Có lẽ đó là điều tốt nhất.
Dialogue: 0,0:38:40.09,0:38:42.39,Default,,0,0,0,,Đại dương quý giá hơn cả cuộc sống của chúng ta, Arthur.
Dialogue: 0,0:38:42.39,0:38:43.79,Default,,0,0,0,,Nó cho ta biết ta là ai.
Dialogue: 0,0:38:43.79,0:38:45.89,Default,,0,0,0,,Đợi đã. Chú có thể nói chuyện dưới nước?
Dialogue: 0,0:38:45.89,0:38:49.40,Default,,0,0,0,,Này, cháu cũng có thể nói chuyện dưới nước!
Dialogue: 0,0:38:49.40,0:38:51.66,Default,,0,0,0,,Điều này thật tuyệt vời!
Dialogue: 0,0:38:51.67,0:38:54.64,Default,,0,0,0,,Chúng ta có thể làm được nhiều điều hơn là chỉ nói chuyện.
Dialogue: 0,0:39:00.64,0:39:05.78,Default,,0,0,0,,Trở thành người Atlantis có nghĩa là\Nkhông chỉ có thể thở trong nước.
Dialogue: 0,0:39:05.78,0:39:10.78,Default,,0,0,0,,Cơ thể ta được trang bị để chịu đựng cái lạnh và áp lực cực độ.
Dialogue: 0,0:39:10.79,0:39:13.32,Default,,0,0,0,,Và đôi mắt của cháu tự điều chỉnh để nhìn trong bóng tối.
Dialogue: 0,0:39:37.28,0:39:38.68,Default,,0,0,0,,Điều này thật tuyệt vời!
Dialogue: 0,0:40:06.31,0:40:09.88,Default,,0,0,0,,Chú Vulko, khi nào cháu có thể gặp mẹ?
Dialogue: 0,0:40:12.01,0:40:14.35,Default,,0,0,0,,Chẳng bao lâu nữa, hoàng tử trẻ của tôi.
Dialogue: 0,0:40:14.35,0:40:18.75,Default,,0,0,0,,Khi đã sẵn sàng, ta sẽ đưa cháu\Nđến Atlantis để gặp Nữ hoàng.
Dialogue: 0,0:40:29.03,0:40:30.90,Default,,0,0,0,,Tôi đã giấu tàu của mình ở đây.
Dialogue: 0,0:40:32.57,0:40:34.40,Default,,0,0,0,,Nghe đây. Tôi không ngồi vào đó.
Dialogue: 0,0:40:34.40,0:40:36.34,Default,,0,0,0,,Để đến nơi chúng ta cần đến, annh sẽ phải làm vậy.
Dialogue: 0,0:40:36.34,0:40:38.50,Default,,0,0,0,,Tàu cá của cô đã được ướp trong rác.
Dialogue: 0,0:40:38.51,0:40:40.51,Default,,0,0,0,,Tôi có thể bị nhiễm mùi thối của nó.
Dialogue: 0,0:40:40.51,0:40:42.24,Default,,0,0,0,,Còn đỡ hơn anh đấy.
Dialogue: 0,0:40:48.15,0:40:49.82,Default,,0,0,0,,Ừ nhỉ, mình hôi nách quá!
Dialogue: 0,0:41:44.58,0:41:47.58,Default,,0,0,0,,Vương quốc Atlantis
Dialogue: 0,0:41:50.58,0:41:52.68,Default,,0,0,0,,Sao lại phải xây cái cầu ở dưới nước làm gì?
Dialogue: 0,0:41:52.68,0:41:55.08,Default,,0,0,0,,Nó là di tích còn sót từ thời cổ đại.
Dialogue: 0,0:41:55.08,0:41:57.08,Default,,0,0,0,,Đó là lối ra vào duy nhất\Ncủa thủ đô.
Dialogue: 0,0:41:57.09,0:41:58.62,Default,,0,0,0,,Nếu có ai đó lẻn vào từ bên trên thì sao?
Dialogue: 0,0:41:58.62,0:42:00.25,Default,,0,0,0,,An ninh ở đây rất nghiêm ngặt.
Dialogue: 0,0:42:00.26,0:42:01.99,Default,,0,0,0,,Cho dù có muốn vào, thì chúng sẽ gặp\Nphải hệ thống canh gác
Dialogue: 0,0:42:01.99,0:42:03.92,Default,,0,0,0,,hoặc sẽ bị nướng chín bởi khẩu súng Hydro.
Dialogue: 0,0:42:06.06,0:42:07.96,Default,,0,0,0,,Luôn có người cố lẻn vào mà.
Dialogue: 0,0:42:09.43,0:42:10.90,Default,,0,0,0,,Ờ, cũng có thể.
Dialogue: 0,0:42:30.88,0:42:32.15,Default,,0,0,0,,Gì nữa đây?
Dialogue: 0,0:42:32.15,0:42:34.59,Default,,0,0,0,,Trạm kiểm soát biên giới.
Dialogue: 0,0:42:34.59,0:42:37.59,Default,,0,0,0,,Đừng lo\NTôi có đặc quyền ra vào ở đây.
Dialogue: 0,0:42:45.47,0:42:46.47,Default,,0,0,0,,Chào mừng anh về nhà.
Dialogue: 0,0:43:35.12,0:43:37.38,Default,,0,0,0,,Có một ngôi nhà an toàn ở thành phố cổ.
Dialogue: 0,0:43:37.38,0:43:40.65,Default,,0,0,0,,Mà quý tộc không bao giờ mạo hiểm xuống đáy biển.
Dialogue: 0,0:43:40.65,0:43:41.66,Default,,0,0,0,,Chúng ta đã đến nơi.
Dialogue: 0,0:44:10.92,0:44:12.58,Default,,0,0,0,,Đây là gì?
Dialogue: 0,0:44:12.59,0:44:14.92,Default,,0,0,0,,Đây là những túi khí chúng tôi sử dụng\Nđể chứa không khí tự nhiên.
Dialogue: 0,0:44:14.92,0:44:17.49,Default,,0,0,0,,Chỉ những người cấp cao mới có thể thở với nó.
Dialogue: 0,0:44:17.49,0:44:19.33,Default,,0,0,0,,Ngoài ra, nó giúp động vật tránh xa nơi này.
Dialogue: 0,0:44:20.13,0:44:22.20,Default,,0,0,0,,Chúng có hơi lộn xộn.
Dialogue: 0,0:44:24.80,0:44:26.70,Default,,0,0,0,,Vulko. Cháu đây, ông già.
Dialogue: 0,0:44:26.70,0:44:29.74,Default,,0,0,0,,Sau tất cả, cuối cùng chú cũng có được thứ mình muốn.
Dialogue: 0,0:44:32.37,0:44:36.21,Default,,0,0,0,,Arthur, chú ước chúng ta ở trong hoàn cảnh tốt hơn.
Dialogue: 0,0:44:36.21,0:44:38.54,Default,,0,0,0,,Cháu không thể tin Orm đã tấn công.
Dialogue: 0,0:44:38.54,0:44:39.48,Default,,0,0,0,,Đó không phải là một cuộc tấn công.
Dialogue: 0,0:44:39.48,0:44:41.81,Default,,0,0,0,,Về mặt pháp lý, hắn chưa được phép.
Dialogue: 0,0:44:41.82,0:44:45.25,Default,,0,0,0,,Hắn đã làm với chiến hạm của \Nngười mặt đất chỉ là một cảnh báo.
Dialogue: 0,0:44:45.25,0:44:46.79,Default,,0,0,0,,Điều gì đã kích động anh ta?
Dialogue: 0,0:44:46.79,0:44:50.42,Default,,0,0,0,,Vua Orm và cha của cháu bị phục kích bởi người mặt đất.
Dialogue: 0,0:44:50.42,0:44:53.06,Default,,0,0,0,,Là người mặt đất? Cháu không nghĩ vậy.
Dialogue: 0,0:44:53.06,0:44:54.99,Default,,0,0,0,,Vì người mặt đất vẫn còn tưởng Atlantis là truyện cổ tích.
Dialogue: 0,0:44:54.99,0:44:56.69,Default,,0,0,0,,Ta đã ở đó.
Dialogue: 0,0:44:56.70,0:44:58.56,Default,,0,0,0,,Một chiến hạm  người mặt đất bắn vào chúng ta.
Dialogue: 0,0:44:58.57,0:45:01.30,Default,,0,0,0,,Bây giờ, Xebel đang bên cạnh Atlantis.
Dialogue: 0,0:45:01.30,0:45:06.20,Default,,0,0,0,,Đưa cho Orm một hạm đội đủ lớn để buộc\Nhai vương quốc khác tham gia cùng hắn ta.
Dialogue: 0,0:45:06.21,0:45:10.78,Default,,0,0,0,,Nếu chúng ta muốn ngăn cuộc chiến này,\Ncháu phải truất phế hắn ngay.
Dialogue: 0,0:45:10.78,0:45:14.04,Default,,0,0,0,,Cháu phải nói với chú bao nhiêu lần nữa?\NCháu không muốn trở thành vua.
Dialogue: 0,0:45:14.05,0:45:15.81,Default,,0,0,0,,Anh không hiểu chuyện rồi.
Dialogue: 0,0:45:15.82,0:45:18.32,Default,,0,0,0,,Khi anh ta trở thành Bá chủ Đại dương thực sự,\Nthì mọi sự sẽ hỏng bét.
Dialogue: 0,0:45:18.32,0:45:22.05,Default,,0,0,0,,Sức mạnh theo ý của anh ta sẽ không giống\Nvới bất cứ thứ gì anh từng thấy.
Dialogue: 0,0:45:22.05,0:45:24.72,Default,,0,0,0,,Tôi đến từ bề mặt đất. Không ai nghiêm túc với tôi cả.
Dialogue: 0,0:45:24.73,0:45:26.29,Default,,0,0,0,,Tôi thậm chí không biết bắt đầu từ đâu, hiểu chưa?
Dialogue: 0,0:45:26.29,0:45:28.76,Default,,0,0,0,,Bằng cách chiến thắng trái tim và khối óc của mọi người\N để chứng minh với họ rằng cháu xứng đáng.
Dialogue: 0,0:45:28.76,0:45:33.50,Default,,0,0,0,,Hãy truy tìm cái này.
Dialogue: 0,0:45:40.21,0:45:41.81,Default,,0,0,0,,Cháu đã có một cái giống thế này, là nó.
Dialogue: 0,0:45:43.04,0:45:45.31,Default,,0,0,0,,Cái của cháu là đồ Tàu nhái lại thôi.
Dialogue: 0,0:45:45.31,0:45:47.98,Default,,0,0,0,,Đây là Cây đinh ba "xịn" bị mất của Atlantis.
Dialogue: 0,0:45:47.98,0:45:49.21,Default,,0,0,0,,Ồ, cháu biết câu chuyện này.
Dialogue: 0,0:45:49.22,0:45:52.72,Default,,0,0,0,,Nó còn hơn là một huyền thoại. Đó là sự thật.
Dialogue: 0,0:45:52.72,0:45:57.76,Default,,0,0,0,,{\i1}Nó được rèn bởi những bậc thầy\Nvũ khí vĩ đại nhất trong lịch sử.{\i}
Dialogue: 0,0:45:57.76,0:46:01.53,Default,,0,0,0,,{\i1}Được chế tác từ thép của Poseidon.\Ndành cho vua Atlan,{\i}
Dialogue: 0,0:46:02.33,0:46:05.53,Default,,0,0,0,,{\i1}người trị vì đầu tiên của đế chế Atlantis.{\i}
Dialogue: 0,0:46:05.53,0:46:09.77,Default,,0,0,0,,{\i1}Truyền thuyết nói, Cây đinh ba được chế tạo với sức mạnh\Nđể chế ngự biển cả.{\i}
Dialogue: 0,0:46:11.14,0:46:12.97,Default,,0,0,0,,Vậy chuyện gì đã xảy ra với nó?
Dialogue: 0,0:46:12.97,0:46:16.57,Default,,0,0,0,,Để biết điều đó, hãy quay trở lại thời điểm\Ntrước thời kỳ Đại Sụp Đổ .
Dialogue: 0,0:46:16.58,0:46:20.81,Default,,0,0,0,,{\i1}Khi vua Atlan cai trị tất cả,{\i}
Dialogue: 0,0:46:20.81,0:46:23.58,Default,,0,0,0,,{\i1}Khi đó, Vương quốc Atlantis là một thể thống nhất.{\i}
Dialogue: 0,0:46:26.02,0:46:32.52,Default,,0,0,0,,{\i1}Đó là thời kỳ thịnh vượng và tiến bộ công nghệ.{\i}
Dialogue: 0,0:46:32.53,0:46:35.16,Default,,0,0,0,,{\i1}Chúng ta đã mở khóa bí mật về năng lượng không giới hạn{\i}
Dialogue: 0,0:46:35.16,0:46:36.89,Default,,0,0,0,,{\i1}Ở thời điểm mà phần còn lại của thế giới{\i}
Dialogue: 0,0:46:36.90,0:46:38.63,Default,,0,0,0,,{\i1}vẫn nghĩ rằng trái đất phẳng.{\i}
Dialogue: 0,0:46:41.10,0:46:44.30,Default,,0,0,0,,{\i1}Nhưng chúng ta đã trở nên quá tham vọng,{\i}
Dialogue: 0,0:46:44.30,0:46:46.97,Default,,0,0,0,,{\i1}quá thèm khát năng lượng.{\i}
Dialogue: 0,0:47:03.19,0:47:06.69,Default,,0,0,0,,{\i1}Đại dương nuốt chửng chúng ta và nhấn chìm Atlantis.{\i}
Dialogue: 0,0:47:08.26,0:47:11.20,Default,,0,0,0,,{\i1}Nhưng chính sức mạnh đã đánh sập\Nnền văn minh của chúng ta{\i}
Dialogue: 0,0:47:11.20,0:47:14.50,Default,,0,0,0,,{\i1}cũng đã mở đường cho tương lai của chúng ta.{\i}
Dialogue: 0,0:47:14.50,0:47:17.70,Default,,0,0,0,,{\i1}Nó cho chúng ta khả năng thở dưới nước.{\i}
Dialogue: 0,0:47:18.40,0:47:21.41,Default,,0,0,0,,{\i1}Và vì vậy, chúng ta đã tiến hóa.{\i}
Dialogue: 0,0:47:21.41,0:47:24.58,Default,,0,0,0,,{\i1}Một số người khác thì thoái hóa hay trở nên man rợ..{\i}
Dialogue: 0,0:47:26.38,0:47:31.72,Default,,0,0,0,,Và nhà vua đã sống hết phần còn lại của mình\Ntrong thời gian lưu vong tự nguyện.
Dialogue: 0,0:47:31.72,0:47:36.09,Default,,0,0,0,,{\i1}Cả ông ấy và cây đinh ba đều không còn được nhìn thấy nữa.{\i}
Dialogue: 0,0:47:36.09,0:47:40.36,Default,,0,0,0,,Một trong nhóm khảo cổ học của chúng ta\Nđã phát hiện ra điều này một vài tháng trước.
Dialogue: 0,0:47:40.36,0:47:44.23,Default,,0,0,0,,Đây là bản ghi âm cổ có từ triều đại đầu tiên.
Dialogue: 0,0:47:44.23,0:47:47.97,Default,,0,0,0,,Ta tin rằng nó chứa thông điệp cuối cùng\Ncủa Atlan cho mọi thần dân.
Dialogue: 0,0:47:47.97,0:47:51.04,Default,,0,0,0,,và địa điểm của Cây đinh ba thiêng liêng.
Dialogue: 0,0:47:51.04,0:47:52.47,Default,,0,0,0,,Vậy nó nói gì?
Dialogue: 0,0:47:52.47,0:47:55.24,Default,,0,0,0,,Chúng ta không biết.\NCông nghệ quá cũ.
Dialogue: 0,0:47:55.24,0:47:59.01,Default,,0,0,0,,Vật thể lăng trụ đứng này mang dấu ấn của Vương quốc Desert.
Dialogue: 0,0:47:59.01,0:48:01.81,Default,,0,0,0,,Cháu phải mang nó đến đó để lấy thông điệp bên trong nó.
Dialogue: 0,0:48:01.81,0:48:05.78,Default,,0,0,0,,Manh mối về nơi an nghỉ cuối cùng của vua Atlan nằm trong này.
Dialogue: 0,0:48:23.03,0:48:26.20,Default,,0,0,0,,{\i1}Lực lượng phòng về biên giới thông báo\NNgươi đã bị bắt vì xâm nhập bất hợp pháp.{\i}
Dialogue: 0,0:48:26.20,0:48:27.41,Default,,0,0,0,,Không.
Dialogue: 0,0:48:29.11,0:48:30.94,Default,,0,0,0,,Cháu không nên để họ nhìn thấy cháu ở đây.
Dialogue: 0,0:49:26.83,0:49:28.80,Default,,0,0,0,,Ta không cho rằng người muốn nói chuyện, phải không?
Dialogue: 0,0:49:33.97,0:49:35.47,Default,,0,0,0,,Ta cũng vậy.
Dialogue: 0,0:50:14.88,0:50:19.88,Default,,0,0,0,,{\i1}Nhân danh vua Orm, ta bắt giữ ngươi.{\i}
Dialogue: 0,0:50:40.44,0:50:42.11,Default,,0,0,0,,Chào mừng đã đến với Atlantis
Dialogue: 0,0:50:43.31,0:50:44.51,Default,,0,0,0,,Anh Hai !
Dialogue: 0,0:50:48.78,0:50:51.51,Default,,0,0,0,,Tôi không thể tin rằng cuối cùng anh cũng ở đây.
Dialogue: 0,0:50:52.92,0:50:55.75,Default,,0,0,0,,Tôi đã nghe rất nhiều câu chuyện về anh.
Dialogue: 0,0:51:01.53,0:51:04.73,Default,,0,0,0,,Trong suốt những năm qua, tôi luôn xấu hổ về
Dialogue: 0,0:51:04.73,0:51:08.87,Default,,0,0,0,,người anh em trên mặt đất của mẹ tôi.
Dialogue: 0,0:51:08.87,0:51:11.93,Default,,0,0,0,,Xấu hổ về việc tôi có một người anh em cùng mẹ khác kha,
Dialogue: 0,0:51:11.94,0:51:15.87,Default,,0,0,0,,tôi chỉ muốn đâm chiếc đinh ba xuyên qua người anh.
Dialogue: 0,0:51:15.87,0:51:20.28,Default,,0,0,0,,Nhưng bây giờ, cuối cùng anh cũng ở đây trước mặt tôi.\NTôi phải thừa nhận rằng...
Dialogue: 0,0:51:28.09,0:51:29.58,Default,,0,0,0,,Tôi bị mâu thuẫn chính mình.
Dialogue: 0,0:51:29.59,0:51:31.89,Default,,0,0,0,,Ngươi muốn tương tàn?
Dialogue: 0,0:51:31.89,0:51:34.56,Default,,0,0,0,,Tại sao không tháo những dây xích này khỏi ta xem, thằng em?
Dialogue: 0,0:51:34.56,0:51:36.49,Default,,0,0,0,,Và để xem ta có thể làm gì.
Dialogue: 0,0:51:38.86,0:51:43.87,Default,,0,0,0,,Phải rồi. Tôi thấy anh đã mang vũ khí của mẹ chúng ta.
Dialogue: 0,0:51:43.87,0:51:47.57,Default,,0,0,0,,Có phải đó là lý do anh đến đây sau tất cả mọi chuyện?
Dialogue: 0,0:51:47.57,0:51:49.17,Default,,0,0,0,,Để giết tôi?
Dialogue: 0,0:51:49.17,0:51:51.97,Default,,0,0,0,,Ta đến đây để ngăn chặn một kẻ điên phá hủy thế giới của ta.
Dialogue: 0,0:51:51.98,0:51:53.91,Default,,0,0,0,,Tôi hiểu rồi.
Dialogue: 0,0:51:53.91,0:51:55.74,Default,,0,0,0,,Và làm thế nào để anh có kế hoạch ngăn chặn
Dialogue: 0,0:51:55.75,0:51:59.68,Default,,0,0,0,,sự tàn bạo của người mặt đất?
Dialogue: 0,0:51:59.68,0:52:04.45,Default,,0,0,0,,Bởi vì nhiều thế kỷ qua họ đã làm\Nô nhiễm nguồn nước
Dialogue: 0,0:52:05.42,0:52:07.79,Default,,0,0,0,,và đã đầu độc con cháu của chúng ta.
Dialogue: 0,0:52:07.79,0:52:11.36,Default,,0,0,0,,Bây giờ chúng lại còn đốt cháy đại dương của chúng ta
Dialogue: 0,0:52:13.46,0:52:17.07,Default,,0,0,0,,Anh đến đây để chống lại chính người của mình sao?
Dialogue: 0,0:52:17.07,0:52:19.23,Default,,0,0,0,,Ta không muốn điều này.
Dialogue: 0,0:52:19.24,0:52:21.97,Default,,0,0,0,,Rõ ràng anh đã chọn bọn họ và\Nđến đây để thách thức ngai vàng
Dialogue: 0,0:52:21.97,0:52:24.01,Default,,0,0,0,,Đó là để ngăn chặn cuộc chiến của ngươi thôi.
Dialogue: 0,0:52:26.08,0:52:28.94,Default,,0,0,0,,Anh đang muốn chiến đấu để giành lấy ngôi đế vương của tôi sao?
Dialogue: 0,0:52:28.95,0:52:33.25,Default,,0,0,0,,Gọi nó là gì thì tùy ngươi.\NNhưng ta gọi nó là một cú đá vào mông.
Dialogue: 0,0:52:33.25,0:52:35.78,Default,,0,0,0,,Vậy thì có lẽ đó là cách chúng ta sẽ tiến hành
Dialogue: 0,0:52:35.79,0:52:37.55,Default,,0,0,0,,- Xin hãy giữ sự uy nghiêm của bệ hạ!\N- Orm, xin anh đấy.
Dialogue: 0,0:52:37.55,0:52:38.95,Default,,0,0,0,,Ngươi không thấy sao?
Dialogue: 0,0:52:38.96,0:52:40.99,Default,,0,0,0,,Nếu tôi đánh bại đứa con cả của Atlanna trước mặt mọi người.
Dialogue: 0,0:52:40.99,0:52:43.46,Default,,0,0,0,,Thì 7 vương quốc sẽ phải thừa nhận rằng
Dialogue: 0,0:52:43.46,0:52:48.56,Default,,0,0,0,,Ta là một vị vua thực sự.
Dialogue: 0,0:52:48.56,0:52:52.57,Default,,0,0,0,,Bệ hạ của tôi, không có chiến thắng vinh quang nào\Ntrong việc đánh bại kẻ ngu dốt.
Dialogue: 0,0:52:52.57,0:52:54.17,Default,,0,0,0,,Anh trai của anh rõ ràng là một kẻ ngu dốt.
Dialogue: 0,0:52:55.84,0:52:58.11,Default,,0,0,0,,Thưa bệ hạ. Hắn không biết phép tắc của chúng ta.
Dialogue: 0,0:52:58.11,0:53:00.24,Default,,0,0,0,,Vậy hãy để ta cho anh ta một bài học.
Dialogue: 0,0:53:01.78,0:53:03.75,Default,,0,0,0,,Anh có chính thức thách thức tôi không?
Dialogue: 0,0:53:03.75,0:53:05.95,Default,,0,0,0,,Đúng vậy, ta sẽ thách thức ngươi.\NVà khi ta thắng...
Dialogue: 0,0:53:05.95,0:53:07.48,Default,,0,0,0,, nếu anh thắng,
Dialogue: 0,0:53:07.48,0:53:11.19,Default,,0,0,0,,Tôi sẽ ngừng mọi hành động ngay lập tức.\NChiến tranh sẽ kết thúc.
Dialogue: 0,0:53:12.06,0:53:13.66,Default,,0,0,0,,Nhưng nếu tôi thắng?
Dialogue: 0,0:53:15.39,0:53:16.59,Default,,0,0,0,,Anh sẽ chết.
Dialogue: 0,0:53:17.36,0:53:18.89,Default,,0,0,0,,Tốt thôi.
Dialogue: 0,0:53:18.89,0:53:20.53,Default,,0,0,0,,Cứ thế mà làm
Dialogue: 0,0:53:20.53,0:53:23.56,Default,,0,0,0,,Lời khiêu chiến đã được nói ra và tôi chấp nhận.
Dialogue: 0,0:53:23.57,0:53:26.03,Default,,0,0,0,,Chuẩn bị cho anh ta đến vòng lửa!
Dialogue: 0,0:53:26.04,0:53:29.24,Default,,0,0,0,,Đợi đã. Cái gì?
Dialogue: 0,0:53:47.36,0:53:51.23,Default,,0,0,0,,Làm thế nào mà cháu ngốc đến mức để\NOrm lừa cháu vào một cuộc chiến như vậy?
Dialogue: 0,0:53:51.23,0:53:53.93,Default,,0,0,0,,Điều đó sẽ giải quyết tất cả các vấn đề của chúng ta.
Dialogue: 0,0:53:53.93,0:53:56.23,Default,,0,0,0,,Cháu sẽ đánh bại hắn ta trong trận chiến.\NXong việc. Cháu sẽ về nhà.
Dialogue: 0,0:53:56.23,0:53:58.47,Default,,0,0,0,,Cháu là một chiến binh đáng gờm trên đất liền.
Dialogue: 0,0:53:58.47,0:54:01.00,Default,,0,0,0,,Nhưng ở đây, cháu không có được lợi thế.
Dialogue: 0,0:54:01.00,0:54:03.80,Default,,0,0,0,,Orm đã dành cả cuộc đời ở dưới nước.
Dialogue: 0,0:54:03.81,0:54:07.51,Default,,0,0,0,,Tập luyện để trở thành một chiến binh\Nvà trở thành người giỏi nhất.
Dialogue: 0,0:54:07.51,0:54:09.68,Default,,0,0,0,,Chú đã dạy cháu cách chiến đấu.
Dialogue: 0,0:54:09.68,0:54:11.81,Default,,0,0,0,,Để xem cháu có nhớ gì không
Dialogue: 0,0:54:15.55,0:54:16.92,Default,,0,0,0,,Cái nĩa to tổ chảng này là gì thế?
Dialogue: 0,0:54:16.92,0:54:19.69,Default,,0,0,0,,Đó là Cây đinh ba.\NHãy phòng thủ đi !
Dialogue: 0,0:54:53.25,0:54:54.75,Default,,0,0,0,,Cái quái gì thế này?
Dialogue: 0,0:54:54.76,0:54:57.06,Default,,0,0,0,,Ta sẽ dạy cháu chiêu làm chủ Cây đinh ba.
Dialogue: 0,0:55:07.24,0:55:08.74,Default,,0,0,0,,Thật không công bằng.
Dialogue: 0,0:55:08.74,0:55:10.80,Default,,0,0,0,,Cái này quá to. Tại sao cháu không được sử một thanh kiếm?
Dialogue: 0,0:55:10.81,0:55:14.58,Default,,0,0,0,,Đó là Cây đinh ba của mẹ cháu.\NMột vũ khí truyền thống của hoàng gia.
Dialogue: 0,0:55:14.58,0:55:18.18,Default,,0,0,0,,Cháu sẽ không bao giờ trở thành Vua\Ncho đến khi cháu sử thành thạo nó.
Dialogue: 0,0:55:18.18,0:55:19.85,Default,,0,0,0,,Tại sao bà ấy không bao giờ đến gặp cháu?
Dialogue: 0,0:55:19.85,0:55:21.98,Default,,0,0,0,,Ta đã nói rồi, khi nào cháu sẵn sàng...
Dialogue: 0,0:55:21.98,0:55:26.09,Default,,0,0,0,,Chú nín đi, chú nói dối cháu!\NCháu đã làm mọi thứ chú yêu cầu.
Dialogue: 0,0:55:26.09,0:55:28.92,Default,,0,0,0,,Và mỗi khi cháu vượt qua bài kiểm tra,\Nchú lại bắt tập một bài khác.
Dialogue: 0,0:55:28.92,0:55:30.36,Default,,0,0,0,,Vậy biết đến khi nào cháu mới giỏi?
Dialogue: 0,0:55:35.26,0:55:37.13,Default,,0,0,0,,Bà ấy không thương cháu à?
Dialogue: 0,0:55:37.13,0:55:40.37,Default,,0,0,0,,Mẹ cháu yêu cháu hơn tất cả mọi thứ trên thế giới này.
Dialogue: 0,0:55:40.37,0:55:44.20,Default,,0,0,0,,Nhưng bà ấy phải quay lại Atlantis để giữ an toàn cho cháu.
Dialogue: 0,0:55:44.21,0:55:47.57,Default,,0,0,0,,Cuộc hôn nhân của bả với vua Orvax đã được sắp đặt từ lâu
Dialogue: 0,0:55:47.58,0:55:49.28,Default,,0,0,0,,vì vậy bả phải cưới ông ta và sinh ra hắn ta
Dialogue: 0,0:55:49.28,0:55:52.75,Default,,0,0,0,,là Hoàng tử Orm.
Dialogue: 0,0:55:52.75,0:55:55.08,Default,,0,0,0,,Nhưng cuối cùng Orvax đã tìm ra cháu,
Dialogue: 0,0:55:55.08,0:55:57.65,Default,,0,0,0,,Ông ta nổi lòng ghen tị
Dialogue: 0,0:55:57.65,0:56:02.32,Default,,0,0,0,,và tế sống bà ấy cho bọn người Trench
Dialogue: 0,0:56:05.06,0:56:07.66,Default,,0,0,0,,Chú nói rằng họ đã xử tử bà ấy
Dialogue: 0,0:56:08.86,0:56:10.17,Default,,0,0,0,,vì bả đẻ ra cháu?
Dialogue: 0,0:56:24.81,0:56:26.65,Default,,0,0,0,,Cháu luôn nhớ...
Dialogue: 0,0:56:26.65,0:56:27.95,Default,,0,0,0,,mọi thứ.
Dialogue: 0,0:56:31.45,0:56:34.35,Default,,0,0,0,,Ta có cái này cho em.
Dialogue: 0,0:56:34.36,0:56:36.46,Default,,0,0,0,,Nó thuộc về mẹ ta.
Dialogue: 0,0:56:38.09,0:56:40.96,Default,,0,0,0,,Nói cho em biết,
Dialogue: 0,0:56:40.96,0:56:44.47,Default,,0,0,0,,bà ấy có thấy vui không khi\Nhai thằng con trai của mình tương tàn?
Dialogue: 0,0:56:46.03,0:56:50.57,Default,,0,0,0,,Ta hiểu nỗi sợ hãi, sự miễn cưỡng của em, Mera.
Dialogue: 0,0:56:50.57,0:56:53.91,Default,,0,0,0,,Ta chắc chắn làm được. Ta không muốn chiến tranh.
Dialogue: 0,0:56:53.91,0:56:55.31,Default,,0,0,0,,Đừng hạ mình như thế.
Dialogue: 0,0:56:55.31,0:56:56.78,Default,,0,0,0,, Em hiểu anh lắm.
Dialogue: 0,0:56:56.78,0:56:58.48,Default,,0,0,0,,Những gì anh đang làm là trái với lời khuyên
Dialogue: 0,0:56:58.48,0:57:00.45,Default,,0,0,0,,mà mẹ anh đã nói khi chúng ta còn nhỏ.
Dialogue: 0,0:57:00.45,0:57:01.55,Default,,0,0,0,,Nếu bà ấy có ở đây...
Dialogue: 0,0:57:01.55,0:57:05.52,Default,,0,0,0,,Nhưng bà ấy không có ở đây!
Dialogue: 0,0:57:05.52,0:57:08.22,Default,,0,0,0,,Những gì mẹ ta dạy chúng ta là phản quốc.
Dialogue: 0,0:57:08.99,0:57:11.79,Default,,0,0,0,,Đừng làm theo cách của bà ấy.
Dialogue: 0,0:57:11.79,0:57:14.73,Default,,0,0,0,,Được rồi, vòng lửa đáng sợ, hả?
Dialogue: 0,0:57:14.73,0:57:17.87,Default,,0,0,0,,Cách thức hoạt động ra sao?\NTa đá mông ngươi ngay tại đây à?.
Dialogue: 0,0:57:21.47,0:57:22.87,Default,,0,0,0,,Vulko.
Dialogue: 0,0:57:22.87,0:57:26.84,Default,,0,0,0,,hãy hộ tống vị hôn thê\Ncủa tôi đến chỗ hoàng gia.
Dialogue: 0,0:57:38.29,0:57:39.75,Default,,0,0,0,,Đi thôi, công chúa Mera.
Dialogue: 0,0:57:45.63,0:57:49.66,Default,,0,0,0,,Ở đây chúng ta có một huyền thoại tên là Karathen.
Dialogue: 0,0:57:49.66,0:57:54.57,Default,,0,0,0,,Một quái vật biển cổ đại mạnh đến nỗi\Nchính vua Atlan cũng sợ nó.
Dialogue: 0,0:57:54.57,0:57:58.97,Default,,0,0,0,,Vì vậy, ông ta đã cầm tù nó.\NỞ dưới đáy đại dương.
Dialogue: 0,0:57:58.97,0:58:03.68,Default,,0,0,0,,Và thật tuyệt vời, sinh vật đó đang vùng vẫy để trở lại.
Dialogue: 0,0:58:03.68,0:58:07.31,Default,,0,0,0,,Atlantis giờ sẽ trỗi dậy.
Dialogue: 0,0:58:07.31,0:58:09.22,Default,,0,0,0,,Và bây giờ con thú đã thức dậy.
Dialogue: 0,0:58:15.82,0:58:21.16,Default,,0,0,0,,Ngươi biết đấy, đã có lúc\Nkhi ta muốn gặp ngươi hơn bất cứ điều gì.
Dialogue: 0,0:58:22.26,0:58:24.46,Default,,0,0,0,,Để tìm hiểu em trai ta.
Dialogue: 0,0:58:24.46,0:58:29.07,Default,,0,0,0,,Để cho nó biết rằng nó không cô đơn.\NVà giờ, chúng ta gặp nhau.
Dialogue: 0,0:58:31.61,0:58:34.44,Default,,0,0,0,,Đối với ta, ta biết ngươi là một kẻ lòng lang dạ thú.
Dialogue: 0,0:58:37.34,0:58:41.31,Default,,0,0,0,,Anh là lý do mẹ chúng ta bị xử tử.
Dialogue: 0,0:58:41.32,0:58:43.92,Default,,0,0,0,,Và ta đã ghét anh ngay từ đầu
Dialogue: 0,0:58:46.19,0:58:49.89,Default,,0,0,0,,Nhưng tôi không muốn giết anh, Arthur.
Dialogue: 0,0:58:49.89,0:58:53.13,Default,,0,0,0,,Tôi sẽ cho anh một cơ hội.
Dialogue: 0,0:58:53.79,0:58:56.16,Default,,0,0,0,,Về nhà.
Dialogue: 0,0:58:56.16,0:58:59.07,Default,,0,0,0,,Đừng bao giờ quay lại Atlantis.
Dialogue: 0,0:59:00.60,0:59:03.54,Default,,0,0,0,,Vì anh sẽ không thắng nổi đâu.
Dialogue: 0,0:59:03.54,0:59:07.34,Default,,0,0,0,,Cuộc chiến đang diễn ra, dù anh có thích hay không.
Dialogue: 0,0:59:07.34,0:59:10.98,Default,,0,0,0,,Tôi sẽ mang theo cơn thịnh nộ của 7 vương quốc đi theo tôi.
Dialogue: 0,0:59:12.11,0:59:14.28,Default,,0,0,0,,Ngươi biết ta không thể để điều đó xảy ra.
Dialogue: 0,0:59:15.55,0:59:17.12,Default,,0,0,0,,Tôi biết.
Dialogue: 0,0:59:19.99,0:59:21.46,Default,,0,0,0,,Cái quái gì thế?
Dialogue: 0,0:59:23.56,0:59:25.06,Default,,0,0,0,,Vòng lửa.
Dialogue: 0,0:59:34.20,0:59:35.10,Default,,0,0,0,,Yeah!
Dialogue: 0,0:59:37.94,0:59:39.87,Default,,0,0,0,,Yeah!
Dialogue: 0,0:59:46.61,0:59:48.01,Default,,0,0,0,,Mẹ kiếp.
Dialogue: 0,0:59:49.12,0:59:51.75,Default,,0,0,0,,Hỡi thần dân Atlantis, hãy nghe ta.
Dialogue: 0,0:59:51.75,0:59:55.62,Default,,0,0,0,, Anh trai ta đến từ mặt đất\Nđể thách thức ta và giành ngai vàng!
Dialogue: 0,0:59:57.36,1:00:00.59,Default,,0,0,0,,Hãy để chúng ta giải quyết vấn đề này theo cách cổ xưa.
Dialogue: 0,1:00:00.59,1:00:05.13,Default,,0,0,0,,Bằng cách đổ máu để cho các vị thần chứng giám!
Dialogue: 0,1:00:14.57,1:00:16.11,Default,,0,0,0,,Đồ phản bội !
Dialogue: 0,1:00:16.74,1:00:18.11,Default,,0,0,0,,Đồ phản bội !
Dialogue: 0,1:00:23.35,1:00:25.62,Default,,0,0,0,,Tên tạp chủng ngươi không xứng đáng làm vua được !
Dialogue: 0,1:00:40.13,1:00:41.90,Default,,0,0,0,,Chưa bao giờ con nghĩ rằng cha của con
Dialogue: 0,1:00:41.90,1:00:43.73,Default,,0,0,0,,sẽ cúi đầu trước Vua của Atlantis.
Dialogue: 0,1:00:43.74,1:00:47.77,Default,,0,0,0,,Người mặt đất đã gây chiến trước.\NCon muốn chúng ta làm gì?
Dialogue: 0,1:00:47.77,1:00:49.24,Default,,0,0,0,,Xin họ lòng từ bi sao?
Dialogue: 0,1:00:49.24,1:00:51.61,Default,,0,0,0,,Con ghét người mặt đất nhiều.
Dialogue: 0,1:00:51.61,1:00:54.95,Default,,0,0,0,,Nhưng cha không thấy lúc này có thuận tiện không?
Dialogue: 0,1:00:58.32,1:01:02.22,Default,,0,0,0,,Cha sẽ không dễ tin đến thế chứ?
Dialogue: 0,1:01:04.89,1:01:06.83,Default,,0,0,0,,Cha cũng muốn cuộc chiến này phải không?
Dialogue: 0,1:01:06.83,1:01:09.43,Default,,0,0,0,,Đã qua rồi, phải để cư dân mặt đất biết chúng ta hùng mạnh như thế nào.
Dialogue: 0,1:01:09.43,1:01:12.16,Default,,0,0,0,,Nếu đây là việc phải làm, thì hãy làm như vậy.
Dialogue: 0,1:01:19.00,1:01:20.40,Default,,0,0,0,,Đó là Cây đinh ba của mẹ ta
Dialogue: 0,1:01:20.41,1:01:23.61,Default,,0,0,0,,Anh sử mạnh mẽ nhưng không hoàn hảo như bà.
Dialogue: 0,1:01:24.64,1:01:26.08,Default,,0,0,0,,Tôi sẽ sử dụng binh khí của cha tôi.
Dialogue: 0,1:01:27.28,1:01:29.08,Default,,0,0,0,,Và nó chưa bao giờ biết đến thất bại!
Dialogue: 0,1:02:17.63,1:02:20.13,Default,,0,0,0,,Ta đã hứa với Atlanna rằng ta sẽ bảo vệ cháu.
Dialogue: 0,1:02:27.41,1:02:30.78,Default,,0,0,0,,Đây không phải là một cuộc thi đấu.\N Mà là một cuộc giết chóc.
Dialogue: 0,1:04:27.79,1:04:31.56,Default,,0,0,0,,Ta là một vị vua thực sự!
Dialogue: 0,1:05:15.14,1:05:17.24,Default,,0,0,0,,Anh còn chờ mời nữa à? Vào đi!
Dialogue: 0,1:05:27.99,1:05:29.35,Default,,0,0,0,,Mera!
Dialogue: 0,1:05:32.32,1:05:34.06,Default,,0,0,0,,Oh, yeah.\NVậy kế hoạch là gì?
Dialogue: 0,1:05:34.06,1:05:37.19,Default,,0,0,0,,Là đi tìm Cây đinh ba của Atlan.\NSau đó sẽ thách đấu với Orm.
Dialogue: 0,1:05:37.20,1:05:38.93,Default,,0,0,0,,Ok. Vậy là do chúng ta làm việc không theo quy trình.
Dialogue: 0,1:05:38.93,1:05:39.93,Default,,0,0,0,, Chết tiệt.
Dialogue: 0,1:05:45.50,1:05:47.44,Default,,0,0,0,,Cẩn thận, có kẻ bám đuôi chúng ta
Dialogue: 0,1:05:47.44,1:05:49.70,Default,,0,0,0,,- Ý anh là gì??\N- Kẻ xấu ở sau chúng ta.
Dialogue: 0,1:05:49.71,1:05:52.47,Default,,0,0,0,,- Sao không nói sớm!\N- Kẻ xấu sau chúng ta!
Dialogue: 0,1:07:00.01,1:07:02.08,Default,,0,0,0,,Whoa, whoa, đợi đã. \NCô nói chúng ta không nên đi qua những bức tường này
Dialogue: 0,1:07:02.08,1:07:04.08,Default,,0,0,0,,- Nhưng chúng ta lỡ đã qua.\N- Cô không nên giấu giếm điều này, nó bắn súng hydro kìa!
Dialogue: 0,1:07:04.08,1:07:05.35,Default,,0,0,0,, Câm miệng đi! Anh lải nhải như đàn bà ấy!
Dialogue: 0,1:07:09.69,1:07:11.69,Default,,0,0,0,,Chết cha rồi!
Dialogue: 0,1:07:34.68,1:07:36.41,Default,,0,0,0,, Chết tiệt.
Dialogue: 0,1:07:37.05,1:07:38.78,Default,,0,0,0,, Chết tiệt.
Dialogue: 0,1:07:38.78,1:07:41.79,Default,,0,0,0,,Chúng ta chưa chết!\NHãy nhảy để chúng tin là đã chết!
Dialogue: 0,1:07:43.52,1:07:44.75,Default,,0,0,0,,Nhảy!
Dialogue: 0,1:07:52.00,1:07:53.40,Default,,0,0,0,,Đây, cầm cái này.
Dialogue: 0,1:07:55.33,1:07:56.73,Default,,0,0,0,,Anh làm gì vậy?
Dialogue: 0,1:07:56.73,1:07:58.57,Default,,0,0,0,,- Nghe này, chúng ta sẽ bắt chước Pinnochio.\N- Ai?
Dialogue: 0,1:07:58.57,1:08:01.37,Default,,0,0,0,,Đừng lải nhải, đến đây. Vào đi.
Dialogue: 0,1:08:03.34,1:08:05.51,Default,,0,0,0,,Tuyệt vời. Chúng ta bị nó ăn rồi.
Dialogue: 0,1:08:27.00,1:08:29.23,Default,,0,0,0,,Anh làm điều này bằng cách nào?
Dialogue: 0,1:08:29.23,1:08:31.00,Default,,0,0,0,,Tôi không biết.
Dialogue: 0,1:08:31.00,1:08:33.57,Default,,0,0,0,,Đôi khi ta có thể nghe thấy chúng.\N Chúng có thể hiểu ta
Dialogue: 0,1:08:33.57,1:08:36.44,Default,,0,0,0,,Phải. Cả hai chúng ta đều ngu ngốc.
Dialogue: 0,1:08:36.44,1:08:38.97,Default,,0,0,0,,- Này, tôi vừa cứu mạng cô.\N- Tôi cứu anh trước nhen.
Dialogue: 0,1:08:43.48,1:08:45.48,Default,,0,0,0,,Nó nói ta đã an toàn.
Dialogue: 0,1:08:45.48,1:08:47.62,Default,,0,0,0,,Vậy đi đâu bây giờ?
Dialogue: 0,1:08:48.85,1:08:52.52,Default,,0,0,0,,Đến đại dương cát, nơi mà người mặt đất gọi là Sahara.
Dialogue: 0,1:08:53.42,1:08:54.99,Default,,0,0,0,,Phải rồi
Dialogue: 0,1:09:11.51,1:09:14.51,Default,,0,0,0,,Đông Sahara
Dialogue: 0,1:09:18.51,1:09:20.11,Default,,0,0,0,,Tôi thường không kể công,
Dialogue: 0,1:09:20.12,1:09:22.65,Default,,0,0,0,,nhưng tôi thường không được\N trả tiền bởi kho báu cướp biển.
Dialogue: 0,1:09:22.65,1:09:23.92,Default,,0,0,0,,Cảm ơn anh đã cho đi ké.
Dialogue: 0,1:09:25.92,1:09:27.89,Default,,0,0,0,,Oh, được rồi.
Dialogue: 0,1:09:27.89,1:09:29.66,Default,,0,0,0,,Cô ổn chứ?
Dialogue: 0,1:09:29.66,1:09:31.86,Default,,0,0,0,,Tôi chưa bao giờ lên trên cao như thế này.
Dialogue: 0,1:09:33.60,1:09:35.60,Default,,0,0,0,,Chúng ta đang ở rất xa nhà.
Dialogue: 0,1:09:36.60,1:09:38.47,Default,,0,0,0,,Ít nhất cô còn có một ngôi nhà.
Dialogue: 0,1:09:39.27,1:09:40.80,Default,,0,0,0,,Không còn nữa.
Dialogue: 0,1:09:40.80,1:09:42.50,Default,,0,0,0,, Tôi không bao giờ có thể quay lại.
Dialogue: 0,1:09:42.50,1:09:45.47,Default,,0,0,0,,Tôi đã phản bội tất cả mọi thứ khi cứu anh tại Đấu trường.
Dialogue: 0,1:09:49.38,1:09:52.65,Default,,0,0,0,,Nhưng cô là vị hôn thê của vua, họ sẽ đưa cô trở lại thôi.
Dialogue: 0,1:09:52.65,1:09:55.82,Default,,0,0,0,,Atlantis có nhiều điều tuyệt vời.
Dialogue: 0,1:09:55.82,1:09:57.72,Default,,0,0,0,,Nhưng tha thứ thì không.
Dialogue: 0,1:09:57.72,1:09:59.05,Default,,0,0,0,,Nhưng cô là người hoàng tộc.
Dialogue: 0,1:09:59.05,1:10:01.36,Default,,0,0,0,,Mẹ anh cũng vậy đấy thôi.
Dialogue: 0,1:10:01.36,1:10:04.22,Default,,0,0,0,,Nếu tôi quay lại bây giờ, tôi sẽ bị đem\Nhiến tế cho bọn người Trench.
Dialogue: 0,1:10:04.23,1:10:06.93,Default,,0,0,0,,Ngay cả cha tôi cũng không dám đưa tôi trở lại.
Dialogue: 0,1:10:11.47,1:10:13.40,Default,,0,0,0,,Nghĩ tích cực đi.
Dialogue: 0,1:10:13.40,1:10:15.67,Default,,0,0,0,,Cô sẽ không phải kết hôn với người cô không yêu.
Dialogue: 0,1:10:15.67,1:10:18.87,Default,,0,0,0,,Nghĩa vụ không phải là tình yêu.
Dialogue: 0,1:10:18.87,1:10:21.44,Default,,0,0,0,,Đây là gia đình và vương quốc của tôi.
Dialogue: 0,1:10:21.44,1:10:23.51,Default,,0,0,0,,Và tôi đã quay lưng lại với cả hai.
Dialogue: 0,1:10:25.81,1:10:29.58,Default,,0,0,0,,Đôi khi chúng ta phải làm chuyện đúng,
Dialogue: 0,1:10:29.58,1:10:32.52,Default,,0,0,0,,ngay cả khi trái tim đau nhói vì nó.
Dialogue: 0,1:10:48.34,1:10:50.74,Default,,0,0,0,,Kính thưa đức Vua.
Dialogue: 0,1:10:50.74,1:10:55.84,Default,,0,0,0,,Công chúa Mera đang ở đây.\NỞ Vương quốc Desert.
Dialogue: 0,1:10:56.48,1:10:57.91,Default,,0,0,0,,Anh nói nó đã chết rồi mà.
Dialogue: 0,1:10:58.51,1:10:59.98,Default,,0,0,0,,Rõ ràng là chưa chết.
Dialogue: 0,1:11:01.42,1:11:03.18,Default,,0,0,0,,Anh trai của anh có còn sống không?
Dialogue: 0,1:11:03.19,1:11:04.52,Default,,0,0,0,,Sẽ không còn lâu nữa.
Dialogue: 0,1:11:04.52,1:11:06.52,Default,,0,0,0,,Thuyền trưởng Murk, tập hợp lực lượng tấn công ngay lập tức.
Dialogue: 0,1:11:06.52,1:11:11.26,Default,,0,0,0,,Không ! Mera sẽ bị bắt và trả lại an toàn cho ta.
Dialogue: 0,1:11:11.26,1:11:12.43,Default,,0,0,0,,Cô ấy đã phản bội vương quyền
Dialogue: 0,1:11:12.43,1:11:14.83,Default,,0,0,0,,Nó là con gái của tộc người Xebel.
Dialogue: 0,1:11:14.83,1:11:17.80,Default,,0,0,0,,Nếu có mệnh hệ gì xảy ra với nó,\Nliên minh của chúng ta sẽ chấm dứt.
Dialogue: 0,1:11:17.80,1:11:19.37,Default,,0,0,0,,Thưa bệ hạ,
Dialogue: 0,1:11:19.37,1:11:22.77,Default,,0,0,0,, chúng ta hiện đang tiến vào\Nvùng biển của Vương quốc Tritonis bọn người cá.
Dialogue: 0,1:11:22.77,1:11:26.87,Default,,0,0,0,,Mối quan tâm trước mắt là phải
Dialogue: 0,1:11:26.87,1:11:28.98,Default,,0,0,0,,đảm bảo sự liên minh giữa hai vương quốc
Dialogue: 0,1:11:32.11,1:11:38.08,Default,,0,0,0,,Tể tướng của ta nói chính xác.\NNhư thường lệ.
Dialogue: 0,1:11:38.09,1:11:44.59,Default,,0,0,0,,Bắt giữ họ và đem con gái của Vua Nereus về đây. Không được gây thương tích.
Dialogue: 0,1:12:07.48,1:12:08.98,Default,,0,0,0,,Đến nơi rồi.
Dialogue: 0,1:12:10.38,1:12:11.65,Default,,0,0,0,,Này,
Dialogue: 0,1:12:11.65,1:12:13.19,Default,,0,0,0,, có thứ gì gần đây không?
Dialogue: 0,1:12:13.19,1:12:15.29,Default,,0,0,0,,Không có gì ngoài sa mạc mênh mông cả.
Dialogue: 0,1:12:15.29,1:12:17.32,Default,,0,0,0,,Anh ta đã nhầm !
Dialogue: 0,1:12:17.33,1:12:20.39,Default,,0,0,0,,- Ê, cô tự tử hả?\N- Mera, không!
Dialogue: 0,1:12:23.10,1:12:25.96,Default,,0,0,0,,Cô ấy nhảy mà không cần dù.
Dialogue: 0,1:12:25.97,1:12:28.47,Default,,0,0,0,,Tóc đỏ vú to. Ta phải yêu họ thôi.
Dialogue: 0,1:12:43.62,1:12:45.75,Default,,0,0,0,,Chết tiệt
Dialogue: 0,1:12:57.90,1:12:59.70,Default,,0,0,0,,Cô khùng hơn tôi nghĩ.
Dialogue: 0,1:12:59.70,1:13:01.14,Default,,0,0,0,,A-ha!
Dialogue: 0,1:13:02.24,1:13:03.70,Default,,0,0,0,,Theo hướng này.
Dialogue: 0,1:13:06.79,1:13:09.79,Default,,0,0,0,,Đâu đó ở Ấn Độ Dương
Dialogue: 0,1:13:24.79,1:13:28.36,Default,,0,0,0,,{\i1}Lần trước, anh đã bị đánh bại vì\Nvũ khí của người mặt đất cùi bắp quá.{\i}
Dialogue: 0,1:13:29.33,1:13:30.46,Default,,0,0,0,,{\i1}Nhưng vũ khí của chúng tôi thì không.{\i}
Dialogue: 0,1:13:33.87,1:13:35.40,Default,,0,0,0,,{\i1}Đó là nguyên mẫu áo giáp thế hệ mới{\i}
Dialogue: 0,1:13:35.40,1:13:38.27,Default,,0,0,0,,{\i1} dành cho lính Atlantis.{\i}
Dialogue: 0,1:13:38.27,1:13:41.01,Default,,0,0,0,,{\i1}Trả thù là điều mà người Atlantis chúng ta hiểu rõ.{\i}
Dialogue: 0,1:13:41.01,1:13:44.38,Default,,0,0,0,,{\i1}Anh sẽ lãnh đạo các chiến binh giỏi nhất của\Ntôi với tư cách là đội quân của riêng anh,{\i}
Dialogue: 0,1:13:44.38,1:13:46.95,Default,,0,0,0,,{\i1}để săn tìm tên cẩu tạp chủng đó.{\i}
Dialogue: 0,1:13:46.95,1:13:51.45,Default,,0,0,0,,{\i1}Tôi không thể chạm vào chúng. Nhưng anh có thể..\NHắn ta trốn sang thế giới mặt đất của anh.{\i}
Dialogue: 0,1:13:51.45,1:13:54.92,Default,,0,0,0,,{\i1}Giết hắn ta và con tiện nhân đi cùng\Nthì anh sẽ được thưởng.{\i}
Dialogue: 0,1:13:54.92,1:13:57.22,Default,,0,0,0,,Giết được hắn là phần thưởng của ta rồi.
Dialogue: 0,1:14:00.69,1:14:03.06,Default,,0,0,0,,{\i1}Vũ khí này vẫn đang thử nghiệm.{\i}
Dialogue: 0,1:14:04.63,1:14:07.50,Default,,0,0,0,,{\i1}Nó hội tụ nước thành chùm plasma.{\i}
Dialogue: 0,1:15:19.67,1:15:21.27,Default,,0,0,0,,Hmm.
Dialogue: 0,1:15:21.27,1:15:23.78,Default,,0,0,0,,Ta nghĩ rằng chúng ta sẽ cần một\Nchiếc mũ bảo hiểm lớn hơn.
Dialogue: 0,1:15:23.78,1:15:26.38,Default,,0,0,0,,Một trong những bộ tộc của Atlantis\Nđã tách ra và định cư ở đây
Dialogue: 0,1:15:26.38,1:15:28.55,Default,,0,0,0,,khi khu vực này vẫn còn là\Nmột vùng biển từ hàng triệu năm trước.
Dialogue: 0,1:15:28.55,1:15:31.38,Default,,0,0,0,,Khi nước cạn, họ đã biến mất cùng với nó.
Dialogue: 0,1:15:31.38,1:15:33.12,Default,,0,0,0,,Bị diệt vong? Vâng, điều đó thật tuyệt.
Dialogue: 0,1:15:33.12,1:15:34.35,Default,,0,0,0,,Chúng ta sắp tới rồi
Dialogue: 0,1:15:34.35,1:15:36.82,Default,,0,0,0,,Oh, tới cái gì?\NChỗ chết khát à?
Dialogue: 0,1:15:36.82,1:15:38.79,Default,,0,0,0,,Tôi sắp đấm vào mặt anh đây.
Dialogue: 0,1:15:38.79,1:15:40.59,Default,,0,0,0,,Đấm nhau? Cô không biết mọi thứ hoạt động ở đây như thế nào.
Dialogue: 0,1:15:40.59,1:15:42.89,Default,,0,0,0,,Tại sao cô không nhìn xung quanh, chúng ta bị lạc rồi.
Dialogue: 0,1:15:42.90,1:15:44.50,Default,,0,0,0,,Cô thấy gì ở đằng kia? Chả có gì.
Dialogue: 0,1:15:44.50,1:15:47.26,Default,,0,0,0,,Nơi này là một vùng đất hoang.
Dialogue: 0,1:15:47.27,1:15:49.77,Default,,0,0,0,,Anh gọi đây là 'vùng đất hoang' sao, nó là nhà anh đấy.
Dialogue: 0,1:15:49.77,1:15:51.40,Default,,0,0,0,,Đây không phải là nhà của tôi.
Dialogue: 0,1:15:51.40,1:15:53.24,Default,,0,0,0,,Chỗ tôi ở không giống như thế này.
Dialogue: 0,1:15:53.24,1:15:55.17,Default,,0,0,0,,Ồ, không. Tất nhiên là không.
Dialogue: 0,1:15:55.17,1:15:58.41,Default,,0,0,0,,Anh cũng có những thành phố kinh tởm\Nvới những cống rãnh xả rác thải ra biển
Dialogue: 0,1:15:58.41,1:16:00.31,Default,,0,0,0,,tạo nên những ngọn núi rác thải.
Dialogue: 0,1:16:00.31,1:16:02.01,Default,,0,0,0,,Các anh còn có những nhà máy lớn
Dialogue: 0,1:16:02.01,1:16:03.91,Default,,0,0,0,, thải khí độc làm nóng lên toàn...
Dialogue: 0,1:16:03.92,1:16:05.98,Default,,0,0,0,,Ok, thôi nín giùm. Tôi biết rồi. Nghe này.
Dialogue: 0,1:16:05.98,1:16:07.45,Default,,0,0,0,,Vâng, chúng tôi đã có một số kẻ dốt làm lãnh đạo.
Dialogue: 0,1:16:07.45,1:16:09.45,Default,,0,0,0,,Nhưng vẫn có những điều tốt mà, phải không?
Dialogue: 0,1:16:09.46,1:16:11.72,Default,,0,0,0,,Chúng tôi có rừng xanh, núi cao,\Nvà những hồ nước tuyệt đẹp.
Dialogue: 0,1:16:11.72,1:16:14.56,Default,,0,0,0,,Cô sẽ thích chúng, chúng như những đại dương thu nhỏ.
Dialogue: 0,1:16:14.56,1:16:16.39,Default,,0,0,0,,Anh đang cố khiêu khích tôi phải không?
Dialogue: 0,1:16:16.40,1:16:18.10,Default,,0,0,0,,Tôi chỉ đang nói cô đừng phán xét\Nmột nơi trước khi cô nhìn thấy nó.
Dialogue: 0,1:16:18.10,1:16:20.93,Default,,0,0,0,,Anh cũng đã đánh giá Atlantis thấp như thế đấy.
Dialogue: 0,1:16:24.34,1:16:27.87,Default,,0,0,0,,Không, không, không. Cô dừng lại đi.
Dialogue: 0,1:16:27.87,1:16:31.48,Default,,0,0,0,,GPS Atlantis ngu ngốc của cô vừa bảo\Ncô phải nhảy ra khỏi máy bay
Dialogue: 0,1:16:31.48,1:16:33.44,Default,,0,0,0,,ở giữa sa mạc. Và bây giờ chúng ta bị mắc kẹt.
Dialogue: 0,1:16:36.75,1:16:39.39,Default,,0,0,0,,Nếu anh là vị vua thực sự tiếp theo,\Nchúng ta sẽ phải diệt vong mất.
Dialogue: 0,1:17:00.71,1:17:03.71,Default,,0,0,0,,Vương quốc Desert - Sa mạc
Dialogue: 0,1:17:07.71,1:17:09.05,Default,,0,0,0,,Thật tuyệt vời.
Dialogue: 0,1:17:14.22,1:17:17.06,Default,,0,0,0,,Hãy nhìn những gì ta tìm thấy.
Dialogue: 0,1:17:18.06,1:17:19.36,Default,,0,0,0,,Anh đã tìm thấy gì à?
Dialogue: 0,1:17:19.36,1:17:20.36,Default,,0,0,0,,Ờ.
Dialogue: 0,1:17:30.77,1:17:32.47,Default,,0,0,0,,Nhìn nơi này đi.
Dialogue: 0,1:17:33.07,1:17:34.70,Default,,0,0,0,,Thật kỳ vĩ !
Dialogue: 0,1:17:36.28,1:17:38.74,Default,,0,0,0,,Đây là nơi chứa vũ khí
Dialogue: 0,1:17:38.74,1:17:42.08,Default,,0,0,0,,Cũng là nơi đúc nên Cây đinh ba của Atlantis
Dialogue: 0,1:17:43.45,1:17:45.58,Default,,0,0,0,,Tôi không còn nghĩ nó là một huyền thoại nữa.
Dialogue: 0,1:17:51.82,1:17:53.26,Default,,0,0,0,,Chú Vulko đã đúng.
Dialogue: 0,1:17:55.79,1:17:57.23,Default,,0,0,0,,Đó là sự thật.
Dialogue: 0,1:18:34.57,1:18:35.60,Default,,0,0,0,,Hừm
Dialogue: 0,1:18:36.33,1:18:38.30,Default,,0,0,0,,Không có gì xảy ra.
Dialogue: 0,1:18:38.30,1:18:40.30,Default,,0,0,0,,Tất nhiên là nó không hoạt động.
Dialogue: 0,1:18:40.30,1:18:42.67,Default,,0,0,0,, Nó đã ở đây và bị bụi bao phủ từ trước khi Sahara là sa mạc.
Dialogue: 0,1:18:44.88,1:18:46.64,Default,,0,0,0,,Mẹ kiếp.
Dialogue: 0,1:18:46.64,1:18:49.68,Default,,0,0,0,,"Từ trước khi Sahara trở thành sa mạc."
Dialogue: 0,1:18:49.68,1:18:51.85,Default,,0,0,0,,Cảm ơn cô đã lặp lại câu nói của tôi.
Dialogue: 0,1:18:51.85,1:18:53.72,Default,,0,0,0,,Nó đã khô cạn hoàn toàn.
Dialogue: 0,1:18:55.65,1:18:57.22,Default,,0,0,0,,Tôi nghĩ đầu anh rỗng tuếch thật.
Dialogue: 0,1:18:57.22,1:18:59.22,Default,,0,0,0,,Anh chỉ khôn ra khi đầu anh không suy nghĩ gì cả
Dialogue: 0,1:18:59.22,1:19:00.29,Default,,0,0,0,,Bây giờ giữ yên nào.
Dialogue: 0,1:19:00.29,1:19:02.56,Default,,0,0,0,,Này. Cô làm gì thế?
Dialogue: 0,1:19:02.56,1:19:05.70,Default,,0,0,0,,Chúng ta cần nước\Nvà anh là nguồn nước gần nhất.
Dialogue: 0,1:19:06.70,1:19:08.33,Default,,0,0,0,,Nào, đứng yên đi.
Dialogue: 0,1:19:14.47,1:19:15.51,Default,,0,0,0,,Hừm.
Dialogue: 0,1:19:44.50,1:19:45.64,Default,,0,0,0,,Tưởng gì
Dialogue: 0,1:19:46.50,1:19:48.14,Default,,0,0,0,,Tôi có thể đi tiểu lên nó cho nhanh.
Dialogue: 0,1:19:58.58,1:19:59.75,Default,,0,0,0,,Vua Atlan.
Dialogue: 0,1:20:01.75,1:20:06.89,Default,,0,0,0,,{\i1}Trong Cây đinh ba này chứa sức mạnh của Atlantis.{\i}
Dialogue: 0,1:20:06.89,1:20:10.79,Default,,0,0,0,,{\i1}Trong tay kẻ ác, nó sẽ mang lại sự hủy diệt.{\i}
Dialogue: 0,1:20:10.80,1:20:13.06,Default,,0,0,0,,{\i1}Trong tay người thừa kế thực sự,\Nnó sẽ hợp nhất tất cả chúng ta{\i}
Dialogue: 0,1:20:13.06,1:20:17.17,Default,,0,0,0,,{\i1}cả vương quốc bên trên và bên dưới.{\i}
Dialogue: 0,1:20:17.17,1:20:22.10,Default,,0,0,0,,{\i1}Nếu ngươi tìm kiếm sức mạnh của ta,\Nngươi phải chứng minh giá trị của mình.{\i}
Dialogue: 0,1:20:22.11,1:20:25.64,Default,,0,0,0,,{\i1}Hành trình vượt ra khỏi rìa thế giới đến vùng biển ẩn.{\i}
Dialogue: 0,1:20:25.64,1:20:28.91,Default,,0,0,0,,{\i1}Nhìn vào bên trong chai để thấy được tọa độ của điểm cần đến.{\i}
Dialogue: 0,1:20:28.91,1:20:34.65,Default,,0,0,0,,{\i1}Chỉ khi trong tay của vị vua thực sự,\Nngười đó mới thực sự có thể tìm thấy.{\i}
Dialogue: 0,1:20:42.89,1:20:44.19,Default,,0,0,0,,Hừm.
Dialogue: 0,1:20:51.03,1:20:52.23,Default,,0,0,0,,Chúng ta không thể để Orm tìm thấy cái này.
Dialogue: 0,1:20:54.67,1:20:55.84,Default,,0,0,0,,Này, đợi đã!
Dialogue: 0,1:20:56.94,1:20:58.47,Default,,0,0,0,,Cái gì?
Dialogue: 0,1:20:58.48,1:21:00.27,Default,,0,0,0,,Chúng ta có nên viết nó xuống trước không?
Dialogue: 0,1:21:00.28,1:21:02.38,Default,,0,0,0,,Tôi đã ghi nhớ nó. Anh không nhớ à?
Dialogue: 0,1:21:03.98,1:21:05.55,Default,,0,0,0,,Ồ, có chứ.
Dialogue: 0,1:21:07.12,1:21:08.42,Default,,0,0,0,,Ông ta vừa nói gì?
Dialogue: 0,1:21:10.25,1:21:12.46,Default,,0,0,0,,"Một cái gì đó về cây đinh ba."
Dialogue: 0,1:21:26.74,1:21:29.71,Default,,0,0,0,,"Nhìn vào bên trong chai để thấy được tọa độ của điểm cần đến."
Dialogue: 0,1:21:35.88,1:21:37.68,Default,,0,0,0,,Đây là điểm dừng chân tiếp theo của chúng ta.
Dialogue: 0,1:23:11.47,1:23:14.37,Default,,0,0,0,,Tôi bắt đầu nghĩ rằng cô thực sự\Nthích những người sống trên bề mặt đất.
Dialogue: 0,1:23:14.38,1:23:17.31,Default,,0,0,0,,Sẽ là sai lầm khi đánh giá một địa điểm mà ta chưa từng tới.
Dialogue: 0,1:23:19.45,1:23:21.92,Default,,0,0,0,,Tôi sẽ quay lại ngay. Đừng đi đâu cả.
Dialogue: 0,1:23:41.07,1:23:44.94,Default,,0,0,0,,Chà, rõ ràng là nơi ta cần đến đang ở ngay\Ntrên ngọn đồi có lâu đài cổ kia.
Dialogue: 0,1:23:44.94,1:23:47.81,Default,,0,0,0,,Tôi vừa học được một điều rất thú vị.
Dialogue: 0,1:23:50.38,1:23:54.28,Default,,0,0,0,,Anh mạo hiểm mạng sống của chúng ta\Nvới quyển sách thiếu nhi?
Dialogue: 0,1:23:54.28,1:23:57.58,Default,,0,0,0,,Là một cuốn sách? Làm thế nào nó...
Dialogue: 0,1:23:57.59,1:23:58.99,Default,,0,0,0,,Oh!
Dialogue: 0,1:23:58.99,1:24:00.39,Default,,0,0,0,,Thế mà tôi tưởng nó là một bộ phim, đi thôi.
Dialogue: 0,1:24:05.89,1:24:09.33,Default,,0,0,0,,Vua Nereus, ta đã hy vọng rằng thần dân
Dialogue: 0,1:24:09.33,1:24:11.50,Default,,0,0,0,,của chúng ta đã phát triển đến mức biết rằng
Dialogue: 0,1:24:11.50,1:24:13.73,Default,,0,0,0,,cuộc chiến với người mặt đất là không thể tránh khỏi.
Dialogue: 0,1:24:13.74,1:24:16.57,Default,,0,0,0,,Nhiệm vụ của Người cá là khi đến lúc,
Dialogue: 0,1:24:16.57,1:24:19.84,Default,,0,0,0,,thì phải ra tay với người mặt đất cho họ biết lễ độ.
Dialogue: 0,1:24:19.84,1:24:23.71,Default,,0,0,0,,Nhưng cần phải giáo dục họ,\Nchứ không phải hủy diệt họ.
Dialogue: 0,1:24:25.01,1:24:27.78,Default,,0,0,0,,Ta ngưỡng mộ đức độ cao cả của Ngài, Vua Ricou
Dialogue: 0,1:24:31.52,1:24:36.52,Default,,0,0,0,,Nhưng ta nghi ngờ nó là một đám mây đen\Nche phủ trái tim hèn nhát
Dialogue: 0,1:24:36.52,1:24:39.06,Default,,0,0,0,,Sao ngươi dám!
Dialogue: 0,1:24:40.93,1:24:43.23,Default,,0,0,0,,Làm ơn đi, ta cầu xin ngài.
Dialogue: 0,1:24:43.23,1:24:47.03,Default,,0,0,0,,Các triết gia và với sự mềm dẻo\Nnhợt nhạt của vương quốc ngài
Dialogue: 0,1:24:47.03,1:24:50.57,Default,,0,0,0,,Những điều đó bị ứ đọng\Nvà nó làm ta phát ốm.
Dialogue: 0,1:24:59.18,1:25:00.68,Default,,0,0,0,,Vua của ta!
Dialogue: 0,1:25:00.68,1:25:04.45,Default,,0,0,0,,Nhà vua đã chết.\NNgười thừa kế muôn năm.
Dialogue: 0,1:25:06.19,1:25:07.42,Default,,0,0,0,,Công chúa.
Dialogue: 0,1:25:08.42,1:25:12.03,Default,,0,0,0,, Chuẩn bị quân đội của ngươi thay nhà vua đi,
Dialogue: 0,1:25:12.03,1:25:15.56,Default,,0,0,0,,chúng ta sẽ cùng nhau chống lại vương quốc Người Brine.
Dialogue: 0,1:25:23.04,1:25:25.11,Default,,0,0,0,,Bản ghi nói gì nữa?
Dialogue: 0,1:25:25.11,1:25:28.07,Default,,0,0,0,,"Hành trình vượt ra khỏi rìa thế giới đến vùng biển ẩn.
Dialogue: 0,1:25:28.08,1:25:30.18,Default,,0,0,0,,"Nhìn vào bên trong chai để thấy được tọa độ của điểm cần đến."
Dialogue: 0,1:25:30.18,1:25:34.48,Default,,0,0,0,,"Chỉ khi trong tay của vị vua thực sự,\Nngười đó mới thực sự có thể tìm thấy."
Dialogue: 0,1:25:36.18,1:25:39.15,Default,,0,0,0,,"Nhìn vào bên trong chai để thấy được tọa độ của điểm cần đến."
Dialogue: 0,1:25:41.12,1:25:42.46,Default,,0,0,0,,Hừm.
Dialogue: 0,1:25:44.29,1:25:45.83,Default,,0,0,0,,Ôi trời.
Dialogue: 0,1:25:46.36,1:25:47.56,Default,,0,0,0,,Cái gì?
Dialogue: 0,1:25:47.56,1:25:49.93,Default,,0,0,0,,Ồ, thật thú vị
Dialogue: 0,1:25:49.93,1:25:51.10,Default,,0,0,0,,Cho xem với
Dialogue: 0,1:25:52.47,1:25:54.67,Default,,0,0,0,,Ồ. Nhưng làm thế nào để anh biết nơi chỉ điểm đó?
Dialogue: 0,1:25:54.67,1:25:56.20,Default,,0,0,0,,Đây, hãy để tôi nhìn thử
Dialogue: 0,1:25:56.20,1:25:57.90,Default,,0,0,0,,Dòng cuối cùng là gì?
Dialogue: 0,1:25:57.91,1:26:01.37,Default,,0,0,0,,"Chỉ khi trong tay của vị vua thực sự,\Nngười đó mới thực sự có thể tìm thấy."
Dialogue: 0,1:26:01.38,1:26:03.11,Default,,0,0,0,,"Chỉ khi trong tay của vị vua thực sự..."
Dialogue: 0,1:26:04.58,1:26:06.71,Default,,0,0,0,,Marcus Agrippa.
Dialogue: 0,1:26:06.71,1:26:08.68,Default,,0,0,0,,Ông là một vị tướng vĩ đại, nhưng ông không phải là một vị vua.
Dialogue: 0,1:26:10.55,1:26:12.05,Default,,0,0,0,,Scipio cũng không.
Dialogue: 0,1:26:13.35,1:26:15.02,Default,,0,0,0,,Làm thế nào để anh biết tất cả những thứ này?
Dialogue: 0,1:26:15.02,1:26:17.12,Default,,0,0,0,,Bởi vì cha tôi bắt tôi học lịch sử.
Dialogue: 0,1:26:17.12,1:26:23.43,Default,,0,0,0,,Nhưng không ai trong số những người này là vua,\Nngoại trừ ông này.
Dialogue: 0,1:26:23.43,1:26:25.47,Default,,0,0,0,,Romulos, vị vua đầu tiên của Rome.
Dialogue: 0,1:26:27.53,1:26:30.70,Default,,0,0,0,,"Chỉ khi trong tay của vị vua thực sự,\Nngười đó mới thực sự có thể tìm thấy."
Dialogue: 0,1:26:40.68,1:26:42.68,Default,,0,0,0,,Đây rồi. Đó là nơi chúng ta đang hướng tới.
Dialogue: 0,1:26:42.68,1:26:44.65,Default,,0,0,0,,Cái gì? Để tôi xem.
Dialogue: 0,1:26:46.89,1:26:48.05,Default,,0,0,0,,Chúng ta tìm ra rồi!
Dialogue: 0,1:26:48.06,1:26:49.22,Default,,0,0,0,,Oh!
Dialogue: 0,1:26:51.02,1:26:52.99,Default,,0,0,0,,Không tệ, đối với một pha ngã ngu ngốc hả?
Dialogue: 0,1:26:55.26,1:26:56.70,Default,,0,0,0,,Không tệ chút nào.
Dialogue: 0,1:27:10.48,1:27:11.78,Default,,0,0,0,,Mera.
Dialogue: 0,1:27:13.61,1:27:19.45,Default,,0,0,0,,"Chờ đợi trong vô cảm\Ntới mức chìm vào giấc ngủ sâu,
Dialogue: 0,1:27:19.45,1:27:24.73,Default,,0,0,0,,"để được thấy sự tàn phá lan rộng khắp trên các\Nthành phố của loài người."
Dialogue: 0,1:27:28.43,1:27:29.66,Default,,0,0,0,,Lui lại !
Dialogue: 0,1:27:35.10,1:27:36.27,Default,,0,0,0,,Arthur!
Dialogue: 0,1:27:39.17,1:27:40.87,Default,,0,0,0,,Ta ra lệnh các ngươi bỏ vũ khí xuống !
Dialogue: 0,1:27:40.88,1:27:43.08,Default,,0,0,0,,{\i1}Công chúa Mera ! Người nên nhớ mình đang mang tội phản bội.{\i}
Dialogue: 0,1:28:00.40,1:28:02.40,Default,,0,0,0,,Khóa mục tiêu
Dialogue: 0,1:28:03.40,1:28:04.73,Default,,0,0,0,,Mày là cái quái gì thế ?
Dialogue: 0,1:28:04.73,1:28:06.93,Default,,0,0,0,,Có lẽ cái này sẽ làm mi nhớ tới
Dialogue: 0,1:28:54.81,1:28:56.98,Default,,0,0,0,,Mày là cái giống lúc ở tàu ngầm à?
Dialogue: 0,1:28:56.98,1:28:58.42,Default,,0,0,0,,Phải rồi
Dialogue: 0,1:28:59.79,1:29:02.22,Default,,0,0,0,,Nhưng ta đã có vũ khí thép của Atlantis rồi !
Dialogue: 0,1:29:07.86,1:29:10.83,Default,,0,0,0,,Hãy gọi ta là Black Manta !
Dialogue: 0,1:29:25.91,1:29:28.11,Default,,0,0,0,,Mi nghĩ sẽ thoát khỏi tay ta sao?
Dialogue: 0,1:29:33.99,1:29:35.89,Default,,0,0,0,,Tao nhớ mày đã làm chuyện này rồi !
Dialogue: 0,1:29:40.33,1:29:41.33,Default,,0,0,0,,Xin lỗi bà
Dialogue: 0,1:29:56.51,1:29:57.81,Default,,0,0,0,,Mi không thể chạy thoát được đâu!
Dialogue: 0,1:29:58.61,1:30:00.88,Default,,0,0,0,,Mi với ta có nợ máu
Dialogue: 0,1:30:00.88,1:30:02.55,Default,,0,0,0,,Làm sao mày tìm được ta?
Dialogue: 0,1:30:02.55,1:30:06.82,Default,,0,0,0,,Ta theo dõi công chúa Mera bên cạnh mi !
Dialogue: 0,1:31:23.56,1:31:24.76,Default,,0,0,0,,Không!
Dialogue: 0,1:31:59.16,1:32:00.17,Default,,0,0,0,,Nào.
Dialogue: 0,1:32:04.80,1:32:06.67,Default,,0,0,0,,Mọi người núp đi !
Dialogue: 0,1:32:11.74,1:32:15.84,Default,,0,0,0,,Ta sẽ cho ngươi thấy sự thương xót\Ngiống như ngươi đã thể hiện với cha ta.
Dialogue: 0,1:32:15.85,1:32:18.68,Default,,0,0,0,,Và ta sẽ cắt ngươi như thát lát một con cá.
Dialogue: 0,1:33:51.22,1:33:52.22,Default,,0,0,0,,Hệ thống bị lỗi
Dialogue: 0,1:34:13.86,1:34:15.40,Default,,0,0,0,,Arthur!
Dialogue: 0,1:34:19.04,1:34:21.54,Default,,0,0,0,,Mera. Họ đang theo dõi cô.
Dialogue: 0,1:34:21.54,1:34:22.67,Default,,0,0,0,,Cái gì?
Dialogue: 0,1:34:22.67,1:34:24.41,Default,,0,0,0,,Họ đang theo dõi cô.
Dialogue: 0,1:34:35.22,1:34:38.39,Default,,0,0,0,,Arthur, ở lại với tôi.
Dialogue: 0,1:34:39.06,1:34:42.13,Default,,0,0,0,,Arthur, tỉnh lại! Arthur.
Dialogue: 0,1:35:30.88,1:35:32.11,Default,,0,0,0,,Này...
Dialogue: 0,1:35:33.68,1:35:35.31,Default,,0,0,0,,Cô đã ăn cắp chiếc thuyền này à?
Dialogue: 0,1:35:35.31,1:35:38.45,Default,,0,0,0,,Những chiếc thuyền tại bến không phải công cộng hay sao?
Dialogue: 0,1:35:38.45,1:35:41.62,Default,,0,0,0,,Không. Nó thuộc về người dân.
Dialogue: 0,1:35:41.62,1:35:43.42,Default,,0,0,0,,Huh.
Dialogue: 0,1:35:45.19,1:35:46.92,Default,,0,0,0,,Đó là những tên lính ưu tú của Orm.
Dialogue: 0,1:35:46.92,1:35:50.22,Default,,0,0,0,,Người dẫn dắt họ không phải là Atlantis.
Dialogue: 0,1:35:50.23,1:35:52.59,Default,,0,0,0,,Tôi chưa bao giờ nhìn thấy họ trước đây.
Dialogue: 0,1:35:52.60,1:35:54.26,Default,,0,0,0,,Vâng, tôi biết.
Dialogue: 0,1:35:54.26,1:35:56.77,Default,,0,0,0,,Cậu ta và cha mình là cướp biển.
Dialogue: 0,1:35:58.23,1:36:00.80,Default,,0,0,0,,Thế mà đổ lỗi cho tôi về cái chết của cha mình.
Dialogue: 0,1:36:00.80,1:36:05.38,Default,,0,0,0,,Anh ta chọn một con đường nguy hiểm.\NĐó không phải là lỗi của anh.
Dialogue: 0,1:36:06.21,1:36:08.14,Default,,0,0,0,,Đó không phải là cảm giác của tôi.
Dialogue: 0,1:36:10.18,1:36:14.15,Default,,0,0,0,,Do tôi, tôi để cha cậu ta chết,
Dialogue: 0,1:36:14.15,1:36:17.05,Default,,0,0,0,,Tôi có thể cứu ông ta nhưng tôi đã không làm.
Dialogue: 0,1:36:17.05,1:36:21.02,Default,,0,0,0,,Tôi biết tôi đã tạo ra một kẻ thù.\NVà cậu ta có thể làm tổn thương cô.
Dialogue: 0,1:36:21.02,1:36:22.53,Default,,0,0,0,,Và đó là lỗi của tôi.
Dialogue: 0,1:36:24.43,1:36:26.53,Default,,0,0,0,,Bây giờ anh ta ở đằng sau chúng ta.
Dialogue: 0,1:36:27.76,1:36:30.37,Default,,0,0,0,,Những gì chờ đợi ở phía trước mới khiến chúng ta quan tâm.
Dialogue: 0,1:36:31.97,1:36:35.30,Default,,0,0,0,,Đường dẫn đến cây đinh ba\Nphải đi qua vương quốc của người Trench.
Dialogue: 0,1:36:36.94,1:36:39.37,Default,,0,0,0,,Đó chính là những sinh vật đã giết mẹ tôi.
Dialogue: 0,1:36:40.01,1:36:41.37,Default,,0,0,0,,Đúng vậy.
Dialogue: 0,1:36:41.38,1:36:43.48,Default,,0,0,0,,Bà đã được đưa đến đó và hiến tế cho họ.
Dialogue: 0,1:36:43.48,1:36:45.65,Default,,0,0,0,,Nơi đó đã trở thành một nơi chết chóc.
Dialogue: 0,1:36:45.65,1:36:47.85,Default,,0,0,0,,Chúng ta không biết về những gì ở dưới đó.
Dialogue: 0,1:36:51.99,1:36:54.22,Default,,0,0,0,,Chúng ta nên quay lại.
Dialogue: 0,1:36:54.22,1:36:57.03,Default,,0,0,0,,Chúng ta vẫn có thể cảnh báo người mặt đất,\Ncố gắng chuẩn bị cho những gì xảy ra sắp tới.
Dialogue: 0,1:36:57.03,1:36:58.63,Default,,0,0,0,,Quay lại?
Dialogue: 0,1:36:58.63,1:37:02.66,Default,,0,0,0,,Nghe này. Tôi được dạy từ nhỏ rằng không nên để lộ điểm yếu.
Dialogue: 0,1:37:02.67,1:37:05.90,Default,,0,0,0,,Tôi kết thúc vấn đề bằng sự tức giận và nắm đấm.
Dialogue: 0,1:37:05.90,1:37:08.90,Default,,0,0,0,,Tôi là chiến binh giỏi với nắm đấm thép.
Dialogue: 0,1:37:08.91,1:37:13.37,Default,,0,0,0,,Nhưng tôi chưa kịp làm gì thì bị đá vào mông trong toàn bộ chuyến đi này.
Dialogue: 0,1:37:13.38,1:37:16.41,Default,,0,0,0,,Tôi không phải là người lãnh đạo, không phải là một vị vua.
Dialogue: 0,1:37:16.41,1:37:20.15,Default,,0,0,0,,Tôi không hợp tác tốt với người khác.
Dialogue: 0,1:37:20.15,1:37:24.85,Default,,0,0,0,,Và tôi sẽ không để cô chết\Nnếu để cô chết thì tôi không phải chính mình.
Dialogue: 0,1:37:24.85,1:37:29.56,Default,,0,0,0,,Anh nghĩ rằng mình không xứng đáng để lãnh đạo\Nvì chúng ta đến từ hai thế giới khác nhau.
Dialogue: 0,1:37:29.56,1:37:32.69,Default,,0,0,0,,Nhưng đó lại là lý do tại sao anh xứng đáng.
Dialogue: 0,1:37:32.70,1:37:37.43,Default,,0,0,0,,Anh là cầu nối giữa đất liền và biển.
Dialogue: 0,1:37:39.27,1:37:40.77,Default,,0,0,0,,Bây giờ tôi có thể thấy điều đó.
Dialogue: 0,1:37:43.51,1:37:44.67,Default,,0,0,0,,Câu hỏi duy nhất là, anh có thể không?
Dialogue: 0,1:37:59.96,1:38:03.76,Default,,0,0,0,,Việc theo dõi công chúa Mera đang gặp trở ngại, thưa bệ hạ.
Dialogue: 0,1:38:04.79,1:38:06.26,Default,,0,0,0,,Vulko,
Dialogue: 0,1:38:07.03,1:38:08.90,Default,,0,0,0,, tể tướng đáng tin cậy của ta.
Dialogue: 0,1:38:08.90,1:38:12.57,Default,,0,0,0,,Ông đã sát cánh bên ngai vàng kể từ khi cha ta còn trị vì.
Dialogue: 0,1:38:14.64,1:38:17.54,Default,,0,0,0,,Ngài có lời khuyên nào cho ta khi cuộc chiến này bắt đầu không?
Dialogue: 0,1:38:17.54,1:38:20.67,Default,,0,0,0,,Tôi đã nó rồi, thưa bệ hạ.
Dialogue: 0,1:38:20.68,1:38:24.18,Default,,0,0,0,,Tôi sẽ luôn ủng hộ quyết định của bệ hạ.
Dialogue: 0,1:38:24.81,1:38:26.35,Default,,0,0,0,,Quyết định của ta
Dialogue: 0,1:38:28.32,1:38:29.72,Default,,0,0,0,,là bảo vệ ngai vàng,
Dialogue: 0,1:38:32.12,1:38:33.49,Default,,0,0,0,,trong khi ngài phản bội nó.
Dialogue: 0,1:38:34.66,1:38:38.19,Default,,0,0,0,,Ngài nghĩ rằng ta không biết về sự phản bội của ngài sao?
Dialogue: 0,1:38:38.19,1:38:43.50,Default,,0,0,0,,Ngài đã từng trung thành với đứa\Ncon trai khốn nạn của mẹ ta.
Dialogue: 0,1:38:43.50,1:38:48.94,Default,,0,0,0,,Ngài đã dày công đào tạo anh ta để giành lấy ngai vàng từ ta.
Dialogue: 0,1:38:51.94,1:38:53.61,Default,,0,0,0,,Đừng chối.
Dialogue: 0,1:38:56.75,1:38:57.88,Default,,0,0,0,,Tôi không phủ nhận
Dialogue: 0,1:38:58.48,1:38:59.75,Default,,0,0,0,,Tại sao?
Dialogue: 0,1:39:01.25,1:39:04.12,Default,,0,0,0,,Ta mang dòng máu thuần khiết.
Dialogue: 0,1:39:04.12,1:39:07.15,Default,,0,0,0,,Ta đã cống hiến cả đời mình cho Atlantis.
Dialogue: 0,1:39:07.16,1:39:10.66,Default,,0,0,0,,Anh ta thì không muốn làm vua.\NNgài thề sẽ phục vụ ngai vàng này.
Dialogue: 0,1:39:10.66,1:39:11.66,Default,,0,0,0,,Tôi phục vụ nó.
Dialogue: 0,1:39:11.66,1:39:14.33,Default,,0,0,0,,Arthur chỉ mang một nửa dòng máu Atlantis,
Dialogue: 0,1:39:14.33,1:39:18.47,Default,,0,0,0,,nhưng cậu ta xứng đáng làm vua hơn bệ hạ.
Dialogue: 0,1:39:24.77,1:39:26.24,Default,,0,0,0,,Đưa ngài ta đi.
Dialogue: 0,1:39:27.44,1:39:29.41,Default,,0,0,0,,Nhưng hãy đảm bảo ông ta có vị trí để quan sát.
Dialogue: 0,1:41:10.54,1:41:12.18,Default,,0,0,0,,Những thứ này là cái quái gì vậy?
Dialogue: 0,1:41:12.18,1:41:14.02,Default,,0,0,0,,Người Trench! Chúng ta đang đến nơi rồi!
Dialogue: 0,1:41:16.55,1:41:18.49,Default,,0,0,0,,- Bọn chúng đông quá!\N- Hãy vào trong!
Dialogue: 0,1:41:28.96,1:41:30.03,Default,,0,0,0,,Chúng đến từ nơi sâu thẳm.
Dialogue: 0,1:41:30.03,1:41:31.70,Default,,0,0,0,,Chúng sợ ánh sáng.
Dialogue: 0,1:41:44.04,1:41:45.45,Default,,0,0,0,,Nhanh lên!
Dialogue: 0,1:41:50.15,1:41:51.52,Default,,0,0,0,,Hãy sẵn sàng để nhảy!
Dialogue: 0,1:43:00.42,1:43:02.06,Default,,0,0,0,,Ở đằng kia!
Dialogue: 0,1:43:02.06,1:43:05.06,Default,,0,0,0,,Vương quốc người Trench
Dialogue: 0,1:43:36.69,1:43:38.69,Default,,0,0,0,,Thứ đó sẽ xé xác chúng ta ra.
Dialogue: 0,1:43:38.69,1:43:40.53,Default,,0,0,0,,Chúng ta không có lựa chọn nào cả.
Dialogue: 0,1:44:00.45,1:44:02.15,Default,,0,0,0,,Arthur!
Dialogue: 0,1:44:05.42,1:44:06.62,Default,,0,0,0,,Mera!
Dialogue: 0,1:44:37.85,1:44:38.92,Default,,0,0,0,,Mera!
Dialogue: 0,1:44:40.05,1:44:41.05,Default,,0,0,0,,Mera!
Dialogue: 0,1:44:47.05,1:44:50.05,Default,,0,0,0,,Vùng biển ẩn - Tâm của Trái Đất
Dialogue: 0,1:45:02.71,1:45:03.91,Default,,0,0,0,,Mera!
Dialogue: 0,1:45:06.91,1:45:07.91,Default,,0,0,0,,Mera!
Dialogue: 0,1:45:52.16,1:45:53.33,Default,,0,0,0,,Arthur.
Dialogue: 0,1:45:54.23,1:45:55.63,Default,,0,0,0,,Mẹ?
Dialogue: 0,1:46:33.80,1:46:38.54,Default,,0,0,0,,Vua Orvax đã tế sống mẹ cho người Trench.
Dialogue: 0,1:46:38.54,1:46:42.00,Default,,0,0,0,,Nhưng mẹ đã sống sót, chiến đấu \Ntheo cách của mẹ và mẹ đã ở đây.
Dialogue: 0,1:46:42.01,1:46:43.84,Default,,0,0,0,,Cô đã ở đây một mình trong hai mươi năm?
Dialogue: 0,1:46:43.84,1:46:45.31,Default,,0,0,0,,Ừ
Dialogue: 0,1:46:45.31,1:46:46.81,Default,,0,0,0,,Hãy đến đây, ngồi ở đây.
Dialogue: 0,1:46:51.72,1:46:54.32,Default,,0,0,0,,Con phải tha thứ cho mẹ.
Dialogue: 0,1:46:54.32,1:46:56.95,Default,,0,0,0,, Tha thứ mọi thứ.
Dialogue: 0,1:46:56.96,1:46:59.46,Default,,0,0,0,,Chuyện này xảy ra vì mẹ đã sinh ra con.
Dialogue: 0,1:46:59.46,1:47:04.33,Default,,0,0,0,,Không ai có lỗi, con cũng không có lỗi.
Dialogue: 0,1:47:04.33,1:47:07.23,Default,,0,0,0,,Mẹ đã lựa chọn.\NMẹ phải rời đi để cứu con.
Dialogue: 0,1:47:07.23,1:47:11.34,Default,,0,0,0,,và cứu cha con.
Dialogue: 0,1:47:13.74,1:47:16.31,Default,,0,0,0,,Hãy cho mẹ biết, cho mẹ biết về ông ấy.
Dialogue: 0,1:47:16.74,1:47:18.14,Default,,0,0,0,,Là ông ấy.
Dialogue: 0,1:47:21.91,1:47:24.38,Default,,0,0,0,,Ông ấy vẫn đi đến cuối bến tàu
Dialogue: 0,1:47:26.22,1:47:27.25,Default,,0,0,0,,Mỗi sáng.
Dialogue: 0,1:47:30.29,1:47:31.76,Default,,0,0,0,,Mỗi ngày.
Dialogue: 0,1:47:34.86,1:47:36.36,Default,,0,0,0,,Để ngóng chờ mẹ.
Dialogue: 0,1:47:43.97,1:47:45.50,Default,,0,0,0,,Tại sao mẹ không bao giờ quay lại?
Dialogue: 0,1:47:51.04,1:47:55.28,Default,,0,0,0,,Vì cổng không gian đưa con đến đây\Nsẽ không bao giờ cho phép mẹ quay lại.
Dialogue: 0,1:47:55.28,1:47:58.91,Default,,0,0,0,,Cây đinh ba là cách duy nhất để quay lại.
Dialogue: 0,1:47:58.92,1:48:02.95,Default,,0,0,0,,Nhưng nó được bảo vệ bởi Karathen.
Dialogue: 0,1:48:05.16,1:48:07.72,Default,,0,0,0,,Sinh vật trong truyền thuyết của chúng ta là có thật.
Dialogue: 0,1:48:07.73,1:48:10.86,Default,,0,0,0,,Nó nằm bên ngoài thác nước.
Dialogue: 0,1:48:14.93,1:48:17.27,Default,,0,0,0,,Chúng ta sẽ đi cùng. Chúng ta chiến đấu với nhau.
Dialogue: 0,1:48:17.27,1:48:18.64,Default,,0,0,0,,Không.
Dialogue: 0,1:48:18.64,1:48:20.47,Default,,0,0,0,,Nó quá mạnh.
Dialogue: 0,1:48:20.47,1:48:24.31,Default,,0,0,0,,Mẹ đã thử nhiều lần trong nhiều năm.
Dialogue: 0,1:48:24.31,1:48:27.91,Default,,0,0,0,,Sinh vật sẽ chỉ cho phép vị vua thực sự vượt qua.
Dialogue: 0,1:48:30.81,1:48:32.45,Default,,0,0,0,,Con sợ không?.
Dialogue: 0,1:48:35.22,1:48:36.29,Default,,0,0,0,,Có.
Dialogue: 0,1:48:37.79,1:48:39.09,Default,,0,0,0,,Tốt.
Dialogue: 0,1:48:40.62,1:48:42.03,Default,,0,0,0,,Con đã sẵn sàng rồi.
Dialogue: 0,1:48:43.29,1:48:45.16,Default,,0,0,0,,Atlantis luôn có một vị vua.
Dialogue: 0,1:48:46.93,1:48:49.13,Default,,0,0,0,,Bây giờ, nó cần một cái gì đó nhiều hơn.
Dialogue: 0,1:48:49.13,1:48:51.27,Default,,0,0,0,,Điều gì có thể vĩ đại hơn một vị vua?
Dialogue: 0,1:48:51.77,1:48:53.04,Default,,0,0,0,,Một anh hùng.
Dialogue: 0,1:48:54.81,1:48:57.21,Default,,0,0,0,,Một vị vua chỉ chiến đấu cho quốc gia của mình.
Dialogue: 0,1:48:58.98,1:49:01.25,Default,,0,0,0,,Con thì chiến đấu vì mọi người.
Dialogue: 0,1:50:29.80,1:50:35.57,Default,,0,0,0,,Ngươi không thuộc về nơi này.
Dialogue: 0,1:50:35.57,1:50:42.14,Default,,0,0,0,,Ta đã bảo vệ Cây đinh ba chống lại các vị vua giả mạo
Dialogue: 0,1:50:42.15,1:50:44.55,Default,,0,0,0,,trong suốt một ngàn năm qua.
Dialogue: 0,1:50:44.55,1:50:50.35,Default,,0,0,0,,Ta đã đấu với những người vĩ đại nhất\Nvà họ đều thất bại.
Dialogue: 0,1:50:50.35,1:50:56.89,Default,,0,0,0,,Nhưng ta chưa bao giờ cảm thấy\Nmột người không xứng đáng như ngươi.
Dialogue: 0,1:50:56.89,1:51:04.13,Default,,0,0,0,,Ngươi dám đến đây với dòng máu lai tạp
Dialogue: 0,1:51:04.13,1:51:07.67,Default,,0,0,0,,để đoạt lấy báu vật lớn nhất của Atlantis sao?
Dialogue: 0,1:51:09.97,1:51:11.21,Default,,0,0,0,,Vậy thì chiến thôi.
Dialogue: 0,1:51:12.61,1:51:14.38,Default,,0,0,0,,Tên tạp chủng!
Dialogue: 0,1:51:33.86,1:51:37.06,Default,,0,0,0,,Ngươi nghĩ mình xứng đáng sao?
Dialogue: 0,1:51:37.07,1:51:40.20,Default,,0,0,0,,Ngươi nghĩ mình là vua à?
Dialogue: 0,1:51:40.20,1:51:44.61,Default,,0,0,0,,Ngươi đã làm mất uy danh nơi này vì đã dám liều mạng đến đây.
Dialogue: 0,1:51:44.61,1:51:45.71,Default,,0,0,0,,Dừng lại!
Dialogue: 0,1:51:53.28,1:51:57.95,Default,,0,0,0,,Mi đã đúng. Ta là một giống lai tạp.
Dialogue: 0,1:51:57.95,1:52:00.89,Default,,0,0,0,,Nhưng ta đã không đến đây vì ta nghĩ mình xứng đáng.
Dialogue: 0,1:52:00.89,1:52:02.79,Default,,0,0,0,,Ta biết ta không phải.
Dialogue: 0,1:52:02.79,1:52:06.86,Default,,0,0,0,,Ngươi hiểu ta sao?
Dialogue: 0,1:52:06.86,1:52:08.63,Default,,0,0,0,,Ta hiểu.
Dialogue: 0,1:52:08.63,1:52:15.44,Default,,0,0,0,,Không có người phàm nào nói chuyện\Nvới ta kể từ thời vua Atlan.
Dialogue: 0,1:52:16.54,1:52:19.74,Default,,0,0,0,,Ngươi là ai?
Dialogue: 0,1:52:20.54,1:52:22.01,Default,,0,0,0,,Ta không là ai cả.
Dialogue: 0,1:52:24.21,1:52:27.08,Default,,0,0,0,,Ta đến vì ta không có lựa chọn nào khác.
Dialogue: 0,1:52:28.55,1:52:33.15,Default,,0,0,0,,Ta đến để cứu nhà của mình,\Nvà những người ta yêu.
Dialogue: 0,1:52:35.26,1:52:39.36,Default,,0,0,0,,Ta đến vì Cây đinh ba vì nó là\Nhy vọng duy nhất của chúng ta.
Dialogue: 0,1:52:39.36,1:52:41.56,Default,,0,0,0,,Và nếu điều đó còn chưa đủ,
Dialogue: 0,1:52:44.00,1:52:45.57,Default,,0,0,0,,thì mặc kệ mi.
Dialogue: 0,1:52:47.50,1:52:54.54,Default,,0,0,0,,Chưa có người nào giải thoát Cây đinh ba khỏi tay Atlan.
Dialogue: 0,1:52:55.38,1:52:58.58,Default,,0,0,0,,Nếu ngài ấy cho rằng ngươi không xứng đáng,
Dialogue: 0,1:52:58.58,1:53:03.95,Default,,0,0,0,,thì ta sẽ tiêu diệt ngươi, vì ta đang đói
Dialogue: 0,1:53:03.95,1:53:07.16,Default,,0,0,0,,ta chưa ăn gì.
Dialogue: 0,1:54:37.05,1:54:40.21,Default,,0,0,0,,Con sẽ là một vị đế vương thực sự.
Dialogue: 0,1:54:52.56,1:54:55.70,Default,,0,0,0,,Hãy nhớ rằng, người Brine là bọn man rợ và mạnh mẽ.
Dialogue: 0,1:54:55.70,1:55:00.23,Default,,0,0,0,,Bọn chúng sẽ đi theo chúng ta nếu lãnh đạo của chúng làm vậy,\Nlúc đó tình thế sẽ xoay chuyển.
Dialogue: 0,1:55:00.24,1:55:02.07,Default,,0,0,0,,Đánh bại vua của người Brine,
Dialogue: 0,1:55:02.07,1:55:05.74,Default,,0,0,0,,thì ngươi sẽ chỉ huy đội quân mạnh nhất trên hành tinh này.
Dialogue: 0,1:55:07.61,1:55:09.61,Default,,0,0,0,,Hôm nay chúng ta hợp nhất vương quốc của chúng ta.
Dialogue: 0,1:55:11.28,1:55:12.88,Default,,0,0,0,,Ngày mai chúng ta sẽ thiêu đốt người mặt đất!
Dialogue: 0,1:55:14.18,1:55:17.59,Default,,0,0,0,,Hỡi người Atlantis! Hãy trỗi dậy !
Dialogue: 0,1:55:35.24,1:55:38.67,Default,,0,0,0,,Giết chết bọn người Atlantis!
Dialogue: 0,1:55:43.88,1:55:46.18,Default,,0,0,0,,Hãy cho bọn chúng biết lễ độ!
Dialogue: 0,1:55:46.18,1:55:50.72,Default,,0,0,0,,Chúng ta sẽ không thua bọn mọi rợ Atlantis!
Dialogue: 0,1:56:57.15,1:56:59.95,Default,,0,0,0,,Dừng tay, chúng ta cần hắn.
Dialogue: 0,1:57:02.16,1:57:03.76,Default,,0,0,0,,Theo ta hoặc là chết!
Dialogue: 0,1:57:03.76,1:57:07.33,Default,,0,0,0,,Ngươi muốn ta gọi ngươi là đế vương sao?
Dialogue: 0,1:57:07.33,1:57:08.76,Default,,0,0,0,,Không phải
Dialogue: 0,1:57:10.76,1:57:12.33,Default,,0,0,0,,Gọi là...
Dialogue: 0,1:57:13.03,1:57:14.73,Default,,0,0,0,, Bá chủ Đại dương.
Dialogue: 0,1:57:14.74,1:57:18.64,Default,,0,0,0,,Ngươi có thể lấy quân đội của ta,
Dialogue: 0,1:57:18.64,1:57:22.21,Default,,0,0,0,,nhưng sẽ không bao giờ có lòng trung thành của ta, tên khốn !
Dialogue: 0,1:57:22.21,1:57:23.48,Default,,0,0,0,,Thế thì...
Dialogue: 0,1:57:55.34,1:57:56.68,Default,,0,0,0,,Cái quái gì thế?...
Dialogue: 0,1:58:31.98,1:58:33.71,Default,,0,0,0,,Đế vương đã trỗi dậy.
Dialogue: 0,1:58:41.32,1:58:43.49,Default,,0,0,0,,Tấn công!
Dialogue: 0,2:00:42.24,2:00:44.94,Default,,0,0,0,,Có quá nhiều thương vong.\NChúng ta phải ngừng cuộc chiến ngay.
Dialogue: 0,2:00:44.94,2:00:48.28,Default,,0,0,0,,Tôi sẽ tìm cha tôi. Anh phải đánh bại Orm.
Dialogue: 0,2:00:49.61,2:00:51.18,Default,,0,0,0,,Và nếu tôi không thể thắng thì sao?
Dialogue: 0,2:00:51.18,2:00:54.42,Default,,0,0,0,,Lần trước anh bị hắn dụ vào nơi thế mạnh của hắn ta.
Dialogue: 0,2:00:54.42,2:00:57.62,Default,,0,0,0,,Lần này, hãy khiến hắn cuốn theo sở trường của anh.
Dialogue: 0,2:01:24.55,2:01:26.48,Default,,0,0,0,,Chà...
Dialogue: 0,2:01:26.49,2:01:29.75,Default,,0,0,0,,- Kế hoạch lại là gì?\N- Kế hoạch là không bị giết.
Dialogue: 0,2:01:30.86,2:01:33.59,Default,,0,0,0,,Kế hoạch hay.\NĐừng để bị giết!
Dialogue: 0,2:02:06.52,2:02:08.56,Default,,0,0,0,,Người Trench!\NHắn ta đang chỉ huy người Trench!
Dialogue: 0,2:02:08.56,2:02:10.06,Default,,0,0,0,,Không thể nào!
Dialogue: 0,2:02:26.91,2:02:28.64,Default,,0,0,0,,Chờ đã! Hãy để cô ấy vượt qua.
Dialogue: 0,2:02:28.65,2:02:31.85,Default,,0,0,0,,Làm ơn, thưa cha.\NAnh ấy đã tìm ra Cây đinh ba.
Dialogue: 0,2:02:31.85,2:02:34.22,Default,,0,0,0,,Con biết cha nghĩ rằng điều này là cần thiết.
Dialogue: 0,2:02:34.22,2:02:36.79,Default,,0,0,0,,Nhưng Arthur giờ là đế vương của chúng ta theo ý Atlan.
Dialogue: 0,2:02:36.79,2:02:38.45,Default,,0,0,0,,Và nếu cha quay lưng lại bây giờ
Dialogue: 0,2:02:38.46,2:02:40.76,Default,,0,0,0,,thì Atlantis mà cha đang chiến đấu sẽ diệt vong.
Dialogue: 0,2:02:40.76,2:02:43.83,Default,,0,0,0,,Đó là sự thật!\NTên lai tạp đang sử cây đinh ba của vua Atlan.
Dialogue: 0,2:02:43.83,2:02:45.10,Default,,0,0,0,,Giờ hắn làm trùm biển cả rồi!
Dialogue: 0,2:02:47.16,2:02:49.23,Default,,0,0,0,,Vậy thì tên tạp chủng đó sẽ là đế vương của các ngươi.
Dialogue: 0,2:04:06.38,2:04:08.88,Default,,0,0,0,,Vulko! Thôi nào, anh ấy cần sự giúp đỡ của chúng ta.
Dialogue: 0,2:04:08.88,2:04:10.78,Default,,0,0,0,,Chúng ta không thể.\NNhìn xem
Dialogue: 0,2:04:10.78,2:04:12.42,Default,,0,0,0,,Mọi thần dân phải làm chứng.
Dialogue: 0,2:04:18.56,2:04:21.02,Default,,0,0,0,,Cây đinh ba không thể thay đổi sức mạnh của ngươi đâu
Dialogue: 0,2:04:21.92,2:04:23.46,Default,,0,0,0,,Tên cẩu tạp chủng nhà ngươi không phải là anh của ta.
Dialogue: 0,2:04:26.06,2:04:29.40,Default,,0,0,0,,Atlantis sẽ không bao giờ chấp nhận tên khốn ngươi là đế vương.
Dialogue: 0,2:04:32.10,2:04:33.47,Default,,0,0,0,,Vậy thì máu của hai ta sẽ sôi sục,
Dialogue: 0,2:04:35.71,2:04:37.74,Default,,0,0,0,,nhưng các vị thần sẽ làm theo ý ta thôi.
Dialogue: 0,2:05:07.17,2:05:09.47,Default,,0,0,0,,Hãy từ bỏ ngai vàng ngay đi.
Dialogue: 0,2:06:45.30,2:06:46.50,Default,,0,0,0,,Kết thúc ta đi
Dialogue: 0,2:06:46.50,2:06:47.87,Default,,0,0,0,,Từ bỏ ngai vàng đi.
Dialogue: 0,2:06:47.87,2:06:49.64,Default,,0,0,0,,Lòng thương xót không phải là cách của chúng ta.
Dialogue: 0,2:06:51.24,2:06:53.38,Default,,0,0,0,,Có thể ngươi không biết đâu, người anh em....
Dialogue: 0,2:06:58.85,2:07:00.72,Default,,0,0,0,,Nhưng ta không phải là các ngươi.
Dialogue: 0,2:07:00.72,2:07:01.95,Default,,0,0,0,,Kết thúc đi!
Dialogue: 0,2:07:04.19,2:07:06.19,Default,,0,0,0,,- Giết ta đi !\N- Không !
Dialogue: 0,2:07:08.83,2:07:10.53,Default,,0,0,0,,Tương tàn thế là đủ rồi !
Dialogue: 0,2:07:18.00,2:07:19.37,Default,,0,0,0,,Mẹ?
Dialogue: 0,2:07:19.90,2:07:21.57,Default,,0,0,0,,Orm, ôi con trai của ta.
Dialogue: 0,2:07:30.71,2:07:32.85,Default,,0,0,0,,Đó là một câu chuyện dài.
Dialogue: 0,2:07:32.85,2:07:34.38,Default,,0,0,0,,Cháu sẽ kể cho chú sau.
Dialogue: 0,2:07:35.35,2:07:36.62,Default,,0,0,0,,Đi nào.
Dialogue: 0,2:07:43.16,2:07:46.33,Default,,0,0,0,,Con không hiểu. Con nghĩ rằng...
Dialogue: 0,2:07:46.33,2:07:47.33,Default,,0,0,0,,Mẹ biết.
Dialogue: 0,2:07:47.90,2:07:49.70,Default,,0,0,0,,Arthur đã cứu mẹ.
Dialogue: 0,2:07:53.40,2:07:55.67,Default,,0,0,0,,- Mẹ đã ở bên cạnh anh ta sao?\N- Đúng thế.
Dialogue: 0,2:07:58.24,2:08:03.34,Default,,0,0,0,,Con, Arthur, cả hai đứa đều là con của\Nta và ta yêu các con rất nhiều.
Dialogue: 0,2:08:03.34,2:08:05.58,Default,,0,0,0,,Nhưng con đã lạc lối rồi.
Dialogue: 0,2:08:06.95,2:08:11.02,Default,,0,0,0,,Cha của con đã dạy con rằng có hai thế giới.
Dialogue: 0,2:08:11.02,2:08:12.89,Default,,0,0,0,,Nhưng ông ấy đã sai.
Dialogue: 0,2:08:12.89,2:08:15.62,Default,,0,0,0,,Đất liền và biển cả
Dialogue: 0,2:08:16.93,2:08:18.46,Default,,0,0,0,,là một.
Dialogue: 0,2:08:38.45,2:08:39.58,Default,,0,0,0,,Nữ hoàng của chúng ta.
Dialogue: 0,2:08:42.68,2:08:44.05,Default,,0,0,0,,Vulko.
Dialogue: 0,2:08:46.35,2:08:48.15,Default,,0,0,0,,Đưa anh ta đi.
Dialogue: 0,2:08:48.16,2:08:50.49,Default,,0,0,0,,Nhưng hãy chắc chắn rằng\Nanh ta có một chỗ để quan sát.
Dialogue: 0,2:08:57.50,2:08:58.50,Default,,0,0,0,,Khi nào ngươi đã sẵn sàng,
Dialogue: 0,2:09:00.90,2:09:02.30,Default,,0,0,0,, lúc đó hãy nói chuyện.
Dialogue: 0,2:09:16.95,2:09:21.09,Default,,0,0,0,,Hỡi thần dân Atlantis, hôm nay chúng ta\Ngặp nhau trong đổ máu.
Dialogue: 0,2:09:21.09,2:09:24.06,Default,,0,0,0,,nhưng hãy để nó kết thúc trong niềm vui.
Dialogue: 0,2:09:24.06,2:09:28.59,Default,,0,0,0,,Hãy cúi chào, Đế vương Arthur của người Atlantis chúng ta.
Dialogue: 0,2:09:28.60,2:09:30.30,Default,,0,0,0,,Kính chào Đế Vương !
Dialogue: 0,2:09:30.30,2:09:33.33,Default,,0,0,0,,- Bệ hạ muôn năm !\N- Bệ hạ muôn năm !
Dialogue: 0,2:09:41.38,2:09:43.71,Default,,0,0,0,,Vậy tôi phải làm gì bây giờ?
Dialogue: 0,2:09:44.31,2:09:46.11,Default,,0,0,0,,Trở thành vua của họ.
Dialogue: 0,2:09:47.32,2:09:48.68,Default,,0,0,0,,Ooh.
Dialogue: 0,2:09:48.68,2:09:50.79,Default,,0,0,0,,Chuyện này sẽ rất vui.
Dialogue: 0,2:10:42.80,2:10:45.70,Default,,0,0,0,,Em đã trở lại.
Dialogue: 0,2:10:45.71,2:10:47.81,Default,,0,0,0,,Đúng thế
Dialogue: 0,2:10:53.68,2:10:56.58,Default,,0,0,0,,{\i1}Cha tôi là người canh giữ ngọn hải đăng.{\i}
Dialogue: 0,2:10:57.39,2:10:59.45,Default,,0,0,0,,{\i1}Mẹ tôi là nữ hoàng.{\i}
Dialogue: 0,2:11:00.96,2:11:02.72,Default,,0,0,0,,{\i1}Họ không bao giờ có ý định gặp nhau.{\i}
Dialogue: 0,2:11:06.23,2:11:08.63,Default,,0,0,0,,{\i1}Nhưng tình yêu của họ đã cứu thế giới.{\i}
Dialogue: 0,2:11:10.83,2:11:13.43,Default,,0,0,0,,{\i1}Họ đã biến tôi thành chính mình.{\i}
Dialogue: 0,2:11:13.43,2:11:17.14,Default,,0,0,0,,{\i1}Một người con của vùng đất, một vị vua của biển cả.{\i}
Dialogue: 0,2:11:18.71,2:11:20.74,Default,,0,0,0,,{\i1}Tôi là người bảo vệ biển cả.{\i}
Dialogue: 0,2:11:22.54,2:11:23.61,Default,,0,0,0,,{\i1}Tôi là...{\i}
Dialogue: 0,2:11:25.28,2:11:27.25,Default,,0,0,0,,{\i1}Aquaman.{\i}
Dialogue: 0,2:14:36.24,2:14:37.50,Default,,0,0,0,,Này!
Dialogue: 0,2:14:41.44,2:14:42.97,Default,,0,0,0,,{\i1}Tôi đã bỏ công sức 15 năm qua{\i}
Dialogue: 0,2:14:42.98,2:14:45.34,Default,,0,0,0,,{\i1}tại Viện Khoa học Hàng hải Hoa Kỳ ...{\i}
Dialogue: 0,2:14:45.34,2:14:48.51,Default,,0,0,0,,- {\i1}Tôi tưởng anh bị đuổi việc rồi chứ?{\i}\N- {\i1}...để nghiên cứu các hiện tượng biển,{\i}
Dialogue: 0,2:14:48.51,2:14:50.88,Default,,0,0,0,,{\i1}và đây là bằng chứng đáng tin nhất chúng ta từng có.{\i}
Dialogue: 0,2:14:50.88,2:14:54.45,Default,,0,0,0,,{\i1}Các máy đo địa chấn biển đã ghi lại \Nnhững vụ nổ dưới đáy đại dương.{\i}
Dialogue: 0,2:14:54.45,2:14:56.65,Default,,0,0,0,,{\i1}Chúng ta có hình ảnh vệ tinh\N{\i}
Dialogue: 0,2:14:56.66,2:14:59.02,Default,,0,0,0,,{\i1}về chiến hạm của người Atlantis nổi trên mặt nước ở Thượng Hải...{\i}
Dialogue: 0,2:14:59.02,2:15:00.82,Default,,0,0,0,,{\i1} - Tiến sĩ Shin\N - Để tôi nói hết được không?{\i}
Dialogue: 0,2:15:00.83,2:15:03.06,Default,,0,0,0,,{\i1}Lại là về Atlantis sao?{\i}
Dialogue: 0,2:15:03.06,2:15:05.83,Default,,0,0,0,,{\i1}Đúng vậy, chính phủ vẫn che giấu sự việc,\Nkhông cho chúng ta biết về mối nguy{\i}
Dialogue: 0,2:15:05.83,2:15:07.40,Default,,0,0,0,,{\i1}đang ở ngay trên thế giới của chúng ta....{\i}
Dialogue: 0,2:15:07.40,2:15:09.73,Default,,0,0,0,,Cẩn thận, nó nổ! tôi sẽ không làm thế nếu là anh đâu.
Dialogue: 0,2:15:21.98,2:15:23.91,Default,,0,0,0,,Đây là công nghệ của người Atlantean, phải không?
