Dialogue: 0,1:53:06.39,1:53:09.42,ENG,,0,0,0,,# All the leaves are brown #
Dialogue: 0,1:53:10.35,1:53:11.91,CHT,,0,0,0,,克連  你沒事吧?\N{\rENG}Clem, you all right?
Dialogue: 0,1:53:12.02,1:53:14.09,ENG,,0,0,0,,# And the sky is gray #
Dialogue: 0,1:53:16.42,1:53:20.40,ENG,,0,0,0,,# I went for a walk #
Dialogue: 0,1:53:21.06,1:53:25.25,ENG,,0,0,0,,# On a winter's day #
Dialogue: 0,1:53:25.43,1:53:30.74,ENG,,0,0,0,,# I'd be safe and warm #
Dialogue: 0,1:53:30.88,1:53:35.21,CHT,,0,0,0,,- # If I was in L.A. # - 你好\N{\rENG}- # If I was in L.A. # - How do?
Dialogue: 0,1:53:35.34,1:53:39.51,CHT,,0,0,0,,- # Oh, California Dreamin' #  - 今天過得很糟  我差點丟盡臉面\N{\rENG}- # Oh, California Dreamin' # - What a day. Shit, I almost lost my shit.
Dialogue: 0,1:53:40.80,1:53:45.45,ENG,,0,0,0,,# On such a winter's day, hey #
Dialogue: 0,1:53:45.72,1:53:48.51,ENG,,0,0,0,,# I went into a church #
Dialogue: 0,1:53:49.29,1:53:53.88,ENG,,0,0,0,,# I stopped along the way #
