Dialogue: 0,0:15:51.08,0:15:54.11,CHT,,0,0,0,,# 沒有衣服  沒有眼淚  沒有饑餓 #\N{\rENG}# No clothing, no tears, no hunger #
Dialogue: 0,0:15:54.25,0:15:57.02,CHT,,0,0,0,,# 你會明白  你會明白 #\N{\rENG}# You shall see, you shall see, you shall see #
