Dialogue: 0,0:13:48.13,0:13:49.35,Default,NTP,0000,0000,0000,,[闷哼声]
Dialogue: 0,0:13:51.22,0:13:53.61,Secondary,NTP,0000,0000,0000,,[officers shouting]
Dialogue: 0,0:13:51.22,0:13:53.61,Default,NTP,0000,0000,0000,,[警察喊叫声]
Dialogue: 0,0:13:58.01,0:13:59.92,Secondary,NTP,0000,0000,0000,,["El Fuereño" continues\Nwith lyrics in Spanish]
Dialogue: 0,0:13:58.01,0:13:59.92,Default,NTP,0000,0000,0000,,["外乡人"继续播放\N歌词为西班牙语]
