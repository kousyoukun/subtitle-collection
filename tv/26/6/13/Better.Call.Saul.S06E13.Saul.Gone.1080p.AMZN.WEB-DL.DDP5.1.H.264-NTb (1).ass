Dialogue: 0,0:00:06.10,0:00:07.67,ENG,,0,0,0,,[bird calling]
Dialogue: 0,0:00:16.08,0:00:17.45,ENG,,0,0,0,,[rustling]
Dialogue: 0,0:00:29.02,0:00:31.79,ENG,,0,0,0,,[exhales sharply]
Dialogue: 0,0:00:31.93,0:00:33.46,ENG,,0,0,0,,[grunting softly]
Dialogue: 0,0:00:40.27,0:00:41.70,ENG,,0,0,0,,[breathing heavily]
Dialogue: 0,0:00:41.84,0:00:42.84,ENG,,0,0,0,,[gasps]
